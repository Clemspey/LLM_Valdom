{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0669P0D1N000024", "chronotag": "1739874717934", "legislature": "17", "identification": {"numeroLong": "CL24", "numeroOrdreDepot": "24", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0669P0D1", "texteLegislatifRef": "PIONANR5L17B0669", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794778", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841701", "PA841351", "PA794022", "PA610968"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Regol,  Mme&#160;Balage&#160;El&#160;Mariky, M.&#160;Duplessy, M.&#160;Iordanoff et M.&#160;Amirshahi"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0669.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer aux alin&#x00E9;as 2 &#x00E0; 4 les quatre alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;L&#x2019;article&nbsp;7 est ainsi modifi&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>a)</i>&nbsp;Apr&#x00E8;s le mot&nbsp;: &#x00AB;&nbsp;derniers&nbsp;&#x00BB;, la fin du troisi&#x00E8;me alin&#x00E9;a est supprim&#x00E9;e&nbsp;;&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>b)</i>&nbsp;Apr&#x00E8;s le troisi&#x00E8;me alin&#x00E9;a, il est ins&#x00E9;r&#x00E9; un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00AB;&nbsp;Le d&#x00E9;lai de prescription d&#x2019;un viol est prolong&#x00E9;, avant son expiration, en cas de nouveau viol, d&#x2019;une agression sexuelle ou d&#x2019;une atteinte sexuelle commise par la m&#x00EA;me personne sur une autre personne, jusqu&#x2019;&#x00E0; la date de prescription de la nouvelle infraction.&nbsp;&#x00BB;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement des &#x00E9;cologistes corrige les incoh&#x00E9;rences de la proposition de loi, qui pr&#x00E9;voit un d&#x00E9;lai de prescription des viols sur majeurs de trente ans &#x00E0; compter de leur majorit&#x00E9;. Il clarifie le m&#x00E9;canisme de prescription glissante en maintenant son application aux viols commis sur des majeurs.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-18", "datePublication": "2025-01-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-22T11:59:33+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0669P0D1N000024?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "2", "divisionCodeLoi": "dc"}, "discussionCommune": {"idDiscussion": "28650", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}