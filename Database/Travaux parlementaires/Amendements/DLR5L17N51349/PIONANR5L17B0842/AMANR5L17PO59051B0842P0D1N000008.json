{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0842P0D1N000008", "chronotag": "1742659260023", "legislature": "17", "identification": {"numeroLong": "CL8", "numeroOrdreDepot": "8", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0842P0D1", "texteLegislatifRef": "PIONANR5L17B0842", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaah", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841893", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840665", "PA608264", "PA840903", "PA840793", "PA340343", "PA720992", "PA794494"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Thi&#233;bault-Martinez,  Mme&#160;Allemand, Mme&#160;Capdevielle, M.&#160;Christophle, M.&#160;Pena, M.&#160;Saulignac, M.&#160;William et M.&#160;Vicot"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0842.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;3, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de l&#x2019;auteur&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;autrui&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, au m&#x00EA;me alin&#x00E9;a, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;autrui&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de l&#x2019;auteur&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; inverser dans la d&#x00E9;finition donn&#x00E9;e par l&#x2019;article L.&nbsp;222&#x2011;22 du code p&#x00E9;nal de l&#x2019;agression sexuelle l&#x2019;ordre des termes &#x00AB;&nbsp;autrui&nbsp;&#x00BB; et &#x00AB;&nbsp;auteur&nbsp;&#x00BB; par coh&#x00E9;rence avec la d&#x00E9;finition donn&#x00E9;e par l&#x2019;article L.&nbsp;222&#x2011;23 du code p&#x00E9;nal&nbsp;de la d&#x00E9;finition du viol, qui mentionne que ce dernier&nbsp;peut &#x00EA;tre commis &#x00AB;&nbsp;sur la personne d&#x2019;autrui ou sur la personne de l&#x2019;auteur&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">Cette harmonisation l&#x00E9;gistique est sugg&#x00E9;r&#x00E9;e par le Conseil d&#x2019;&#x00C9;tat, dans son avis sur la proposition de loi, en son consid&#x00E9;rant 15.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-21", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0842P0D1N000008?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "id1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}