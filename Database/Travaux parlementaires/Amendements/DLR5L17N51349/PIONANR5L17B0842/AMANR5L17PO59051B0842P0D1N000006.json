{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0842P0D1N000006", "chronotag": "1742659259949", "legislature": "17", "identification": {"numeroLong": "CL6", "numeroOrdreDepot": "6", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0842P0D1", "texteLegislatifRef": "PIONANR5L17B0842", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaks", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841893", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840665", "PA608264", "PA840903", "PA840793", "PA340343", "PA720992", "PA794494"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Thi&#233;bault-Martinez,  Mme&#160;Allemand, Mme&#160;Capdevielle, M.&#160;Christophle, M.&#160;Pena, M.&#160;Saulignac, M.&#160;William et M.&#160;Vicot"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0842.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;consentement&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">r&#x00E9;diger ainsi la fin de la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;5&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;est libre et &#x00E9;clair&#x00E9;, sp&#x00E9;cifique, pr&#x00E9;alable et r&#x00E9;vocable.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, supprimer la deuxi&#x00E8;me phrase de l&#x2019;alin&#x00E9;a&nbsp;5.</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, supprimer l&#x2019;alin&#x00E9;a&nbsp;7.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement de nature l&#x00E9;gistique des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; pr&#x00E9;ciser que le consentement est &#x00E9;clair&#x00E9; et r&#x00E9;vocable, tout en maintenant les apports de la proposition de loi concernant sa nature libre&nbsp;et sp&#x00E9;cifique.</p><p style=\"text-align: justify;\">L&#x2019;ajout du terme &#x00AB;&nbsp;&#x00E9;clair&#x00E9;&nbsp;&#x00BB; permet notamment de couvrir des situations o&#x00F9; la victime pr&#x00E9;sum&#x00E9;e a un discernement alt&#x00E9;r&#x00E9;, notamment par la consommation de produits psychoactifs (alcool, drogues notamment).</p><p style=\"text-align: justify;\">L&#x2019;ajout du terme &#x00AB;&nbsp;r&#x00E9;vocable&nbsp;&#x00BB; permet, lui, de cr&#x00E9;er une base l&#x00E9;gale pour les situations o&#x00F9; l&#x2019;auteur pr&#x00E9;sum&#x00E9; a exploit&#x00E9; un consentement ant&#x00E9;rieur devenu inadapt&#x00E9; ou caduc (ex.&nbsp;: pratique du retrait de pr&#x00E9;servatif)&nbsp;; le Conseil d&#x2019;&#x00C9;tat relevant par ailleurs que la r&#x00E9;vocation du consentement ne pourrait &#x00EA;tre post&#x00E9;rieure &#x00E0; l&#x2019;acte.</p><p style=\"text-align: justify;\">La r&#x00E9;daction propos&#x00E9;e est celle du Conseil d&#x2019;&#x00C9;tat dans son avis sur la pr&#x00E9;sente proposition de loi, consid&#x00E9;rant 14.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-21", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0842P0D1N000006?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "id"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}