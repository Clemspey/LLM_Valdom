{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC0636P0D1N000076", "chronotag": "1739874716194", "legislature": "17", "identification": {"numeroLong": "76", "numeroOrdreDepot": "76", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC0636P0D1", "texteLegislatifRef": "PIONANR5L17BTC0636", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadzzyc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794778", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793780", "PA610968", "PA588884", "PA841701", "PA2940", "PA795362", "PA795454", "PA840997", "PA841885", "PA841507", "PA794008", "PA721210", "PA793452", "PA841351", "PA793980", "PA795010", "PA841419", "PA842013", "PA840947", "PA794022", "PA794146", "PA841223", "PA795636", "PA842045", "PA793708", "PA793664", "PA794154", "PA606545", "PA795076", "PA722142", "PA610002", "PA795808", "PA796018", "PA736201", "PA841713", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Regol,  Mme&#160;Arrighi, M.&#160;Amirshahi, Mme&#160;Autain, Mme&#160;Balage&#160;El&#160;Mariky, Mme&#160;Voynet, Mme&#160;Belluco, M.&#160;Ben&#160;Cheikh, M.&#160;Biteau, M.&#160;Arnaud&#160;Bonnet, M.&#160;Nicolas&#160;Bonnet, Mme&#160;Chatelain, M.&#160;Corbi&#232;re, M.&#160;Davi, M.&#160;Duplessy, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Damien&#160;Girard, M.&#160;Gustave, Mme&#160;Catherine&#160;Hervieu, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lahais, M.&#160;Lucas-Lundy, Mme&#160;Ozenne, M.&#160;Peytavie, Mme&#160;Pochon, M.&#160;Raux, M.&#160;Roum&#233;gas, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sas, Mme&#160;Sebaihi, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian, M.&#160;Tavernier et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0636.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "22", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 22"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par les deux alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;III. &#x2013; &#x00C0; titre exp&#x00E9;rimental, du 1<sup>er</sup> juillet 2025 au 31 d&#x00E9;cembre 2026, lorsqu&#x2019;ils proc&#x00E8;dent &#x00E0; l&#x2019;inspection visuelle des bagages, &#x00E0; leur fouille ou &#x00E0; des palpations de s&#x00E9;curit&#x00E9; en application de l&#x2019;article L.&nbsp;2251&#x2011;9 du code des transports, les agents des services internes de la SNCF et de la R&#x00E9;gie autonome des transports parisiens &#x00E9;tablissent un document indiquant le motif et les modalit&#x00E9;s du contr&#x00F4;le et permettant d&#x2019;identifier l&#x2019;agent l&#x2019;ayant effectu&#x00E9;. Ils en d&#x00E9;livrent une copie &#x00E0; la personne ayant fait l&#x2019;objet de la mesure.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le Gouvernement remet au Parlement, au plus tard le 1<sup>er</sup> septembre 2026, un rapport d&#x2019;&#x00E9;valuation de l&#x2019;exp&#x00E9;rimentation.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement du groupe &#x00C9;cologiste vise &#x00E0; exp&#x00E9;rimenter la mise en place du r&#x00E9;cepiss&#x00E9; lors des inspections visuelles, fouilles et palpations de s&#x00E9;curit&#x00E9; r&#x00E9;alis&#x00E9;es par les agents de la S&#x00FB;ret&#x00E9; ferroviaire et du GPSR. Il s&#x2019;agit d&#x2019;&#x00E9;valuer l&#x2019;efficacit&#x00E9; de ce dispositif dans la lutte contre les contr&#x00F4;les abusifs et discriminatoires et de juger de la pertinence de son extension aux forces de s&#x00E9;curit&#x00E9; int&#x00E9;rieure, en particulier pour les contr&#x00F4;les d&#x2019;identit&#x00E9;.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-05", "datePublication": "2025-02-11", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-02-11T17:24:05+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0636P0D1N000076?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29019", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "28864", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}