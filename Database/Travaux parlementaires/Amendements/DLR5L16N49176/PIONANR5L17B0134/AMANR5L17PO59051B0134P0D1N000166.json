ntAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement propose la suppression de l&#x2019;article&nbsp;16, qui pr&#x00E9;voyait initialement un fichier destin&#x00E9; &#x00E0; suivre la peine compl&#x00E9;mentaire d&#x2019;interdiction de para&#x00EE;tre dans un r&#x00E9;seau de transport public. Cet article a &#x00E9;t&#x00E9; modifi&#x00E9; au S&#x00E9;nat pour pr&#x00E9;voir une simple transmission d&#x2019;information au minist&#x00E8;re public. </p><p style=\"text-align: justify;\">Intimement li&#x00E9; &#x00E0; la mise en &#x0153;uvre de la peine compl&#x00E9;mentaire d&#x2019;interdiction de para&#x00EE;tre dans les r&#x00E9;seaux de transports, la suppression de cet article est coh&#x00E9;rente avec la suppression propos&#x00E9;e de l&#x2019;article&nbsp;13.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-22", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-11-27T17:33:45+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0134P0D1N000166?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSche