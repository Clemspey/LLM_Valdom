{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0134P0D1N000179", "chronotag": "1739874522978", "legislature": "17", "identification": {"numeroLong": "CL179", "numeroOrdreDepot": "179", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0134P0D1", "texteLegislatifRef": "PIONANR5L17B0134", "triAmendement": "akaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaci", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607619", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA822617"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Molac et Mme&#160;Froger"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0134.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; supprimer l&#x2019;article&nbsp;2 en ce qu&#x2019;il permettrait&nbsp;aux services de s&#x00E9;curit&#x00E9; de la SNCF et de la RATP d&#x2019;intervenir sur la voie publique aux abords des gares et stations, ce qui revient &#x00E0; leur confier une mission de surveillance g&#x00E9;n&#x00E9;rale de la voie publique entra&#x00EE;nant de fait une d&#x00E9;l&#x00E9;gation, &#x00E0; une personne priv&#x00E9;e, d&#x2019;une comp&#x00E9;tence de police administrative inh&#x00E9;rente &#x00E0; l&#x2019;exercice de la force publique.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">Une telle mesure est&nbsp;inconstitutionnelle, de plus, les agents de la SNCF et de la RATP ne sont pas form&#x00E9;s et encadr&#x00E9;s, &#x00E0; la diff&#x00E9;rence des forces de l&#x2019;ordre, pour exercer ces missions.</p><p style=\"text-align: justify;\">Cet amendement a &#x00E9;t&#x00E9; travaill&#x00E9; avec le Conseil National des Barreaux (CNB).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-22", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-11-27T15:14:45+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0134P0D1N000179?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "74362", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}