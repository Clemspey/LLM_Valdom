nstance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer l'alin&#x00E9;a 18.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de repli du groupe &#x00C9;cologiste vise &#x00E0; supprimer l&#x2019;inclusion dans le &#x201C;d&#x00E9;lit d&#x2019;incivilit&#x00E9; habituelle&#x201D; du fait de ne pas mettre d&#x2019;&#x00E9;tiquette sur son bagage. L&#x2019;absence totale de gravit&#x00E9; d&#x2019;une telle infraction, m&#x00EA;me r&#x00E9;p&#x00E9;t&#x00E9;e, entre en effet en contradiction flagrante avec la peine propos&#x00E9;e de 6 mois de prison et 7 500&#x20AC; d&#x2019;amende.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-20", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-27T16:52:26+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0134P0D1N000016?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": "s18"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema