tionnel": "false", "urlDivisionTexteVise": "/17/textes/0134.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;7, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;g&#x00EA;ner ou incommoder les voyageurs&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement de repli.</p><p>Cet amendement vise &#x00E0; supprimer les mots &#x00AB;&nbsp;g&#x00EA;ner ou incommoder les voyageurs&nbsp;&#x00BB; de l&#x2019;alin&#x00E9;a 7 de l&#x2019;article 1er.</p><p>L&#x2019;article 1er de la pr&#x00E9;sente proposition de loi vise &#x00E0; &#x00E9;tendre les pr&#x00E9;rogatives des agents de s&#x00FB;ret&#x00E9; de la SNCF et de la RATP, en leur octroyant le droit de confisquer des objets, autres que des armes, qui peuvent g&#x00EA;ner ou incommoder les voyageurs par leur quantit&#x00E9; ou insuffisance d&#x2019;emballage. Les mots &#x00AB;&nbsp;g&#x00EA;ner et incommoder&nbsp;&#x00BB; peuvent conduire &#x00E0; des confiscations arbitraires et laiss&#x00E9;es &#x00E0; seule appr&#x00E9;ciation de l&#x2019;agent de s&#x00FB;ret&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-22", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-27T12:41:47+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0134P0D1N000101?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "74474", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}