{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000592", "chronotag": "1742423533166", "legislature": "17", "identification": {"numeroLong": "592 (2\u00e8me Rect)", "numeroOrdreDepot": "592", "prefixeOrganeExamen": "AN", "numeroRect": "2"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "aecaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaati", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA842311", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Caure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23 quinquies", "articleDesignationCourte": "ART. 23 QUINQUIES", "articleDesignation": "ARTICLE 23 QUINQUIES", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_23_quinquies"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "16", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 16"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;16, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;224&#x2011;8-1.</i> &#x2013;&nbsp; Les dispositions de la pr&#x00E9;sente section ne sont pas applicables aux d&#x00E9;tenus b&#x00E9;n&#x00E9;ficiant du statut de collaborateurs de justice mentionn&#x00E9;s aux titre XXI <i>bis</i> du livre IV du code de proc&#x00E9;dure p&#x00E9;nale en application de l&#x2019;article&nbsp;706&#x2011;63&#x2011;1-A du code de proc&#x00E9;dure p&#x00E9;nale ou ayant b&#x00E9;n&#x00E9;fici&#x00E9; de ce statut dans le cadre de la proc&#x00E9;dure pour laquelle ils ex&#x00E9;cutent leur peine, sauf en cas de mise &#x00E0; ex&#x00E9;cution par le tribunal de l&#x2019;application des peines de tout ou partie de l&#x2019;emprisonnement d&#x00E9;cid&#x00E9; en application de l&#x2019;article&nbsp;132&#x2011;78&#x2011;1 du code p&#x00E9;nal.&nbsp;&#x00BB;&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le pr&#x00E9;sent amendement vise &#x00E0; exclure les collaborateurs de justice du dispositif permettant d&#x2019;affecter un d&#x00E9;tenu dans un quartier de lutte contre la criminalit&#x00E9; organis&#x00E9;e, en coh&#x00E9;rence avec la volont&#x00E9; de rendre plus attractif le dispositif de collaborateur de justice.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-14", "datePublication": "2025-03-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-19T23:32:00+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1043P0D1N000592?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29210", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "76114", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}