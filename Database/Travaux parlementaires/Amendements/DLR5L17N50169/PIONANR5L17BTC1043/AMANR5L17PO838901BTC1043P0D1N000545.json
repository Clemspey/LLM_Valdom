{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000545", "chronotag": "1742318923591", "legislature": "17", "identification": {"numeroLong": "545", "numeroOrdreDepot": "545", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "oeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaah", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721442", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719968", "PA718850", "PA793508", "PA718884", "PA793094", "PA266793", "PA642725", "PA266797", "PA722390", "PA721816"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Petex,  Mme&#160;Sylvie&#160;Bonnet, M.&#160;Cordier, M.&#160;Brigand, Mme&#160;Bazin-Malgras, M.&#160;Ray, M.&#160;Bony, M.&#160;Li&#233;geon, M.&#160;Gosselin, Mme&#160;Corneloup et M.&#160;Boucard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 22", "articleDesignationCourte": "ART. 22", "articleDesignation": "ARTICLE 22", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_22"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "49", "alineaDesignation": "Alin\u00e9a 49"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;49, apr&#x00E8;s le mot&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E9;lev&#x00E9;s&nbsp;&#x00BB;, </p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;en mati&#x00E8;re de s&#x00FB;ret&#x00E9; et de s&#x00E9;curit&#x00E9;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>L'article L.5332-16 du code de la s&#x00E9;curit&#x00E9; int&#x00E9;rieure vise &#x00E0; renforcer la s&#x00E9;curit&#x00E9; en exigeant une autorisation pour acc&#x00E9;der &#x00E0; toute installation portuaire pr&#x00E9;sentant des risques &#x00E9;lev&#x00E9;s, m&#x00EA;me en l'absence de zone &#x00E0; acc&#x00E8;s restreint. L'objectif de cet amendement est de pr&#x00E9;ciser que toute personne doit disposer d'une autorisation pour acc&#x00E9;der aux installations portuaires qui peuvent pr&#x00E9;senter des risques&nbsp;en mati&#x00E8;re de s&#x00FB;ret&#x00E9; et de s&#x00E9;curit&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-14", "datePublication": "2025-03-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1043P0D1N000545?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}