{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000861", "chronotag": "1742319103234", "legislature": "17", "identification": {"numeroLong": "861", "numeroOrdreDepot": "861", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "oeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaavo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA842311", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Caure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 22", "articleDesignationCourte": "ART. 22", "articleDesignation": "ARTICLE 22", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_22"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "76", "alineaDesignation": "Alin\u00e9a 76"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;76 les quatre alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;C <i>bis</i> Les articles L.&nbsp;5763&#x2011;1, L.&nbsp;5773&#x2011;1 et L.&nbsp;5783&#x2011;1 sont ainsi modifi&#x00E9;s&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Au deuxi&#x00E8;me alin&#x00E9;a, la r&#x00E9;f&#x00E9;rence&nbsp;: &#x00AB;&nbsp;L.&nbsp;5332&#x2011;1&nbsp;&#x00BB; est remplac&#x00E9;e par les mots&nbsp;: &#x00AB;&nbsp;L.&nbsp;5332&#x2011;2 &#x00E0; L.&nbsp;5332&#x2011;4, L.&nbsp;5332&#x2011;6, L.&nbsp;5332&#x2011;8, L.&nbsp;5332&#x2011;9, L.&nbsp;5332&#x2011;12, L.&nbsp;5332&#x2011;13 et L.&nbsp;5332&#x2011;19&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Apr&#x00E8;s le m&#x00EA;me deuxi&#x00E8;me alin&#x00E9;a, il est ins&#x00E9;r&#x00E9; un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les articles L.&nbsp;5332&#x2011;1, L.&nbsp;5332&#x2011;5, L.&nbsp;5332&#x2011;7, L.&nbsp;5332&#x2011;10, L.&nbsp;5332&#x2011;11 et L.&nbsp;5332&#x2011;14 &#x00E0; L.&nbsp;5332&#x2011;18&#x2011;2 s&#x2019;appliquent dans leur r&#x00E9;daction r&#x00E9;sultant de la loi n&#x00B0;&nbsp;&nbsp;&nbsp;&nbsp;du&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;visant &#x00E0; sortir la France du pi&#x00E8;ge du narcotrafic&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement de coordination permettant l'application des dispositions de l'article 22 modifiant le code des transports en Nouvelle-Cal&#x00E9;donie, en Polyn&#x00E9;sie fran&#x00E7;aise et &#x00E0; Wallis-et-Futuna.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-14", "datePublication": "2025-03-16", "soumisArticle40": "true", "etatDesTraitements": {"etat"