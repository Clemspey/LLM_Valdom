{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000812", "chronotag": "1742419183112", "legislature": "17", "identification": {"numeroLong": "812", "numeroOrdreDepot": "812", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "aecaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaali", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA842311", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Caure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23 quinquies", "articleDesignationCourte": "ART. 23 QUINQUIES", "articleDesignation": "ARTICLE 23 QUINQUIES", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_23_quinquies"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;14 par les mots et la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, sans pr&#x00E9;judice des dispositions des articles L.&nbsp;225&#x2011;1 &#x00E0; L.&nbsp;225&#x2011;5. Ces dispositions s&#x2019;appliquent sous r&#x00E9;serve des adaptations d&#x00E9;cid&#x00E9;es par l&#x2019;autorit&#x00E9; administrative comp&#x00E9;tente.&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement, propos&#x00E9; afin de tirer les cons&#x00E9;quences de l&#x2019;avis du Conseil d&#x2019;&#x00C9;tat relatif &#x00E0; la prise en charge des personnes d&#x00E9;tenues membres de la criminalit&#x00E9; organis&#x00E9;e du 14 mars 2025, vise &#x00E0; permettre &#x00E0; l&#x2019;autorit&#x00E9; administrative d&#x2019;individualiser le r&#x00E9;gime de fouilles int&#x00E9;grales auquel doit &#x00EA;tre soumis une personne d&#x00E9;tenue affect&#x00E9;e en quartier de lutte contre la criminalit&#x00E9; organis&#x00E9;e, par exemple pour des motifs tenant, en vertu des dispositions g&#x00E9;n&#x00E9;rales de l&#x2019;article L.&nbsp;6 du code p&#x00E9;nitentiaire, &#x00E0; l&#x2019;&#x00E2;ge, &#x00E0; l&#x2019;&#x00E9;tat de sant&#x00E9;, au handicap, &#x00E0; l&#x2019;identit&#x00E9; de genre ou &#x00E0; la personnalit&#x00E9; de chaque personne d&#x00E9;tenue.&nbsp;</p><p style=\"text-align: justify;\">Les modalit&#x00E9;s d&#x2019;application de ces dispositions et les conditions d&#x2019;adaptation du r&#x00E9;gime seront pr&#x00E9;cis&#x00E9;es par d&#x00E9;cret en Conseil d&#x2019;&#x00C9;tat, comme pr&#x00E9;vu par le nouvel article L.&nbsp;224&#x2011;9 du code p&#x00E9;nitentiaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-14", "datePublication": "2025-03-19", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-19T22:19:16+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1043P0D1N000812?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29210", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "76006", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}