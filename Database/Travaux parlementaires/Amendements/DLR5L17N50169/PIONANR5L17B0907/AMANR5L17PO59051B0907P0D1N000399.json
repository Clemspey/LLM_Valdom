{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0907P0D1N000399", "chronotag": "1741162675777", "legislature": "17", "identification": {"numeroLong": "CL399", "numeroOrdreDepot": "399", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0907P0D1", "texteLegislatifRef": "PIONANR5L17B0907", "triAmendement": "abiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA610968", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841701", "PA794022", "PA794778", "PA841351"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Amirshahi,  Mme&#160;Balage&#160;El&#160;Mariky, M.&#160;Iordanoff, Mme&#160;Regol et M.&#160;Duplessy"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0907.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin de l&#x2019;alin&#x00E9;a&nbsp;6, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, y compris par l&#x2019;acc&#x00E8;s &#x00E0; des traitements informatis&#x00E9;s de donn&#x00E9;es, dans des conditions garantissant notamment la confidentialit&#x00E9; de leurs &#x00E9;changes&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe &#x00C9;cologiste et Social vise &#x00E0; supprimer la possibilit&#x00E9;, pr&#x00E9;vue pour le futur &#x00AB;&nbsp;&#x00C9;tat-major anti-criminalit&#x00E9; organis&#x00E9;e&nbsp;&#x00BB;, d&#x2019;acc&#x00E9;der aux traitements informatis&#x00E9;s de donn&#x00E9;es sans encadrement ad&#x00E9;quat. La r&#x00E9;daction actuelle, trop vague, ne pr&#x00E9;cise ni les acteurs habilit&#x00E9;s &#x00E0; consulter ces donn&#x00E9;es, ni la nature exacte des fichiers concern&#x00E9;s. En l&#x2019;absence de garanties minimales, cette disposition contrevient au principe de minimisation des donn&#x00E9;es et soul&#x00E8;ve un risque important en mati&#x00E8;re de protection des libert&#x00E9;s individuelles.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-01", "datePublication": "2025-03-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-05T09:17:40+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0907P0D1N000399?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "8", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}