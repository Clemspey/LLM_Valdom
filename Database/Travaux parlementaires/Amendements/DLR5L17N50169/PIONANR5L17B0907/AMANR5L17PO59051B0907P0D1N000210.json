{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0907P0D1N000210", "chronotag": "1741274996092", "legislature": "17", "identification": {"numeroLong": "CL210", "numeroOrdreDepot": "210", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0907P0D1", "texteLegislatifRef": "PIONANR5L17B0907", "triAmendement": "dkaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaku", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608264", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794494", "PA340343", "PA840903", "PA840665", "PA335054", "PA840793", "PA841893", "PA720992", "PA841315", "PA841295", "PA793394", "PA840939", "PA840765", "PA841463", "PA794094"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "Mme&#160;Capdevielle,  M.&#160;Vicot, M.&#160;Saulignac, M.&#160;Christophle, Mme&#160;Allemand, Mme&#160;Karamanli, M.&#160;Pena, Mme&#160;Thi&#233;bault-Martinez, M.&#160;William, M.&#160;Benbrahim, M.&#160;Courbon, M.&#160;Delaporte, Mme&#160;Godard, M.&#160;Lhardit, Mme&#160;Mercier, M.&#160;Bouloux et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 11", "articleDesignationCourte": "ART. 11", "articleDesignation": "ARTICLE 11", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0907.asp#D_Article_11"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;3 par la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00C0; chaque prolongation de la dur&#x00E9;e de la garde &#x00E0; vue, la personne plac&#x00E9;e en garde &#x00E0; vue b&#x00E9;n&#x00E9;ficie des droits qui lui sont garantis par l&#x2019;article&nbsp;63&#x2011;1, ainsi que ceux pr&#x00E9;vus aux articles 63&#x2011;3, 63&#x2011;3&#x2011;1 &#x00E0; 63&#x2011;4&#x2011;3.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de repli du Groupe Socialistes et apparent&#x00E9;s, sugg&#x00E9;r&#x00E9; par l&#x2019;Ordre des Avocats au Barreau de Paris, vise &#x00E0; pr&#x00E9;ciser qu&#x2019;en cas de prolongation de la garde &#x00E0; vue comme le pr&#x00E9;voit l&#x2019;article 11 de la pr&#x00E9;sente proposition de loi, la personne plac&#x00E9;e en garde &#x00E0; vue continue &#x00E0; b&#x00E9;n&#x00E9;ficier des m&#x00EA;mes droits garantis par les articles 63-1, 63-3 et 63-4 du code de proc&#x00E9;dure p&#x00E9;nale, notamment le droit d&#x2019;&#x00EA;tre examin&#x00E9; par un m&#x00E9;decin en vue de statuer sur la compatibilit&#x00E9; de la mesure avec son &#x00E9;tat de sant&#x00E9;, d&#x2019;&#x00EA;tre assist&#x00E9;e par un avocat, et ce, &#x00E0; chaque fois que la garde &#x00E0; vue sera prolong&#x00E9;e.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-28", "datePublication": "2025-03-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-03-06T16:29:45+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0907P0D1N000210?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "9", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}