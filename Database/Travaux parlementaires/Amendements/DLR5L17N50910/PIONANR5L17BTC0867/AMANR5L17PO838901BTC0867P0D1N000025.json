{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC0867P0D1N000025", "chronotag": "1739874730822", "legislature": "17", "identification": {"numeroLong": "25", "numeroOrdreDepot": "25", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC0867P0D1", "texteLegislatifRef": "PIONANR5L17BTC0867", "triAmendement": "daaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840657", "groupePolitiqueRef": "PO847173", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841067", "PA841207", "PA840889", "PA840701", "PA267561", "PA330240", "PA840721", "PA840773", "PA841395", "PA841183", "PA840983", "PA793322", "PA840685", "PA841769", "PA840809"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Ricourt&#160;Vaginay,  M.&#160;Allegret-Pilot, M.&#160;Alloncle, M.&#160;Bloch, M.&#160;Chaix, Mme&#160;Bar&#232;ges, M.&#160;Ciotti, M.&#160;Chavent, M.&#160;Fayssat, M.&#160;Michelet, Mme&#160;Mansouri, M.&#160;Lenoir, Mme&#160;D&apos;Intorni, M.&#160;Tr&#233;buchet, M.&#160;Michoux et M.&#160;Verny"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0867.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">La section 1 du chapitre I<sup>er</sup> du titre III du livre IV du code p&#x00E9;nal est compl&#x00E9;t&#x00E9;e par un article&nbsp;431&#x2011;2&#x2011;1 ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;431&#x2011;2&#x2011;1.</i> &#x2013;&nbsp;Les personnes morales qui incitent, organisent ou soutiennent des actes de perturbation ou d&#x2019;obstruction dans le but d&#x2019;emp&#x00EA;cher le d&#x00E9;roulement d&#x2019;activit&#x00E9;s l&#x00E9;gales et autoris&#x00E9;es, comme mentionn&#x00E9; aux articles 431&#x2011;1 et 431&#x2011;2&#x2011;1, sont passibles de la sanction suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x2013; l&#x2019;interdiction de b&#x00E9;n&#x00E9;ficier de tout financement public ou de subvention pour une dur&#x00E9;e de cinq ans.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement introduit une sanction &#x00E0; l&#x2019;encontre des personnes morales organisant des actions de perturbation et d&#x2019;obstruction, en pr&#x00E9;voyant une interdiction de b&#x00E9;n&#x00E9;ficier de tout argent public.</p><p>Cela vise &#x00E0; dissuader l'organisation de ces actions.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-03", "datePublication": "2025-02-05", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0867P0D1N000025?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c p\u00e9nal", "divisionCodeLoi": "431-2-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}