{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1029P0D1N000056", "chronotag": "1741645225846", "legislature": "17", "identification": {"numeroLong": "56", "numeroOrdreDepot": "56", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1029P0D1", "texteLegislatifRef": "PIONANR5L17BTC1029", "triAmendement": "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaakzy", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793210", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA721474", "PA267763", "PA840817", "PA719138", "PA794554", "PA942", "PA841359", "PA267042", "PA822617", "PA1592", "PA841017", "PA795244", "PA720976", "PA841931", "PA607619", "PA721486", "PA721110", "PA720960", "PA793796", "PA2952", "PA796078"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Panifous,  M.&#160;Viry, M.&#160;Bataille, M.&#160;Bruneau, M.&#160;Castellani, M.&#160;Castiglione, M.&#160;de&#160;Courson, Mme&#160;de&#160;P&#233;lichy, M.&#160;Favennec-B&#233;cot, Mme&#160;Froger, M.&#160;Habib, M.&#160;Huwart, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Mazaury, M.&#160;Molac, M.&#160;Naegelen, Mme&#160;Sanquer, M.&#160;Serva, M.&#160;Taupiac, M.&#160;Warsmann et Mme&#160;Youssouffa"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1029.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "10", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 10"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;10, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;<i>bis</i>&nbsp;Dans le cadre de son r&#x00F4;le propre et de son r&#x00F4;le prescrit, participer aux soins de premier recours en acc&#x00E8;s direct comme d&#x00E9;fini &#x00E0; l&#x2019;article L.&nbsp;1411&#x2011;11 du code de la sant&#x00E9; publique&nbsp;;&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement, travaill&#x00E9; avec le SNPI, propose de reconnaitre la participation des infirmiers, notamment lib&#x00E9;raux, aux soins de premier recours.</p><p style=\"text-align: justify;\">L'article 1er bis, ajout&#x00E9;&nbsp;en commission, reconnait d&#x00E9;sormais le r&#x00F4;le des infirmiers, y compris les IPA, pour les soins de premiers recours. Cet amendement s'inscrit en compl&#x00E9;ment, en le reconnaissant comme une mission &#x00E0; part enti&#x00E8;re.</p><p style=\"text-align: justify;\">Le premier recours contribue &#x00E0; l&#x2019;offre de soins ambulatoire en assurant aux patients la pr&#x00E9;vention, le d&#x00E9;pistage, le diagnostic, le traitement, l&#x2019;orientation et le suivi ainsi que l&#x2019;&#x00E9;ducation pour la sant&#x00E9;.&nbsp;</p><p style=\"text-align: justify;\">Cela&nbsp;favorisera l&#x2019;acc&#x00E8;s aux soins des patients sur l&#x2019;ensemble du territoire notamment dans les d&#x00E9;serts m&#x00E9;dicaux.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-06", "datePublication": "2025-03-10", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-10T23:20:20+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1029P0D1N000056?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29158", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "75904", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}