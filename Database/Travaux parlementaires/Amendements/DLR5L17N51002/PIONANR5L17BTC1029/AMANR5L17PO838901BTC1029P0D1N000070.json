ttachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1029.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "8", "alineaDesignation": "Alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;8, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;sont&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;notamment&nbsp;&#x00BB;.&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; sortir du cadre rigide du d&#x00E9;cret d&#x2019;actes gr&#x00E2;ce au passage &#x00E0; missions, tout en se r&#x00E9;servant la possibilit&#x00E9; d&#x2019;ajouter d&#x2019;autres missions par voie r&#x00E9;glementaire.&nbsp;</p><p style=\"text-align: justify;\">&nbsp;L'amendement&nbsp;offre ainsi une flexibilit&#x00E9; indispensable, en reconnaissant de mani&#x00E8;re &#x00E9;volutive l&#x2019;ensemble des comp&#x00E9;tences infirmi&#x00E8;res et en facilitant leur adaptation aux mutations du secteur.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-06", "datePublication": "2025-03-10", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-03-10T23:00:27+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"no