{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0519P0D1N000015", "chronotag": "1739874687226", "legislature": "17", "identification": {"numeroLong": "AC15", "numeroOrdreDepot": "15", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0519P0D1", "texteLegislatifRef": "PIONANR5L17B0519", "triAmendement": "oaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793716", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719558", "PA794702", "PA795258", "PA720256"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Croizier,  M.&#160;Balanant, Mme&#160;Lingemann, M.&#160;Gumbs et Mme&#160;Bannier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "visant \u00e0 rendre accessible \u00e0 tous les \u00e9tudiants le repas \u00e0 1 euro,", "articleDesignationCourte": "TITRE", "articleDesignation": "TITRE", "type": "TITRE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0519.asp#D_visant_a_rendre_accessible_a_tous_les_"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;visant &#x00E0;&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">&nbsp;r&#x00E9;diger ainsi la fin de l&#x2019;intitul&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&nbsp;&#x00AB;&nbsp;garantir un tarif r&#x00E9;duit aux &#x00E9;tudiants boursiers et pr&#x00E9;caires dans les sites de restauration g&#x00E9;r&#x00E9;s par les centres r&#x00E9;gionaux des &#x0153;uvres universitaires et scolaires&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">En coh&#x00E9;rence avec l&#x2019;amendement de r&#x00E9;&#x00E9;criture de l&#x2019;article&nbsp;1<sup>er</sup>, cet amendement souhaite modifier le titre de la pr&#x00E9;sente proposition de loi pour pr&#x00E9;ciser que les repas &#x00E0; 1&nbsp;euro ne concernent que les &#x00E9;tudiants boursiers ou pr&#x00E9;caires.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-29", "datePublication": "2024-12-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-12-04T15:03:00+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0519P0D1N000015?format=