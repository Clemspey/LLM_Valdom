DivisionTexteVise": "/17/textes/0698.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin de l&#x2019;alin&#x00E9;a&nbsp;6, substituer aux mots&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ainsi qu&#x2019;&#x00E0; 25&nbsp;% de la surface totale sur l&#x2019;ensemble d&#x2019;un d&#x00E9;partement d&#x2019;outre-mer&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou est fix&#x00E9; par&nbsp;le repr&#x00E9;sentant de l&#x2019;&#x00C9;tat territorialement comp&#x00E9;tent&nbsp;au regard de la surface totale occup&#x00E9;e par les enseignes d&#x2019;un m&#x00EA;me groupe sur la collectivit&#x00E9; concern&#x00E9;e&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Il s&#x2019;agit de remettre en vigueur une disposition vot&#x00E9;e en 2003 dont l&#x2019;objectif &#x00E9;tait de limiter, dans les d&#x00E9;partements d&#x2019;Outre-mer, la constitution de monopoles (ou de quasi-monopoles) dans le domaine de la grande distribution par le biais de leur superficie.&nbsp; Au moment o&#x00F9; les crises qui secouent l&#x2019;outre-mer ont toutes mises en &#x00E9;vidence la chert&#x00E9; de la vie et par cons&#x00E9;quent le probl&#x00E8;me de la formation et de la transparence des prix, il est important&nbsp;de revenir sur une suppression qui favorise les situations de quasi-monopole contre lesquelles il est&nbsp;devenu encore plus urgent de lutter.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-20", "datePublication": "2025-01-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-23T11:39:16+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0698P0D1N000051?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28912", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "74960", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}