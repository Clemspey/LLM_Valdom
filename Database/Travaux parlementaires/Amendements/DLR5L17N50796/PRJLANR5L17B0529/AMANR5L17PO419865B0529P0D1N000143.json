{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419865B0529P0D1N000143", "chronotag": "1739874681047", "legislature": "17", "identification": {"numeroLong": "CD143", "numeroOrdreDepot": "143", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419865B0529P0D1", "texteLegislatifRef": "PRJLANR5L17B0529", "triAmendement": "iiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaastzzs", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722336", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Thi&#233;baut, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 27", "articleDesignationCourte": "ART. 27", "articleDesignation": "ARTICLE 27", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0529.asp#D_Article_27"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "96", "alineaDesignation": "Alin\u00e9a 96"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013; Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;96 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;les deux derniers alin&#x00E9;as de l&#x2019;article L.&nbsp;221&#x2011;7&#x2011;1 du code de l&#x2019;&#x00E9;nergie, dans leur r&#x00E9;daction r&#x00E9;sultant du pr&#x00E9;sent article, l&#x2019;article L.&nbsp;236&#x2011;1 du m&#x00EA;me code et le III du pr&#x00E9;sent article entrent en vigueur le lendemain de la publication de la pr&#x00E9;sente loi au Journal officiel.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II. &#x2013; En cons&#x00E9;quence, supprimer les alin&#x00E9;as 97 et 98.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; supprimer l&#x2019;entr&#x00E9;e en vigueur des deux derniers alin&#x00E9;as de l&#x2019;article L. 221-7-1 du code de l&#x2019;&#x00E9;nergie le 1er janvier 2025, date fix&#x00E9;e par l&#x2019;article 35 de la directive (UE) 2024/1275 du Parlement europ&#x00E9;en et du Conseil du 24 avril 2024 sur la performance &#x00E9;nerg&#x00E9;tique des b&#x00E2;timents. Diff&#x00E9;rer cette entr&#x00E9;e en vigueur n&#x2019;&#x00E9;tait utile pour &#x00EA;tre en conformit&#x00E9; avec le droit europ&#x00E9;en que si l&#x2019;ensemble de la loi &#x00E9;tait promulgu&#x00E9;e avant cette date, ce qui, en raison du temps n&#x00E9;cessaire pour que la discussion parlementaire arrive &#x00E0; son terme, n&#x2019;est pas envisageable.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-25", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-11-27T11:25:12+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419865B0529P0D1N000143?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "z", "divisionCodeLoi": null}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}