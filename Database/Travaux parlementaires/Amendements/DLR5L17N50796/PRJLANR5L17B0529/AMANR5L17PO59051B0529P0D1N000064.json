{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59051B0529P0D1N000064", "chronotag": "1739874683020", "legislature": "17", "identification": {"numeroLong": "CL64", "numeroOrdreDepot": "64", "prefixeOrganeExamen": "CION_LOIS", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59051B0529P0D1", "texteLegislatifRef": "PRJLANR5L17B0529", "triAmendement": "dbaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA266797", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Gosselin, rapporteur"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 16", "articleDesignationCourte": "ART. 16", "articleDesignation": "ARTICLE 16", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0529.asp#D_Article_16"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "2", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;2, ins&#x00E9;rer les trois alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;<i>bis</i> Au 1&#x00B0;&nbsp;du IV, le mot&nbsp;: &#x00AB;&nbsp;cinq&nbsp;&#x00BB; est remplac&#x00E9; par le mot&nbsp;: &#x00AB;&nbsp;deux&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;<i>ter</i> Apr&#x00E8;s le m&#x00EA;me 1&#x00B0;&nbsp;du IV, il est ins&#x00E9;r&#x00E9; un 1&#x00B0;&nbsp;<i>bis</i> ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00AB;&nbsp;1&#x00B0;&nbsp;<i>bis</i> Les associations r&#x00E9;guli&#x00E8;rement d&#x00E9;clar&#x00E9;es agissant pour le compte soit d&#x2019;au moins cinquante personnes physiques, soit d&#x2019;au moins cinq personnes morales de droit priv&#x00E9; inscrites au registre du commerce et des soci&#x00E9;t&#x00E9;s depuis au moins deux ans, soit d&#x2019;au moins cinq collectivit&#x00E9;s territoriales ou groupements de collectivit&#x00E9;s se d&#x00E9;clarant victimes d&#x2019;un dommage caus&#x00E9; par le d&#x00E9;fendeur et r&#x00E9;pondant aux conditions pr&#x00E9;vues au II du pr&#x00E9;sent article.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement de repli vise &#x00E0; ouvrir la qualit&#x00E9; pour agir en mati&#x00E8;re d&#x2019;action de groupe &#x00AB;&nbsp;Donn&#x00E9;es personnelles&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-26", "datePublication": "2024-11-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-27T10:11:28+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0529P0D1N000064?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}