{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC0631P0D1N000158", "chronotag": "1739874706775", "legislature": "17", "identification": {"numeroLong": "158 (Rect)", "numeroOrdreDepot": "158", "prefixeOrganeExamen": "AN", "numeroRect": "1"}, "examenRef": "EXANR5L17PO838901BTC0631P0D1", "texteLegislatifRef": "PRJLANR5L17BTC0631", "triAmendement": "cfaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaczy", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA266797", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Gosselin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0631.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "110", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 110"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;110, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;XV <i>bis</i>.&nbsp;&#x2013;&nbsp;&#x00C0; la premi&#x00E8;re phrase&nbsp; de l&#x2019;article&nbsp;38 de la loi n&#x00B0;&nbsp;78&#x2011;17 du 6 janvier 1978 relative &#x00E0; l&#x2019;informatique, aux fichiers et aux libert&#x00E9;s, les mots&nbsp;:&nbsp;&#x00AB;&nbsp;ou une organisation mentionn&#x00E9;e au IV de l&#x2019;article&nbsp;37&nbsp;&#x00BB; sont remplac&#x00E9;s par les mots&nbsp;: &#x00AB;&nbsp;r&#x00E9;guli&#x00E8;rement d&#x00E9;clar&#x00E9;e depuis cinq ans au moins ayant dans son objet statutaire la protection de la vie priv&#x00E9;e ou la protection des donn&#x00E9;es &#x00E0; caract&#x00E8;re personnel, une association de d&#x00E9;fense des consommateurs repr&#x00E9;sentatives au niveau national et agr&#x00E9;&#x00E9;es en application de l&#x2019;article L.&nbsp;811&#x2011;1 du code de la consommation, lorsque le traitement de donn&#x00E9;es &#x00E0; caract&#x00E8;re personnel affecte des consommateurs, une organisation syndicale de salari&#x00E9;s ou de fonctionnaires repr&#x00E9;sentative au sens des articles L.&nbsp;2122&#x2011;1, L.&nbsp;2122&#x2011;5 ou L.&nbsp;2122&#x2011;9 du code du travail ou de l'article L.&nbsp;222-2 du code g&#x00E9;n&#x00E9;ral de la fonction publique ou les syndicats repr&#x00E9;sentatifs de magistrats de l&#x2019;ordre judiciaire, lorsque le traitement affecte les int&#x00E9;r&#x00EA;ts des personnes que les statuts de cette organisation la chargent de d&#x00E9;fendre&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement de coordination</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-16", "datePublication": "2025-02-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-02-17T17:53:49+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0631P0D1N000158?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29057", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}