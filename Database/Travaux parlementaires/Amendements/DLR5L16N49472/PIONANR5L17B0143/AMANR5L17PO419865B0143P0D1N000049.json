{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419865B0143P0D1N000049", "chronotag": "1739957306111", "legislature": "17", "identification": {"numeroLong": "CD49", "numeroOrdreDepot": "49", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419865B0143P0D1", "texteLegislatifRef": "PIONANR5L17B0143", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA795636", "PA795362", "PA841507", "PA842045", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Pochon,  M.&#160;Lucas-Lundy, Mme&#160;Belluco, M.&#160;Nicolas&#160;Bonnet, Mme&#160;Ozenne et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0143.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;-&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, supprimer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;asiatique&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;-&nbsp;En cons&#x00E9;quence, supprimer toutes les occurences du m&#x00EA;me mot &#x00E0; l&#x2019;article unique. </p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Par commodit&#x00E9;s, nous parlons commun&#x00E9;ment de &#x00AB;&nbsp;frelons asiatiques&nbsp;&#x00BB; pour parler en r&#x00E9;alit&#x00E9; d&#x2019;une sous-esp&#x00E8;ce de vespa velutina, le vespa velutina nigrithorax. Cet amendement propose donc de substituer &#x00E0; toute mention du nom familier de &#x00AB;&nbsp;frelons asiatiques&nbsp;&#x00BB;, dans la pr&#x00E9;sente proposition de loi, le nom plus explicite de &#x00AB;&nbsp;frelons &#x00E0; pattes jaunes&nbsp;&#x00BB; qui est une appellation plus neutre et plus signifiante.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-14", "datePublication": "2025-02-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-02-19T10:28:13+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSc