{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC0994P0D1N000074", "chronotag": "1741275595870", "legislature": "17", "identification": {"numeroLong": "74 (Rect)", "numeroOrdreDepot": "74", "prefixeOrganeExamen": "AN", "numeroRect": "1"}, "examenRef": "EXANR5L17PO838901BTC0994P0D1", "texteLegislatifRef": "PIONANR5L17BTC0994", "triAmendement": "jiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA267318", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Grelier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0994.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi les alin&#x00E9;as&nbsp;2&nbsp;et 3&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;4139&#x2011;18</i>. &#x2013;&nbsp;Les militaires du service de sant&#x00E9; des arm&#x00E9;es&nbsp;peuvent b&#x00E9;n&#x00E9;ficier, dans le cadre des dispositifs d&#x2019;acc&#x00E8;s &#x00E0; la fonction publique civile pr&#x00E9;vus par les articles L.&nbsp;4139&#x2011;2 et L.&nbsp;4139&#x2011;3, de modalit&#x00E9;s simplifi&#x00E9;es d&#x2019;int&#x00E9;gration directe dans le cadre d&#x2019;emplois de sapeurs-pompiers professionnels relevant de la profession qu'ils exer&#x00E7;aient au service de sant&#x00E9; des arm&#x00E9;es.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Un d&#x00E9;cret en Conseil d&#x2019;&#x00C9;tat d&#x00E9;termine&nbsp;les modalit&#x00E9;s d&#x2019;application du pr&#x00E9;sent article.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement du rapporteur inscrit le mouvement statutaire pr&#x00E9;vu par l'article 6 dans le cadre des dispositifs existants du code de la d&#x00E9;fense s&#x2019;agissant de la reconversion des militaires (d&#x00E9;tachement-int&#x00E9;gration, stage-int&#x00E9;gration et nomination aux emplois r&#x00E9;serv&#x00E9;s).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-03", "datePublication": "2025-03-06", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-06T16:39:28+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0994P0D1N000074?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29103", "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}