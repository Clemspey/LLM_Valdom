{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B0324P1D1N001125", "chronotag": "1739874538742", "legislature": "17", "identification": {"numeroLong": "I-CF1125", "numeroOrdreDepot": "1125", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B0324P1D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "iiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaamo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719138", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA267763"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Castellani et M.&#160;Bataille"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 26", "articleDesignationCourte": "APR\u00c8S ART. 26", "articleDesignation": "APR\u00c8S L'ARTICLE 26, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324A.asp#D_Article_26"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Au 2&#x00B0;&nbsp;de l&#x2019;article L.&nbsp;453&#x2011;70 du code des impositions sur les biens et les services, le taux&nbsp;: &#x00AB;&nbsp;3&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;5&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objet de cet amendement est d&#x2019;accroitre le taux de la taxe sur les services num&#x00E9;riques en faisant &#x00E9;voluer le taux de 3&nbsp;% &#x00E0; 5&nbsp;%.&nbsp;</p><p style=\"text-align: justify;\">Le groupe LIOT souhaite,&nbsp;dans un esprit de responsabilit&#x00E9;, concourir au redressement des comptes publics en ne ciblant d&#x2019;une part que les tr&#x00E8;s hauts revenus et les grandes entreprises les plus prosp&#x00E8;res.&nbsp;</p><p style=\"text-align: justify;\">Le rendement de cette taxe en 2023 a &#x00E9;t&#x00E9; de presque 700 millions d&#x2019;euros. Pour rappel, elle ne concerne que les grandes entreprises du secteur num&#x00E9;rique qui r&#x00E9;alisent un chiffre d&#x2019;affaires mondial sup&#x00E9;rieur &#x00E0; 750 millions d&#x2019;euros et un chiffre d&#x2019;affaires en France sup&#x00E9;rieur &#x00E0; 25 millions d&#x2019;euros sur certaines activit&#x00E9;s num&#x00E9;riques.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-13", "datePublication": "2024-10-19", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-10-19T11:53:39+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P1D1N001125?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "CIBS", "divisionCodeLoi": "453-70"}, "discussionCommune": {"idDiscussion": "25782", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "71854", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}