{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0324P1D1N001229", "chronotag": "1739874633263", "legislature": "17", "identification": {"numeroLong": "I-1229", "numeroOrdreDepot": "1229", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0324P1D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "iiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaxzo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840979", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722390", "PA642847", "PA793094", "PA793508", "PA721442", "PA719968", "PA267794", "PA718850", "PA607090", "PA712015", "PA266793"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Le&#160;Fur,  Mme&#160;Corneloup, M.&#160;Bazin, M.&#160;Ray, M.&#160;Brigand, Mme&#160;Petex, Mme&#160;Sylvie&#160;Bonnet, Mme&#160;Tabarot, M.&#160;Cordier, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Dive et M.&#160;Bony"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 26", "articleDesignationCourte": "APR\u00c8S ART. 26", "articleDesignation": "APR\u00c8S L'ARTICLE 26, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324A.asp#D_Article_26"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">A&#x0300; la fin du V de l&#x2019;article&nbsp;235 <i>ter</i> ZD du code ge&#x0301;ne&#x0301;ral des impo&#x0302;ts, le taux&nbsp;:&nbsp;&#x00AB;&nbsp;0,3&nbsp;%&nbsp;&#x00BB; est remplace&#x0301; par le taux&nbsp;:&nbsp;&#x00AB;&nbsp;0,5&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; augmenter le taux de la taxe sur les transactions financi&#x00E8;res &#x00E0; 0,5&nbsp;% contre 0,3&nbsp;% actuellement. En se basant sur l&#x2019;&#x00E9;valuation des recettes de la TTF pr&#x00E9;vue dans le projet de loi de finances pour 2025, cette augmentation du taux permettrait &#x00E0; l&#x2019;&#x00C9;tat de r&#x00E9;colter plus de 3 milliards d&#x2019;euros de recettes, contre 1,86 milliard pour un taux &#x00E0; 0,3&nbsp;%.&nbsp;</p><p style=\"text-align: justify;\">Quand bien m&#x00EA;me les recettes nouvelles n&#x2019;augmenteraient pas aussi lin&#x00E9;airement, il est incontestable que les montants en jeux seraient tr&#x00E8;s cons&#x00E9;quents.</p><p style=\"text-align: justify;\">L&#x2019;objectif assum&#x00E9; par cet amendement est d&#x2019;accro&#x00EE;tre la ressource publique disponible pour le financement de l&#x2019;aide internationale de la France.</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-11-08", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-08T15:36:42+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P1D1N001229?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28633", "article99": "false", "loiReference": {"codeLoi": "CGI", "divisionCodeLoi": "235 ter ZD"}, "discussionCommune": {"idDiscussion": "26694", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "72918", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}