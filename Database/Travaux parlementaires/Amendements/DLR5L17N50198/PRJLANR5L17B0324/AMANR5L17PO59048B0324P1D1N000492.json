, Mme&#160;Pochon, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sebaihi, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian, M.&#160;Tavernier, M.&#160;Thierry et Mme&#160;Voynet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 26", "articleDesignationCourte": "APR\u00c8S ART. 26", "articleDesignation": "APR\u00c8S L'ARTICLE 26, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324A.asp#D_Article_26"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin du V de l&#x2019;article&nbsp;235 <i>ter</i> ZD du code g&#x00E9;n&#x00E9;ral des imp&#x00F4;ts, le taux&nbsp;: &#x00AB;&nbsp;0,3&nbsp;%&nbsp;&#x00BB; est remplac&#x00E9; par le taux&nbsp;: &#x00AB;&nbsp;0,5&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement de repli - Cet amendement propose de relever le taux de la taxe sur les transactions financi&#x00E8;res (TTF) &#x00E0; 0,5 %, afin d'augmenter son rendement et d'aligner la France avec les autres grandes places financi&#x00E8;res europ&#x00E9;ennes. Le renforcement de la TTF n'aurait pas d'impact n&#x00E9;gatif sur l'&#x00E9;conomie fran&#x00E7;aise, comme l'ont d&#x00E9;montr&#x00E9; les pr&#x00E9;c&#x00E9;dentes hausses de 2016 et 2017, sans baisse du nombre de transactions ni de l'attractivit&#x00E9; de Paris, au contraire.</p><p style=\"text-align: justify;\">Actuellement sous-utilis&#x00E9;e avec un taux de 0,3 % et un champ d'application limit&#x00E9;, cette r&#x00E9;forme permettrait d'am&#x00E9;liorer son efficacit&#x00E9;, d'o&#x00F9; la proposition d'atteindre 0,5 %.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-12", "datePublication": "2024-10-19", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-10-19T11:00:08+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P1D1N000492?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "CGI", "divisionCodeLoi": "235 ter ZD"}, "discussionCommune": {"idDiscussion": "25780", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "71356", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}