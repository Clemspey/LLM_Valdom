": [{"libelle": "Pr\u00e9sidence de la R\u00e9publique", "programmeRef": "523558", "autorisationEngagement": "-3000000", "creditPaiement": "-3000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Assembl\u00e9e nationale", "programmeRef": "523560", "autorisationEngagement": "3000000", "creditPaiement": "3000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00e9nat", "programmeRef": "523562", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "La Cha\u00eene parlementaire", "programmeRef": "523564", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Indemnit\u00e9s des repr\u00e9sentants fran\u00e7ais au Parlement europ\u00e9en", "programmeRef": "523566", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conseil constitutionnel", "programmeRef": "523568", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Haute Cour", "programmeRef": "523570", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cour de justice de la R\u00e9publique", "programmeRef": "523572", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Par cet amendement, les d&#x00E9;put&#x00E9;.es du groupe LFI-NFP souhaitent r&#x00E9;cup&#x00E9;rer les d&#x00E9;penses de luxe du Pr&#x00E9;sident de la R&#x00E9;publique au b&#x00E9;n&#x00E9;fice des travailleurs les plus pr&#x00E9;caires de l'Assembl&#x00E9;e nationale.</p><p style=\"text-align: justify;\">Les repas et r&#x00E9;ceptions pour l'accueil du roi Charles III, du Premier ministre indien Narendra Modi ainsi que la r&#x00E9;ception du pr&#x00E9;sident mongol, Ukhnaagiin Kh&#x00FC;rels&#x00FC;kh ont co&#x00FB;t&#x00E9; pr&#x00E8;s d'un million d'euros. Ces r&#x00E9;ceptions ind&#x00E9;centes, alors que la France subissait une inflation record depuis 20 ans, que des millions de Fran&#x00E7;ais se privaient pour manger, ne sont pas acceptables. Un Pr&#x00E9;sident de la R&#x00E9;publique ne devrait pas agir comme &#x00E7;a ! Nous proposons de r&#x00E9;cup&#x00E9;rer ce million et de le red&#x00E9;ployer dans le budget de l'Assembl&#x00E9;e nationale, afin de revaloriser les salaires des travailleurs les plus pr&#x00E9;caires au sein de ces institutions : le personnel d'entretien notamment, ainsi que le personnel fonctionnaire b&#x00E9;n&#x00E9;ficiant des plus basses indemnit&#x00E9;s.</p><p style=\"text-align: justify;\">Pour ce faire, nous proposons de red&#x00E9;ployer 3 000 000 d'euros en AE et en CP de l'action 01 &#x00AB; Pr&#x00E9;sidence de la R&#x00E9;publique &#x00BB; du programme 501 &#x00AB; Pr&#x00E9;sidence de la R&#x00E9;publique &#x00BB; au sein de l'action 01 du programme &#x00AB; Assembl&#x00E9;e nationale &#x00BB;, notamment en ce qui concerne le financement des entreprises d'entretien, dans le but de revaloriser les salaires des travailleurs, ainsi qu'en ce qui concerne la revalorisation des salaires des fonctionnaires les plus pr&#x00E9;caires de l'Assembl&#x00E9;e nationale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-08", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003117?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "E", "divisionCodeLoi": "33"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}