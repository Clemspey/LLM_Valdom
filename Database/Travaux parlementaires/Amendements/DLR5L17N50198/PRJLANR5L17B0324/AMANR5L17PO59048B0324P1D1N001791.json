Standard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013; Au d&#x00E9;but du k) du II de l&#x2019;article&nbsp;244 <i>quater</i> B du code g&#x00E9;n&#x00E9;ral des imp&#x00F4;ts, l&#x2019;ann&#x00E9;e&nbsp;: &#x00AB;&nbsp;2024&nbsp;&#x00BB; est remplac&#x00E9;e par l&#x2019;ann&#x00E9;e&nbsp;: &#x00AB;&nbsp;2027&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II. &#x2013; La perte de recettes pour l&#x2019;&#x00C9;tat est compens&#x00E9;e &#x00E0; due concurrence par la cr&#x00E9;ation d&#x2019;une taxe additionnelle &#x00E0; l&#x2019;accise sur les tabacs pr&#x00E9;vue au chapitre IV du titre I<sup>er</sup> du livre III du code des impositions sur les biens et services.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>&nbsp;Le CII est une extension du CIR qui permet de financer les prototypes de pr&#x00E8;s de 10 000 PME partout sur le territoire. Il est essentiel de proroger ce dispositif pour la vitalit&#x00E9; de notre tissu industriel et entrepreneurial et pour pr&#x00E9;server sa capacit&#x00E9; d&#x2019;innovation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-13", "datePublication": "2024-10-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-18T17:05:54+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P1D1N001791?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "CGI", "divisionCodeLoi": "244 quater B"}, "discussionCommune": {"idDiscussion": "25678", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "71604", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}