nEngagement": "-1181467408", "creditPaiement": "-1181467408", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'\u00e9cologie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "454642", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds d'acc\u00e9l\u00e9ration de la transition \u00e9cologique dans les territoires", "programmeRef": "454644", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00fbret\u00e9 nucl\u00e9aire et radioprotection", "programmeRef": "454646", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-1181467408", "soldeCP": "-1181467408", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Dans la continuit&#x00E9; des arguments d&#x00E9;velopp&#x00E9;s pour les autres &#x00E9;nergies renouvelables, nous consid&#x00E9;rons que les investissements publics doivent se limiter &#x00E0; la phase de d&#x00E9;veloppement des prototypes industriels. Une fois qu&#x2019;une technologie a atteint sa maturit&#x00E9;, il n&#x2019;incombe plus &#x00E0; l&#x2019;&#x00C9;tat de la subventionner pour garantir sa comp&#x00E9;titivit&#x00E9; sur le march&#x00E9; de l&#x2019;&#x00E9;lectricit&#x00E9;. Prolonger ce soutien financier ne fait que consolider des rentes pour les op&#x00E9;rateurs, en d&#x00E9;connexion totale de la production et de la viabilit&#x00E9; r&#x00E9;elle de ces &#x00E9;nergies.<br>&nbsp;<br><b>Par cons&#x00E9;quent, le pr&#x00E9;sent amendement, en coh&#x00E9;rence avec le programme du Rassemblement national, pr&#x00E9;voit ainsi de minorer les cr&#x00E9;dits, en autorisations d&#x2019;engagement et en cr&#x00E9;dits de paiement, d&#x2019;un montant de 1 181 467 408&nbsp;euros la sous-action n&#x00B0;&nbsp;10.01 &#x00AB;&nbsp;Soutien &#x00E0; l&#x2019;injection de biom&#x00E9;thane&nbsp;&#x00BB; de l&#x2019;action n&#x00B0;&nbsp;09 &#x00AB;&nbsp;Soutien aux &#x00E9;nergies renouvelables &#x00E9;lectriques en m&#x00E9;tropole continentale&nbsp;&#x00BB; du programme n&#x00B0;&nbsp;345 &#x00AB;&nbsp;Service public de l&#x2019;&#x00E9;nergie&nbsp;&#x00BB;.</b></p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-29T19:03:12+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0324P2D1N000260?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLo