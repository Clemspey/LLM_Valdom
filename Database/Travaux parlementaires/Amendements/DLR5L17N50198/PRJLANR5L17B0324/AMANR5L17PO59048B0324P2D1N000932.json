orps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Comp\u00e9titivit\u00e9 et durabilit\u00e9 de l'agriculture, de l'agroalimentaire et de la for\u00eat", "programmeRef": "423580", "autorisationEngagement": "10000000", "creditPaiement": "10000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00e9curit\u00e9 et qualit\u00e9 sanitaires de l'alimentation", "programmeRef": "423582", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'agriculture", "programmeRef": "423584", "autorisationEngagement": "-10000000", "creditPaiement": "-10000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "All\u00e8gements du co\u00fbt du travail en agriculture (TODE-AG)", "programmeRef": "423586", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Pour pr&#x00E9;server la comp&#x00E9;titivit&#x00E9; et l'autonomie du secteur agricole, la suppression de toute substance active ne devrait se faire qu'&#x00E0; la condition qu'il existe une solution alternative, techniquement efficace et &#x00E9;conomiquement accessible pour les agriculteurs et &#x00E9;leveurs. La transition agro-&#x00E9;cologique repose sur l'innovation et la recherche, indispensables pour d&#x00E9;velopper des substituts et &#x00E9;viter de laisser une fili&#x00E8;re sans solution technique viable. Cet amendement propose d'allouer 10 millions d&#x2019;euros suppl&#x00E9;mentaires &#x00E0; l&#x2019;action 23 &#x00AB; Appui au renouvellement et &#x00E0; la modernisation des exploitations agricoles &#x00BB; dans le programme 149 &#x00AB; Comp&#x00E9;titivit&#x00E9; et durabilit&#x00E9; de l&#x2019;agriculture, de l&#x2019;agroalimentaire et de la for&#x00EA;t &#x00BB;, de r&#x00E9;duire de 10 millions d&#x2019;euros les cr&#x00E9;dits affect&#x00E9;s &#x00E0; l&#x2019;action 01 &#x00AB; Moyens de l&#x2019;administration centrale &#x00BB; dans le programme 215 &#x00AB; Conduite et pilotage des politiques de l&#x2019;agriculture &#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-10-28T21:38:15+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P2D1N000932?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1.23", "divisionCodeLoi": {"@xmlns:xsi": "http