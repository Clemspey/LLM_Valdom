programmeRef": "526482", "autorisationEngagement": "-50000000", "creditPaiement": "-50000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Renforcement des fonds propres de l'Agence fran\u00e7aise de d\u00e9veloppement", "programmeRef": "526484", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Solidarit\u00e9 \u00e0 l'\u00e9gard des pays en d\u00e9veloppement", "programmeRef": "526486", "autorisationEngagement": "50000000", "creditPaiement": "50000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Restitution des \u00ab biens mal acquis \u00bb", "programmeRef": "526488", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds de solidarit\u00e9 pour le d\u00e9veloppement", "programmeRef": "526490", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le pr&#x00E9;sent amendement propose de renforcer les cr&#x00E9;dits affect&#x00E9;s &#x00E0; la relance de la francophonie qui fait partie des priorit&#x00E9;s du pr&#x00E9;sident de la R&#x00E9;publique. En tant que priorit&#x00E9; nationale, la France pourrait pertinemment investir des moyens &#x00E0; la hauteur de ses ambitions en mati&#x00E8;re de francophonie et de rayonnement de la langue fran&#x00E7;aise dans le monde. Le pr&#x00E9;sent amendement pr&#x00E9;voit d'abonder les cr&#x00E9;dits, en autorisations d'engagement et en cr&#x00E9;dits de paiement, d'un montant de 50 millions d'euros pour l'action 5 coop&#x00E9;ration multilat&#x00E9;rale du programme 209 solidarit&#x00E9; &#x00E0; l'&#x00E9;gard des pays en d&#x00E9;veloppement et, pour les besoins de la recevabilit&#x00E9; financi&#x00E8;re, de minorer du m&#x00EA;me montant les cr&#x00E9;dits, en autorisations d'engagement et en cr&#x00E9;dits de paiement, pour l'action 2 aide &#x00E9;conomique et financi&#x00E8;re bilat&#x00E9;rale du programme 110 aide &#x00E9;conomique et financi&#x00E8;re au d&#x00E9;veloppement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-30", "datePublication": "2024-11-15", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N000387?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "D", "divisionCo