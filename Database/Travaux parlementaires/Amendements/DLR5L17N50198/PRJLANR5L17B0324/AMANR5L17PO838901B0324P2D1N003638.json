{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0324P2D1N003638", "chronotag": "1739874668139", "legislature": "17", "identification": {"numeroLong": "II-3638", "numeroOrdreDepot": "3638", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0324P2D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "ctsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaazzzzi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794114", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": "PO59048"}, "cosignataires": {"acteurRef": "PA840665"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Mandon, rapporteur sp&#233;cial au nom de la commission des finances et Mme&#160;Allemand"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 61", "articleDesignationCourte": "APR\u00c8S ART. 61", "articleDesignation": "APR\u00c8S L'ARTICLE 61, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Article_61"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "RC", "libelleMission": "Relations avec les collectivit\u00e9s territoriales", "missionRef": "2890"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin de la dernie&#x0300;re phrase de l&#x2019;avant-dernier aline&#x0301;a de l&#x2019;article L.&nbsp;2334&#x2011;37 du code ge&#x0301;ne&#x0301;ral des collectivite&#x0301;s territoriales, le montant&nbsp;: &#x00AB;&nbsp;100 000&nbsp;euros&nbsp;&#x00BB; est remplac&#x00E9; par le montant&nbsp;: &#x00AB;&nbsp;50&nbsp;000&nbsp;euros&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Actuellement, la commission des e&#x0301;lus relative a&#x0300; la dotation d&#x2019;&#x00E9;quipement des territoires ruraux constitue&#x0301;e dans chaque de&#x0301;partement n&#x2019;est saisie pour avis que pour les subventions d&#x2019;un montant supe&#x0301;rieur a&#x0300; 100&nbsp;000&nbsp;euros.</p><p style=\"text-align: justify;\">Afin de renforcer la consultation des e&#x0301;lus sur les choix de l&#x2019;E&#x0301;tat et de ses repre&#x0301;sentants dans le de&#x0301;partement sur l&#x2019;attribution de la dotation d&#x2019;&#x00E9;quipement des territoires ruraux, cet amendement propose d&#x2019;abaisser ce seuil a&#x0300; 50&nbsp;000&nbsp;euros.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-09", "datePublication": "2024-11-15", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003638?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}