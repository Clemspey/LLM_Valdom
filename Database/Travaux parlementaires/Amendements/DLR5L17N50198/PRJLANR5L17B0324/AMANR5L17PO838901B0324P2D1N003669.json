Ref": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "\u00c9TAT B", "articleDesignationCourte": "ART. 42", "articleDesignation": "ARTICLE 42", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 42", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Etat_B"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "DB", "libelleMission": "\u00c9conomie", "missionRef": "2864"}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "D\u00e9veloppement des entreprises et r\u00e9gulations", "programmeRef": "531442", "autorisationEngagement": "1000000", "creditPaiement": "1000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Plan France Tr\u00e8s haut d\u00e9bit", "programmeRef": "531444", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Statistiques et \u00e9tudes \u00e9conomiques", "programmeRef": "531446", "autorisationEngagement": "-1000000", "creditPaiement": "-1000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Strat\u00e9gies \u00e9conomiques", "programmeRef": "531448", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Financement des op\u00e9rations patrimoniales en 2025 sur le compte d'affectation sp\u00e9ciale \u00ab Participations financi\u00e8res de l'\u00c9tat \u00bb", "programmeRef": "531450", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Conform&#x00E9;ment &#x00E0; la recommandation du rapport pour Soutenir l'investissement dans les start-ups, PME innovantes et PME de croissance, le pr&#x00E9;sent amendement vise &#x00E0; augmenter les cr&#x00E9;dits du programme 134 et plus particuli&#x00E8;rement les moyens allou&#x00E9;s au programme French Tech Tremplin.</p><p style=\"text-align: justify;\">Lanc&#x00E9; en 2019 &#x00E0; l&#x2019;initiative de la Mission French Tech, le programme French Tech Tremplin permet &#x00E0; des personnes &#x00E9;loign&#x00E9;es de l&#x2019;entrepreneuriat de cr&#x00E9;er leur start-up gr&#x00E2;ce &#x00E0; l&#x2019;accompagnement de structures comme Diversidays ou les D&#x00E9;termin&#x00E9;s. Au total pr&#x00E8;s de 500 personnes ont &#x00E9;t&#x00E9; accompagn&#x00E9;es lors de la premi&#x00E8;re phase du programme. L'objectif de cet amendement d'appel est de mettre en avant cette initiative et d'appeler &#x00E0; son d&#x00E9;veloppement.</p><p style=\"text-align: justify;\">Il ne s&#x2019;agit pas de p&#x00E9;naliser le programme 305 mais uniquement de respecter les conditions de recevabilit&#x00E9; financi&#x00E8;re. Il conviendra que le Gouvernement l&#x00E8;ve le gage en cas d&#x2019;adoption de l&#x2019;amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-11", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003669?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}