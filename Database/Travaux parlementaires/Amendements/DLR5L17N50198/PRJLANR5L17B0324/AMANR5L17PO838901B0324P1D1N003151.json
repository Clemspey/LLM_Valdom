{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0324P1D1N003151", "chronotag": "1739874609385", "legislature": "17", "identification": {"numeroLong": "I-3151", "numeroOrdreDepot": "3151", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0324P1D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabwzzw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841931", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267763", "PA840817", "PA721110"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Mazaury,  M.&#160;Bataille, M.&#160;Bruneau et Mme&#160;Sanquer"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "APR\u00c8S ART. 10", "articleDesignation": "APR\u00c8S L'ARTICLE 10, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324A.asp#D_Article_10"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;L&#x2019;article&nbsp;278 du code g&#x00E9;n&#x00E9;ral des imp&#x00F4;ts est compl&#x00E9;t&#x00E9; par trois alin&#x00E9;as ainsi r&#x00E9;dig&#x00E9;s&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le taux relev&#x00E9; de la taxe sur la valeur ajout&#x00E9;e est fix&#x00E9; &#x00E0; 21&nbsp;% en ce qui concerne&nbsp;:</p><p style=\"text-align: justify;\"><i>a)</i> La haute couture&nbsp;;</p><p style=\"text-align: justify;\"><i>b)</i> La maroquinerie et les v&#x00EA;tements de luxe.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;Le I, s&#x2019;applique aux op&#x00E9;rations pour lesquelles la taxe sur la valeur ajout&#x00E9;e est exigible &#x00E0; compter du 1<sup>er</sup> janvier 2025.</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;Le I s&#x2019;applique aux op&#x00E9;rations dont le fait g&#x00E9;n&#x00E9;rateur intervient &#x00E0; compter du 1<sup>er</sup> janvier 2025. Toutefois, ils ne s&#x2019;appliquent pas aux encaissements pour lesquels la taxe sur la valeur ajout&#x00E9;e est exigible avant cette date.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; augmenter le taux de TVA d'un point pour les produits de haute couture ainsi que pour la maroquinerie et les v&#x00EA;tements de luxe.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-19", "datePublication": "2024-10-26", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-26T17:42:30+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P1D1N003151?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28689", "article99": "false", "loiReference": {"codeLoi": "CGI", "divisionCodeLoi": "278"}, "discussionCommune": {"idDiscussion": "26478", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}