e": [{"libelle": "Inclusion sociale et protection des personnes", "programmeRef": "494908", "autorisationEngagement": "-91791830", "creditPaiement": "-91791830", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Handicap et d\u00e9pendance", "programmeRef": "494910", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "\u00c9galit\u00e9 entre les femmes et les hommes", "programmeRef": "494912", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-91791830", "soldeCP": "-91791830", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme annonc&#x00E9; par le Gouvernement lors du d&#x00E9;p&#x00F4;t du PLF 2025, dans le cadre de mesures visant &#x00E0; atteindre 5&nbsp;% du PIB, cet amendement minore les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement de la mission &#x00AB;&nbsp;Solidarit&#x00E9;, insertion et &#x00E9;galit&#x00E9; des chances&nbsp;&#x00BB;. Ainsi, le pr&#x00E9;sent amendement proc&#x00E8;de &#x00E0; une annulation de cr&#x00E9;dits sur le programme 304 &#x00AB;&nbsp;Inclusion sociale et protection des personnes&nbsp;&#x00BB; &#x00E0; hauteur de&nbsp;91,8 M&#x20AC; en AE et&nbsp;CP.</p><p style=\"text-align: justify;\">Cette annulation porte sur la prime d&#x2019;activit&#x00E9;, dont les param&#x00E8;tres vont &#x00EA;tre ajust&#x00E9;s &#x00E0; la marge pour prendre en compte la dynamique importante du SMIC ces derni&#x00E8;res ann&#x00E9;es.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-04", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N001321?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "A", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}