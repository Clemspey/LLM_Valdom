rps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Comp\u00e9titivit\u00e9 et durabilit\u00e9 de l'agriculture, de l'agroalimentaire et de la for\u00eat", "programmeRef": "423922", "autorisationEngagement": "12000000", "creditPaiement": "12000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00e9curit\u00e9 et qualit\u00e9 sanitaires de l'alimentation", "programmeRef": "423924", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'agriculture", "programmeRef": "423926", "autorisationEngagement": "-12000000", "creditPaiement": "-12000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "All\u00e8gements du co\u00fbt du travail en agriculture (TODE-AG)", "programmeRef": "423928", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; r&#x00E9;tablir l'&#x00E9;quilibre des financements entre la recherche agricole et l'enseignement technique. Les cr&#x00E9;dits allou&#x00E9;s &#x00E0; la recherche agricole subissent une diminution de 12 millions d'euros, tandis que ceux destin&#x00E9;s &#x00E0; l'enseignement technique augmentent de 41 millions d'euros. Il est essentiel de garantir un financement stable pour la recherche, qui joue un r&#x00F4;le important dans l'innovation et l'am&#x00E9;lioration des pratiques agricoles. En cons&#x00E9;quence, cet amendement propose de maintenir les cr&#x00E9;dits allou&#x00E9;s &#x00E0; la recherche agricole tout en limitant l'augmentation des cr&#x00E9;dits pour l'enseignement technique &#x00E0; 29 millions d'euros. Cette mesure permettra de soutenir la recherche tout en r&#x00E9;pondant aux besoins de formation dans le secteur agricole.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-10-28T23:36:44+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P2D1N000936?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1.01", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDep