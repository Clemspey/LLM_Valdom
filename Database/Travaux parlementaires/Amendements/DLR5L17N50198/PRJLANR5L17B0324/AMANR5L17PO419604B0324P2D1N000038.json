"missionVisee": {"codeEtat": "B", "codeMissionMinefi": "CB", "libelleMission": "Culture", "missionRef": "2856"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Patrimoines", "programmeRef": "386014", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cr\u00e9ation", "programmeRef": "386016", "autorisationEngagement": "1000000", "creditPaiement": "1000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Transmission des savoirs et d\u00e9mocratisation de la culture", "programmeRef": "386018", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Soutien aux politiques du minist\u00e8re de la culture", "programmeRef": "386020", "autorisationEngagement": "-1000000", "creditPaiement": "-1000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; abonder les cre&#x0301;dits de&#x0301;die&#x0301;s au soutien a&#x0300; la cre&#x0301;ation, a&#x0300; la production et a&#x0300; la diffusion du spectacle vivant de 1 million d&#x2019;euros.</p><p style=\"text-align: justify;\">&#x00C0; La Re&#x0301;union, des difficulte&#x0301;s particulie&#x0300;res sont rencontre&#x0301;es par les artistes, lie&#x0301;es a&#x0300; la situation insulaire. Par exemple, une tourne&#x0301;e en dehors de la Re&#x0301;union comporte un cou&#x0302;t nettement plus important, limitant la capacite&#x0301; d&#x2019;export de la musique re&#x0301;unionnaise.</p><p style=\"text-align: justify;\">&nbsp;Aussi, un accompagnement plus important est ne&#x0301;cessaire afin de favoriser notamment la diffusion des &#x0153;uvres re&#x0301;unionnaises, tel est l&#x2019;objet de cet amendement.</p><p style=\"text-align: justify;\">Afin de se conformer a&#x0300; la LOLF et aux re&#x0300;gles de recevabilite&#x0301; des amendements, cet amendement est ainsi re&#x0301;dige&#x0301;&nbsp;:</p><p style=\"text-align: justify;\">L&#x2019;action 1 (soutien a&#x0300; la cre&#x0301;ation, a&#x0300; la production et a&#x0300; la diffusion du spectacle vivant) du programme 131 (cre&#x0301;ation) est abonde&#x0301;e en AE et en CP de 1 million d&#x2019;euros.</p><p style=\"text-align: justify;\">Ces cre&#x0301;dits sont pre&#x0301;leve&#x0301;s hors titre 2 sur l&#x2019;action 7 du programme 224.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-22T22:30:20+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0324P2D1N000038?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}