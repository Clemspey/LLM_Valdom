xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Enseignement scolaire public du premier degr\u00e9", "programmeRef": "453888", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement scolaire public du second degr\u00e9", "programmeRef": "453890", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Vie de l'\u00e9l\u00e8ve", "programmeRef": "453892", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement priv\u00e9 du premier et du second degr\u00e9s", "programmeRef": "453894", "autorisationEngagement": "-10430924", "creditPaiement": "-10430924", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Soutien de la politique de l'\u00e9ducation nationale", "programmeRef": "453896", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement technique agricole", "programmeRef": "453898", "autorisationEngagement": "10430924", "creditPaiement": "10430924", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "12388", "libelle": "dont titre 2", "autorisationEngagement": "10430924", "creditPaiement": "10430924", "action": "modification"}}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; r&#x00E9;tablir les 196 postes d&#x2019;enseignants supprim&#x00E9;s dans l&#x2019;enseignement agricole public depuis 2019. Ces suppressions conduisent certains &#x00E9;tablissements &#x00E0; ne plus &#x00EA;tre en capacit&#x00E9; d&#x2019;assurer les missions d&#x00E9;volues.&nbsp;</p><p style=\"text-align: justify;\">Alors que les crises successives - sanitaire, &#x00E9;conomique -&nbsp;ont lourdement fragilis&#x00E9; les &#x00E9;tablissements, et que les ambitions fix&#x00E9;es par le Gouvernement pour la transition agro&#x00E9;cologique et le renouvellement des g&#x00E9;n&#x00E9;rations n&#x00E9;cessitent un effort de formation et d&#x2019;accompagnement sans pr&#x00E9;c&#x00E9;dent, il para&#x00EE;t indispensable d&#x2019;&#x00E9;toffer les effectifs dans l&#x2019;enseignement agricole public,&nbsp;a minima&nbsp;en r&#x00E9;tablissant les postes supprim&#x00E9;s progressivement chaque ann&#x00E9;e depuis 2019.</p><p style=\"text-align: justify;\">Afin d&#x2019;assurer sa recevabilit&#x00E9; financi&#x00E8;re, cet amendement de cr&#x00E9;dit&nbsp;:<br> <br> - Augmente de 10 430 924&nbsp;euros les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement du titre 2 de l&#x2019;action 01 &#x00AB;&nbsp;Mise en &#x0153;uvre de l&#x2019;enseignement dans les &#x00E9;tablissements publics&nbsp;&#x00BB; du programme 143 &#x00AB;&nbsp;Enseignement technique agricole&nbsp;&#x00BB;&nbsp;;<br> <br> - Diminue de 10 430 924&nbsp;euros les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement de l&#x2019;action 09 &#x00AB;&nbsp;Fonctionnement des &#x00E9;tablissements&nbsp;&#x00BB; du programme 139 &#x00AB;&nbsp;Enseignement priv&#x00E9; du premier et du second degr&#x00E9;&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">Les d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s tiennent &#x00E0; pr&#x00E9;ciser qu&#x2019;ils ne souhaitent pas r&#x00E9;duire les moyens attribu&#x00E9;s au programme 139, et appellent donc le Gouvernement &#x00E0; lever ce gage.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-25", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-30T23:57:18+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0324P2D1N000497?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}