sa recevabilit\u00e9", "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "France T\u00e9l\u00e9visions", "programmeRef": "481484", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "ARTE France", "programmeRef": "481486", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Radio France", "programmeRef": "481488", "autorisationEngagement": "-23000000", "creditPaiement": "-23000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "France M\u00e9dias Monde", "programmeRef": "481490", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Institut national de l'audiovisuel", "programmeRef": "481492", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "TV5 Monde", "programmeRef": "481494", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Programme de transformation", "programmeRef": "481496", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-23000000", "soldeCP": "-23000000", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement pr&#x00E9;voit de minorer les cr&#x00E9;dits, en autorisations d&#x2019;engagement et en cr&#x00E9;dits de paiement, d&#x2019;un montant de&nbsp;23 000 000&nbsp;d&#x2019;euros pour l&#x2019;action n&#x00B0;&nbsp;1 &#x00AB;&nbsp;Radio France&nbsp;&#x00BB; du programme n&#x00B0;&nbsp;374 &#x00AB;&nbsp;Radio France&nbsp;&#x00BB;.&nbsp;Ce montant repr&#x00E9;sentant le d&#x00E9;passement des recettes publicitaires&nbsp;de Radio France estim&#x00E9;es par les COM&nbsp;pour&nbsp;l&#x2019;ann&#x00E9;e 2024.</p><p style=\"text-align: justify;\">Si les COM 2024&#x2011;2028 pr&#x00E9;cisent que&nbsp;les recettes publicitaires de Radio France sont plafonn&#x00E9;es &#x00E0; 42 millions d&#x2019;euros par an, comprenant les recettes des publicit&#x00E9;s commerciales et les parrainages et &#x00AB;&nbsp;qu&#x2019;en&nbsp;cas de non-respect par la soci&#x00E9;t&#x00E9; de ce plafond, le Gouvernement en tire les cons&#x00E9;quences&nbsp;&#x00BB;,&nbsp;Radio France&nbsp;continue chaque ann&#x00E9;e d&#x2019;effectuer un d&#x00E9;passement de ce plafonnement avec un total de ressources publicitaires estim&#x00E9;e pour la fin de l&#x2019;ann&#x00E9;e 2024 &#x00E0; plus de 65 millions d&#x2019;euros sans aucun contr&#x00F4;le ou sanctions de la part de l&#x2019;Arcom ou du Minist&#x00E8;re.&nbsp;</p><p style=\"text-align: justify;\">Il est&nbsp;indispensable que Radio France respecte ce plafonnement publicitaire afin de pallier aux asym&#x00E9;tries publicitaires qui existent entre Radio France et les acteurs priv&#x00E9;s, dans un contexte de financement de la fili&#x00E8;re d&#x00E9;j&#x00E0; tendu et la concurrence de plus en plus croissante des plateformes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-12", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003701?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}