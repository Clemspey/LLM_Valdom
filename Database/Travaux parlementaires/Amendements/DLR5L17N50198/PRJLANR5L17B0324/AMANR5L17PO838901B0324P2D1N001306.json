e": [{"libelle": "Structures et dispositifs de s\u00e9curit\u00e9 routi\u00e8re", "programmeRef": "494542", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Contr\u00f4le et modernisation de la politique de la circulation et du stationnement routiers", "programmeRef": "494544", "autorisationEngagement": "-19335", "creditPaiement": "-19335", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Contribution \u00e0 l'\u00e9quipement des collectivit\u00e9s territoriales pour l'am\u00e9lioration des transports en commun, de la s\u00e9curit\u00e9 et de la circulation routi\u00e8res", "programmeRef": "494546", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "D\u00e9sendettement de l'\u00c9tat", "programmeRef": "494548", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-19335", "soldeCP": "-19335", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme annonc&#x00E9; par le Gouvernement lors du d&#x00E9;p&#x00F4;t du PLF 2025, dans le cadre de mesures visant &#x00E0; atteindre 5&nbsp;% du PIB, cet amendement minore les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement de la mission &#x00AB;&nbsp;Contr&#x00F4;le de la circulation et du stationnement routiers&nbsp;&#x00BB;. Ainsi, le pr&#x00E9;sent amendement proc&#x00E8;de &#x00E0; une annulation de cr&#x00E9;dits sur le programme 753 &#x00AB;&nbsp;Contr&#x00F4;le et modernisation de la politique de la circulation et du stationnement routiers&nbsp;&#x00BB; &#x00E0; hauteur de&nbsp;19&nbsp;k&#x20AC; en AE et&nbsp;CP.<br>Il en r&#x00E9;sulte un total d&#x2019;annulations de&nbsp;19&nbsp;k&#x20AC; en AE et en CP sur le p&#x00E9;rim&#x00E8;tre de la mission, en subvention pour charges de service public, li&#x00E9;es aux mesures transversales prises en mati&#x00E8;re de masse salariale (indemnit&#x00E9;s journali&#x00E8;res, ajout de deux jours de carence).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-04", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N001306?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "A", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-in