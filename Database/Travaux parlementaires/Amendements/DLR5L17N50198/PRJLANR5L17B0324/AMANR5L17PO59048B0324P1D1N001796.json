hapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324A.asp#D_Article_liminaire"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la quatri&#x00E8;me ligne de la troisi&#x00E8;me colonne du tableau de l&#x2019;alin&#x00E9;a&nbsp;2, substituer au nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-5,7&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-5,5&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la septi&#x00E8;me ligne de la m&#x00EA;me colonne du m&#x00EA;me tableau, substituer au nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-6,1&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-5,9&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Le pr&#x00E9;sent amendement a pour objet d&#x2019;inviter le Gouvernement &#x00E0; prendre toutes les mesures possibles pour agir sur la d&#x00E9;pense publique en 2024, comme l&#x2019;y invite le Haut Conseil aux Finances Publiques.</p><p>En effet, si l&#x2019;ensemble des mesures de r&#x00E9;gulation de fin d&#x2019;ann&#x00E9;e sont prises &#x00E0; l&#x2019;occasion du Projet de loi de fin de gestion, le d&#x00E9;ficit effectif devrait &#x00EA;tre inf&#x00E9;rieur &#x00E0; 6% du PIB.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-13", "datePublication": "2024-11-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-11-29T14:34:05+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P1D1N001796?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "25466", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}