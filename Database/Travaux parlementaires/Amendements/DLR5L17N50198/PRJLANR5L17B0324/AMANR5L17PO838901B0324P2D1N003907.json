ramme": [{"libelle": "Administration territoriale de l'\u00c9tat", "programmeRef": "534240", "autorisationEngagement": "-3101109", "creditPaiement": "-3101109", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "14938", "libelle": "dont titre 2", "autorisationEngagement": "-3101109", "creditPaiement": "-3101109", "action": "modification"}}}, {"libelle": "Vie politique", "programmeRef": "534242", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'int\u00e9rieur", "programmeRef": "534244", "autorisationEngagement": "-1139227", "creditPaiement": "-1139227", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "14940", "libelle": "dont titre 2", "autorisationEngagement": "-1139227", "creditPaiement": "-1139227", "action": "modification"}}}]}, "soldeAE": "-4240336", "soldeCP": "-4240336", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; annuler -3 101 109&nbsp;euros en AE et en CP sur le titre 2 du programme 354 &#x00AB;&nbsp;Administration territoriale de l&#x2019;&#x00C9;tat&nbsp;&#x00BB; et -1 139 227&nbsp;euros en AE et en CP sur le titre 2 du programme 216 &#x00AB;&nbsp;Conduite et pilotage des politiques de l&#x2019;int&#x00E9;rieur&nbsp;&#x00BB; pour tirer les cons&#x00E9;quences de l&#x2019;actualisation du calendrier de d&#x00E9;ploiement de la protection sociale compl&#x00E9;mentaire en sant&#x00E9; au sein du minist&#x00E8;re de l&#x2019;Int&#x00E9;rieur.</p><p style=\"text-align: justify;\">Les cr&#x00E9;dits ainsi annul&#x00E9;s viendront abonder le programme 176 &#x00AB;&nbsp;Police nationale&nbsp;&#x00BB; afin de contribuer au financement de d&#x00E9;penses permettant une pr&#x00E9;sence renforc&#x00E9;e sur la voie publique et une reconnaissance des personnels de la police nationale (r&#x00E9;serve op&#x00E9;rationnelle, indemnisation des heures suppl&#x00E9;mentaires, renforcement de l&#x2019;enveloppe de la prime de r&#x00E9;sultats exceptionnels), tout en garantissant la soutenabilit&#x00E9; des d&#x00E9;penses de personnel de ce m&#x00EA;me programme.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-12", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003907?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "a