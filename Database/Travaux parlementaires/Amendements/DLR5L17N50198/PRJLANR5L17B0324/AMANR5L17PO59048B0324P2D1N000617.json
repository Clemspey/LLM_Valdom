{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59048B0324P2D1N000617", "chronotag": "1739874564557", "legislature": "17", "identification": {"numeroLong": "II-CF617", "numeroOrdreDepot": "617", "prefixeOrganeExamen": "CION_FIN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59048B0324P2D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "ayoaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA942", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;de&#160;Courson"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 60", "articleDesignationCourte": "APR\u00c8S ART. 60", "articleDesignation": "APR\u00c8S L'ARTICLE 60, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Article_60"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "EB", "libelleMission": "Engagements financiers de l'\u00c9tat", "missionRef": "2866"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; compter du 1<sup>er</sup> janvier 2025, la composition du comit&#x00E9; strat&#x00E9;gique de l&#x2019;Agence France Tr&#x00E9;sor comprend notamment un d&#x00E9;put&#x00E9; et un s&#x00E9;nateur d&#x00E9;sign&#x00E9;s respectivement par le pr&#x00E9;sident de l&#x2019;Assembl&#x00E9;e nationale et par le pr&#x00E9;sident du S&#x00E9;nat sur proposition de leur commission charg&#x00E9;e des finances.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le Parlement ne dispose d&#x2019;aucun contr&#x00F4;le sur les activit&#x00E9;s de l&#x2019;Agence France Tr&#x00E9;sor. Il est propos&#x00E9; qu&#x2019;un d&#x00E9;put&#x00E9; et un s&#x00E9;nateur si&#x00E8;gent au comit&#x00E9; strat&#x00E9;gique afin de rendre compte de la bonne organisation de l&#x2019;Agence et des choix de celle-ci quant &#x00E0; la strat&#x00E9;gie d&#x2019;&#x00E9;mission des obligations souveraines fran&#x00E7;aises.</p><p style=\"text-align: justify;\">Cet amendement avait &#x00E9;t&#x00E9; d&#x00E9;pos&#x00E9; par le groupe LIOT lors de l&#x2019;examen du projet de loi de finances pour 2024, et avait &#x00E9;t&#x00E9; adopt&#x00E9; par la commission des finances.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-23", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-31T00:14:32+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P2D1N000617?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "yy", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}