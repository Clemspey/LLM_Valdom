ionTexteVise": "/17/textes/0324C.asp#D_Etat_B"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "JA", "libelleMission": "Justice", "missionRef": "2876"}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Justice judiciaire", "programmeRef": "518674", "autorisationEngagement": "10000000", "creditPaiement": "10000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Administration p\u00e9nitentiaire", "programmeRef": "518676", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Protection judiciaire de la jeunesse", "programmeRef": "518678", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Acc\u00e8s au droit et \u00e0 la justice", "programmeRef": "518680", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage de la politique de la justice", "programmeRef": "518682", "autorisationEngagement": "-10000000", "creditPaiement": "-10000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conseil sup\u00e9rieur de la magistrature", "programmeRef": "518684", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; mettre en place un m&#x00E9;canisme de r&#x00E9;gulation carc&#x00E9;rale contraignant afin de lutter contre la surpopulation carc&#x00E9;rale et d&#x2019;inciter l&#x2019;&#x00C9;tat &#x00E0; d&#x00E9;velopper une politique de r&#x00E9;insertion plus ambitieuse.<br>&nbsp;<br>L&#x2019;objectif est de financer, dans tous les territoires, des commissions qui r&#x00E9;uniraient l&#x2019;ensemble des parties prenantes dans l&#x2019;objectif de pr&#x00E9;voir des peines alternatives d&#x00E8;s qu&#x2019;un &#x00E9;tablissement p&#x00E9;nitentiaire d&#x00E9;passe un taux d&#x2019;occupation de 100&nbsp;%.<br>&nbsp;<br>Afin d&#x2019;assurer la recevabilit&#x00E9; financi&#x00E8;re de cet amendement, il est proc&#x00E9;d&#x00E9; aux mouvements de cr&#x00E9;dits suivants&nbsp;:</p><ul><li style=\"text-align: justify;\">Une hausse de 10M&#x20AC; en AE et CP sur l&#x2019;action 02 du programme 166 Justice Judiciaire.</li><li style=\"text-align: justify;\">Une baisse de 10M&#x20AC; en AE et CP sur l&#x2019;action 04 du programme 310. Il est demand&#x00E9; au Gouvernement de lever le gage.</li></ul>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-07", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verb