t": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Administration p\u00e9nitentiaire", "programmeRef": "419698", "autorisationEngagement": "-55000", "creditPaiement": "-55000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Protection judiciaire de la jeunesse", "programmeRef": "419700", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Acc\u00e8s au droit et \u00e0 la justice", "programmeRef": "419702", "autorisationEngagement": "55000", "creditPaiement": "55000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage de la politique de la justice", "programmeRef": "419704", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conseil sup\u00e9rieur de la magistrature", "programmeRef": "419706", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; abonder le budget allou&#x00E9; au dispositif du t&#x00E9;l&#x00E9;phone grave danger &#x00E0; hauteur de 55 000&nbsp;euros, pour financer l&#x2019;attribution de 25 TGD suppl&#x00E9;mentaires au tribunal judiciaire de Pontoise.</p><p style=\"text-align: justify;\">La quasi-totalit&#x00E9; des TGD du tribunal sont aujourd&#x2019;hui attribu&#x00E9;s &#x00E0; des personnes victimes de violences conjugales&nbsp;: il appara&#x00EE;t donc n&#x00E9;cessaire de renforcer leur budget pour garantir que l&#x2019;ensemble des personnes qui en ont besoin puissent &#x00EA;tre prot&#x00E9;g&#x00E9;es.</p><p style=\"text-align: justify;\">En cons&#x00E9;quence, le pr&#x00E9;sent amendement proc&#x00E8;de au mouvement de cr&#x00E9;dits suivants&nbsp;: il abonde l&#x2019;action 3 Aide aux victimes du programme 101 Acc&#x00E8;s au droit et &#x00E0; la justice &#x00E0; hauteur de 55 000&nbsp;euros en AE et CP et il ponctionne, &#x00E0; hauteur du m&#x00EA;me montant en AE et CP, l&#x2019;action 1 Garde et contr&#x00F4;le des personnes plac&#x00E9;es sous main de justice du programme 107 Administration p&#x00E9;nitentiaire, dot&#x00E9; de 3,5 milliards d&#x2019;euros.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-10-31T11:05:37+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59048B0324P2D1N001446?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "arti