nt": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Assembl\u00e9e nationale", "programmeRef": "362652", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00e9nat", "programmeRef": "362654", "autorisationEngagement": "-7000000", "creditPaiement": "-7000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "La Cha\u00eene parlementaire", "programmeRef": "362656", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Indemnit\u00e9s des repr\u00e9sentants fran\u00e7ais au Parlement europ\u00e9en", "programmeRef": "362658", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conseil constitutionnel", "programmeRef": "362660", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Haute Cour", "programmeRef": "362662", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cour de justice de la R\u00e9publique", "programmeRef": "362664", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "D\u00e9sendettement de l'Etat", "programmeRef": "362666", "autorisationEngagement": "7000000", "creditPaiement": "7000000", "action": "cr\u00e9ation", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Si les Fran&#x00E7;ais doivent faire des efforts fiscaux compte tenu de la situation inqui&#x00E9;tante des finances de la France, l'Etat, et &#x00E0; fortiori le Parlement doit contribuer &#x00E0; cet effort.</p><p style=\"text-align: justify;\">Pourtant, le budget du S&#x00E9;nat pour 2025 pr&#x00E9;voit une hausse&nbsp;de 1,69 % par rapport au budget 2024 soit 6 millions d'euros.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement pr&#x00E9;voit donc la cr&#x00E9;ation&nbsp;d'un nouveau programme \"D&#x00E9;sendettement de l'Etat\" dont l'action unique intitul&#x00E9;e \"D&#x00E9;sendettement de l'Etat\" est abond&#x00E9;e &#x00E0; hauteur de 7 millions d'euros pr&#x00E9;lev&#x00E9;s sur l'action n&#x00B0;1 \"S&#x00E9;nat\" du programme 521 \"S&#x00E9;nat\".</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-14", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-11-06T21:24:06+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "con