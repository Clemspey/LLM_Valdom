utorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Jeunesse et vie associative", "programmeRef": "503158", "autorisationEngagement": "-728246375", "creditPaiement": "-728246375", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Jeux olympiques et paralympiques 2024", "programmeRef": "503160", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Exp\u00e9rimentation d\u2019un revenu universel d\u2019existence", "programmeRef": "503162", "autorisationEngagement": "728246375", "creditPaiement": "728246375", "action": "cr\u00e9ation", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le Service National Universel est un g&#x00E2;chis de moyens qui ne remplit pas son r&#x00F4;le. L&#x2019;amour de la R&#x00E9;publique ne se transmet pas dans des casernes mais par la consid&#x00E9;ration et l&#x2019;&#x00E9;mancipation. Cet amendement se propose donc d'utiliser les fonds du SNU pour exp&#x00E9;rimenter un Revenu Universel d&#x2019;Existence pour les 18-25 ans afin de les &#x00E9;manciper et les prot&#x00E9;ger dans la p&#x00E9;riode de leur vie o&#x00F9; la pr&#x00E9;carit&#x00E9; les menace le plus.</p><p style=\"text-align: justify;\"><br>L'auteur&nbsp;de cet amendement propose de transf&#x00E9;rer les cr&#x00E9;dits des actions &#x00AB; D&#x00E9;veloppement du service civique &#x00BB; et &#x00AB; Service National Universel &#x00BB; vers un programme nouveau d&#x00E9;di&#x00E9; &#x00E0; exp&#x00E9;rimenter la mise en place d&#x2019;un Revenu Universel d&#x2019;Existence.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-05", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N001798?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "C", "divisionCodeLoi": "14"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}