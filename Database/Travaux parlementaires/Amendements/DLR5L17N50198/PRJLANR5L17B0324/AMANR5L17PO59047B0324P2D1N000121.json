programmeRef": "486956", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Renforcement des fonds propres de l'Agence fran\u00e7aise de d\u00e9veloppement", "programmeRef": "486958", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Solidarit\u00e9 \u00e0 l'\u00e9gard des pays en d\u00e9veloppement", "programmeRef": "486960", "autorisationEngagement": "-36572373", "creditPaiement": "-36572373", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Restitution des \u00ab biens mal acquis \u00bb", "programmeRef": "486962", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Fonds de solidarit\u00e9 pour le d\u00e9veloppement", "programmeRef": "486964", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-36572373", "soldeCP": "-36572373", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement de Repli</p><p style=\"text-align: justify;\">Les cr&#x00E9;dits de l&#x2019;action 02 &#x00AB;&nbsp;Coop&#x00E9;ration bilat&#x00E9;rale&nbsp;&#x00BB; du programme 209 &#x00AB;&nbsp;Solidarit&#x00E9; &#x00E0; l&#x2019;&#x00E9;gard des pays en d&#x00E9;veloppement&nbsp;&#x00BB; sont minor&#x00E9;s de 36 572 373&nbsp;euros en AE et en CP.</p><p style=\"text-align: justify;\">Avec une dette qui a d&#x00E9;pass&#x00E9; le montant record de 3 200 milliards d&#x2019;euros, la situation budg&#x00E9;taire de la France est plus qu&#x2019;alarmante. L&#x2019;ensemble des d&#x00E9;penses non indispensables ou dont l&#x2019;int&#x00E9;r&#x00EA;t est limit&#x00E9; au regard de leur affectation doivent &#x00EA;tre r&#x00E9;duites.</p><p style=\"text-align: justify;\">En mati&#x00E8;re d&#x2019;aide bilat&#x00E9;rale, l&#x2019;activit&#x00E9; &#x00AB;&nbsp;partenariat en mati&#x00E8;re d&#x2019;objectifs du d&#x00E9;veloppement durable&nbsp;&#x00BB; qui se voit affect&#x00E9;e 36,5 millions d&#x2019;euros en AE et en CP n&#x2019;est pas la priorit&#x00E9; de l&#x2019;action diplomatique bilat&#x00E9;rale fran&#x00E7;aise.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-01", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-11-05T18:56:06+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59047B0324P2D1N000121?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "d