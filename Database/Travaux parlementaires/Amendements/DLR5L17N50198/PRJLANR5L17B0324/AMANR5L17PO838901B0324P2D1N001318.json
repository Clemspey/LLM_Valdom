programmeRef": "521404", "autorisationEngagement": "-31626428", "creditPaiement": "-31626428", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "14224", "libelle": "dont titre 2", "autorisationEngagement": "-31558341", "creditPaiement": "-31558341", "action": "modification"}}}, {"libelle": "Gendarmerie nationale", "programmeRef": "521406", "autorisationEngagement": "-20384389", "creditPaiement": "-20384389", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "14226", "libelle": "dont titre 2", "autorisationEngagement": "-20384389", "creditPaiement": "-20384389", "action": "modification"}}}, {"libelle": "S\u00e9curit\u00e9 et \u00e9ducation routi\u00e8res", "programmeRef": "521408", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "S\u00e9curit\u00e9 civile", "programmeRef": "521410", "autorisationEngagement": "-692889", "creditPaiement": "-692889", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "14228", "libelle": "dont titre 2", "autorisationEngagement": "-688598", "creditPaiement": "-688598", "action": "modification"}}}]}, "soldeAE": "-52703706", "soldeCP": "-52703706", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme annonc&#x00E9; par le Gouvernement lors du d&#x00E9;p&#x00F4;t du PLF 2025, dans le cadre de mesures visant &#x00E0; atteindre 5&nbsp;% du PIB, cet amendement minore les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement de la mission &#x00AB;&nbsp;S&#x00E9;curit&#x00E9;s&nbsp;&#x00BB;. Ainsi, le pr&#x00E9;sent amendement proc&#x00E8;de &#x00E0; une annulation de cr&#x00E9;dits sur le programme 176 &#x00AB;&nbsp;Police nationale&nbsp;&#x00BB; &#x00E0; hauteur de&nbsp;31,62 M&#x20AC; en AE et&nbsp;CP dont 31,56&nbsp;M&#x20AC; en AE et CP sur le titre 2, le programme 152 &#x00AB;&nbsp;Gendarmerie nationale&nbsp;&#x00BB; sur les cr&#x00E9;dits de titre 2 &#x00E0; hauteur de&nbsp;20,4&nbsp;M&#x20AC; en AE et&nbsp;CP, le programme 161 &#x00AB;&nbsp;S&#x00E9;curit&#x00E9; civile&nbsp;&#x00BB; &#x00E0; hauteur de&nbsp;693&nbsp;k&#x20AC; &#x20AC; en AE et&nbsp;CP dont 689&nbsp;k&#x20AC; en AE et CP sur le titre 2.<br>Il en r&#x00E9;sulte un total d&#x2019;annulations de&nbsp;52,7 M&#x20AC; en AE et&nbsp;CP sur le p&#x00E9;rim&#x00E8;tre de la mission.<br>L&#x2019;int&#x00E9;gralit&#x00E9; de ces 52,7 M&#x20AC; en AE et CP d&#x2019;annulations de cr&#x00E9;dits est li&#x00E9;e aux mesures transversales prises en mati&#x00E8;re de masse salariale (indemnit&#x00E9;s journali&#x00E8;res, ajout de deux jours de carence) au sein de l'Etat et de ses op&#x00E9;rateurs.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-04", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N001318?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLS