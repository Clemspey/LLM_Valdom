{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59047B0324P2D1N000047", "chronotag": "1739874529671", "legislature": "17", "identification": {"numeroLong": "II-AE47", "numeroOrdreDepot": "47", "prefixeOrganeExamen": "CION_AFETR", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59047B0324P2D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "akiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaad", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793314", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "Mme&#160;Alexandra&#160;Masson et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "\u00c9TAT B", "articleDesignationCourte": "ART. 42", "articleDesignation": "ARTICLE 42", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 42", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Etat_B"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "IA", "libelleMission": "Immigration, asile et int\u00e9gration", "missionRef": "2872"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Immigration et asile", "programmeRef": "390610", "autorisationEngagement": "-80000000", "creditPaiement": "-80000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Int\u00e9gration et acc\u00e8s \u00e0 la nationalit\u00e9 fran\u00e7aise", "programmeRef": "390612", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-80000000", "soldeCP": "-80000000", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;&#x00E9;tat critique des finances publiques requiert des &#x00E9;conomies g&#x00E9;n&#x00E9;ralis&#x00E9;es, auxquelles tous doivent participer, y compris les demandeurs d&#x2019;asile.</p><p style=\"text-align: justify;\">L&#x2019;amendement vise en cons&#x00E9;quence &#x00E0; minorer de 5&nbsp;% le budget allou&#x00E9; &#x00E0;&nbsp;l&#x2019;allocation pour demandeurs d&#x2019;asile (ADA) et de 10&nbsp;%&nbsp;les cr&#x00E9;dits d&#x2019;h&#x00E9;bergement, pour une &#x00E9;conomie totale&nbsp;de 80 000 000&nbsp;euros.&nbsp;</p><p style=\"text-align: justify;\">Ainsi, le pr&#x00E9;sent amendement pr&#x00E9;voit de minorer d&#x2019;un montant de 80 000 000&nbsp;euros, en autorisations d&#x2019;engagement et en cr&#x00E9;dits de paiement, l&#x2019;action n&#x00B0;&nbsp;02 &#x00AB;&nbsp;Garantie de l&#x2019;exercice du droit d&#x2019;asile&nbsp;&#x00BB; du programme n&#x00B0;&nbsp;303 &#x00AB;&nbsp;Immigration et asile&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-18", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-30T10:40:46+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59047B0324P2D1N000047?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}