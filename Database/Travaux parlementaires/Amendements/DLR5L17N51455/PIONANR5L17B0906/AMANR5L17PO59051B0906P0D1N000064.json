cueil. Il &#x00E9;vite que les gens du voyage soient trop fortement p&#x00E9;nalis&#x00E9;s alors que des solutions l&#x00E9;gales leur sont refus&#x00E9;es.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-22", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0906P0D1N000064?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "dc"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}