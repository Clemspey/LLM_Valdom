{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0666P0D1N000029", "chronotag": "1742661808764", "legislature": "17", "identification": {"numeroLong": "AS29", "numeroOrdreDepot": "29", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0666P0D1", "texteLegislatifRef": "PIONANR5L17B0666", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841431", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Colin-Oesterl&#233;"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0666.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article L.&nbsp;4341&#x2011;1 du code de la sant&#x00E9; publique est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;Au cinqui&#x00E8;me alin&#x00E9;a, le mot&nbsp;: &#x00AB;&nbsp;sur&nbsp;&#x00BB; est remplac&#x00E9; par le mot&nbsp;: &#x00AB;&nbsp;sans&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;Le sixi&#x00E8;me alin&#x00E9;a est supprim&#x00E9;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;acc&#x00E8;s aux soins orthophoniques constitue un enjeu majeur de sant&#x00E9; publique, notamment dans le contexte actuel de tensions sur notre syst&#x00E8;me de sant&#x00E9;. La loi du 19 mai 2023 a permis une avanc&#x00E9;e significative en autorisant l&#x2019;acc&#x00E8;s direct aux orthophonistes exer&#x00E7;ant au sein de structures coordonn&#x00E9;es. Cependant, cette mesure, bien que positive, cr&#x00E9;e une in&#x00E9;galit&#x00E9; de traitement entre les professionnels et une complexit&#x00E9; pour les patients.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; &#x00E9;tendre l&#x2019;acc&#x00E8;s direct &#x00E0; l&#x2019;ensemble des orthophonistes, quel que soit leur mode d&#x2019;exercice. Cette mesure permettra d&#x2019;harmoniser les conditions d&#x2019;exercice, &#x00E9;vitant ainsi la cr&#x00E9;ation de niveaux d&#x2019;acc&#x00E8;s diff&#x00E9;rents selon les structures d&#x2019;exercice et offrira donc une meilleure lisibilit&#x00E9; du parcours de soins pour les patients, facilitant ainsi leur prise en charge.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-22", "datePublication": "2025-03-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0666P0D1N000029?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c sant\u00e9 publique", "divisionCodeLoi": "L4341-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}