{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0769P0D1N000019", "chronotag": "1739874711392", "legislature": "17", "identification": {"numeroLong": "AS19", "numeroOrdreDepot": "19", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0769P0D1", "texteLegislatifRef": "PIONANR5L17B0769", "triAmendement": "oaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaam", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719472", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793270", "PA642695", "PA720154", "PA719736", "PA720022", "PA719404", "PA719756", "PA720446", "PA795962", "PA720066", "PA841927", "PA722102"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jean-Ren&#233;&#160;Cazeneuve,  M.&#160;Rousset, M.&#160;Bothorel, Mme&#160;Dubr&#233;-Chirat, Mme&#160;Galliard-Minier, M.&#160;Lauzzana, M.&#160;Le&#160;Gac, Mme&#160;Le&#160;Nabour, Mme&#160;Liso, Mme&#160;Missoffe, Mme&#160;Rist, Mme&#160;Ronceret et Mme&#160;Vidal"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "APR\u00c8S ART. 4", "articleDesignation": "APR\u00c8S L'ARTICLE 4, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0769.asp#D_Article_4"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport sur les effets de l&#x2019;extension des obligations de revitalisation et de recherche de repreneur sur l&#x2019;investissement des entreprises et sur la dynamique de l&#x2019;emploi. Ce rapport analyse &#x00E9;galement les effets de la pr&#x00E9;sente loi sur l&#x2019;attractivit&#x00E9; &#x00E9;conomique des territoires concern&#x00E9;s.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;extension des obligations de revitalisation et de recherche de repreneur constitue une &#x00E9;volution significative du cadre l&#x00E9;gislatif actuel. Il est essentiel d&#x2019;&#x00E9;valuer pr&#x00E9;cis&#x00E9;ment les cons&#x00E9;quences potentielles de cette mesure sur l&#x2019;investissement des entreprises et sur leur capacit&#x00E9; &#x00E0; maintenir et cr&#x00E9;er des emplois en France. Un rapport d&#x00E9;taill&#x00E9; permettra d&#x2019;analyser ces impacts, en tenant compte des sp&#x00E9;cificit&#x00E9;s des diff&#x00E9;rents secteurs &#x00E9;conomiques et des territoires concern&#x00E9;s. Cette &#x00E9;valuation contribuera &#x00E0; ajuster, si n&#x00E9;cessaire, les dispositions l&#x00E9;gislatives afin de concilier efficacement la protection de l&#x2019;emploi et le dynamisme &#x00E9;conomique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-08", "datePublication": "2025-02-12", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-02-12T00:00:00+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0769P0D1N000019?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "zzz", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}