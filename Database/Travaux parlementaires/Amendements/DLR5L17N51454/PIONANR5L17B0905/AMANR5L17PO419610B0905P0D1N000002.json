{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0905P0D1N000002", "chronotag": "1742572857439", "legislature": "17", "identification": {"numeroLong": "CE2", "numeroOrdreDepot": "2", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0905P0D1", "texteLegislatifRef": "PIONANR5L17B0905", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaal", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793298", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841495", "PA842279", "PA642988", "PA793246", "PA793592", "PA794418", "PA794354", "PA841837", "PA794590", "PA719436", "PA842227", "PA841479"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Tivoli,  M.&#160;Amblard, Mme&#160;Bamana, M.&#160;de&#160;L&#233;pinau, M.&#160;Falcon, Mme&#160;Grangier, Mme&#160;Florence&#160;Goulet, Mme&#160;Laporte, M.&#160;Le&#160;Bourgeois, M.&#160;Loubet, M.&#160;Meizonnet, M.&#160;Rivi&#232;re et M.&#160;Weber"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0905.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer l&#x2019;alin&#x00E9;a&nbsp;5.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement pr&#x00E9;voit d&#x2019;abaisser le seuil de revenus exc&#x00E9;dant les conditions d&#x2019;attribution d&#x2019;un logement social. Cet article ainsi r&#x00E9;dig&#x00E9; est en partie fauss&#x00E9; puisqu&#x2019;il met en avant une pr&#x00E9;f&#x00E9;rence salariale plut&#x00F4;t qu&#x2019;une pr&#x00E9;f&#x00E9;rence nationale.</p><p style=\"text-align: justify;\">Pourtant, la priorit&#x00E9; nationale doit s&#x2019;appliquer dans l&#x2019;attribution au logement. Tandis que 2 millions de nos compatriotes attendent un logement alors que plus d&#x2019;un quart des logements sociaux sont attribu&#x00E9;s &#x00E0; des immigr&#x00E9;s, la solidarit&#x00E9; nationale doit profiter avant tout &#x00E0; nos compatriotes.&nbsp;</p><p style=\"text-align: justify;\">D&#x00E8;s lors, abaisser ce seuil pour nos compatriotes reviendrait &#x00E0; ne pas inciter les prestataires d&#x2019;un logement social &#x00E0; s&#x2019;ins&#x00E9;rer dans la soci&#x00E9;t&#x00E9; par la voie du travail.&nbsp;</p><p style=\"text-align: justify;\">Abaisser ce seuil &#x00E0; 120&nbsp;%, sans en exempter les fran&#x00E7;ais est ainsi une dispositif&nbsp;qui va &#x00E0; l&#x2019;encontre&nbsp;des int&#x00E9;r&#x00EA;ts de nos compatriotes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0905P0D1N000002?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}