{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0824P0D1N000010", "chronotag": "1739898026242", "legislature": "17", "identification": {"numeroLong": "AC10", "numeroOrdreDepot": "10", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0824P0D1", "texteLegislatifRef": "PIONANR5L17B0824", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaasf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794486", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Spillebout"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0824.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;4 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;qui rel&#x00E8;vent de l&#x2019;article L.&nbsp;218&#x2011;1 du code la propri&#x00E9;t&#x00E9; intellectuelle, et en lien avec l&#x2019;exploitation des contenus vis&#x00E9;s&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; clarifier l&#x2019;article&nbsp;1<sup>er</sup> en pr&#x00E9;cisant que les donn&#x00E9;es concern&#x00E9;es doivent porter sur les contenus vis&#x00E9;s et &#x00EA;tre transmises exclusivement aux &#x00E9;diteurs et agences de presse reconnus au titre de l&#x2019;article L.&nbsp;218&#x2011;1 du Code de la propri&#x00E9;t&#x00E9; intellectuelle.</p><p style=\"text-align: justify;\">L&#x2019;application de la loi a &#x00E9;t&#x00E9; entrav&#x00E9;e par un manque de clart&#x00E9; quant &#x00E0; la d&#x00E9;finition des publications de presse &#x00E9;ligibles. Cette impr&#x00E9;cision a conduit certains services de communication au public &#x00E0; h&#x00E9;siter, voire &#x00E0; refuser, de transmettre les donn&#x00E9;es requises aux &#x00E9;diteurs de presse, faute de certitude sur leur inclusion dans le champ d&#x2019;application de la loi n&#x00B0;&nbsp;2019&#x2011;775 du 24 juillet 2019 instituant un droit voisin au profit des agences et &#x00E9;diteurs de presse.</p><p style=\"text-align: justify;\">Afin de lever toute ambigu&#x00EF;t&#x00E9;, cet amendement pr&#x00E9;cise que la transmission des informations ne concerne que les &#x00E9;diteurs et agences de presse dont les publications rel&#x00E8;vent express&#x00E9;ment de l&#x2019;article L.&nbsp;218&#x2011;1 du Code de la propri&#x00E9;t&#x00E9; intellectuelle.</p><p style=\"text-align: justify;\">Tel est l&#x2019;objet du pr&#x00E9;sent amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-14", "datePublication": "2025-02-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-02-18T17:59:58+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0824P0D1N000010?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "75640", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}