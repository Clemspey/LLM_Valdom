{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0824P0D1N000012", "chronotag": "1739899286071", "legislature": "17", "identification": {"numeroLong": "AC12", "numeroOrdreDepot": "12", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0824P0D1", "texteLegislatifRef": "PIONANR5L17B0824", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841777", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA796118", "PA793158", "PA720822", "PA794198", "PA840159", "PA795644", "PA793362", "PA841749", "PA840869", "PA795552", "PA794670", "PA841199", "PA793656", "PA842255"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Sanvert,  Mme&#160;Auzanot, M.&#160;Beaurain, M.&#160;Bilde, M.&#160;Chudeau, M.&#160;Clavet, Mme&#160;Da&#160;Conceicao&#160;Carvalho, M.&#160;Gonzalez, Mme&#160;Joncour, Mme&#160;Joubert, M.&#160;Odoul, Mme&#160;Parmentier, M.&#160;Perez, Mme&#160;Pollet et Mme&#160;Sicard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0824.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le Gouvernement remet au Parlement, dans un d&#x00E9;lai d&#x2019;un an &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, un rapport sur l&#x2019;efficacit&#x00E9; et la port&#x00E9;e des sanctions pr&#x00E9;vues par l&#x2019;article&nbsp;2 en mati&#x00E8;re de non-respect des droits voisins des &#x00E9;diteurs et agences de presse.</p><p style=\"text-align: justify;\">Ce rapport &#x00E9;value notamment&nbsp;: la coh&#x00E9;rence des sanctions pr&#x00E9;vues par le code de la propri&#x00E9;t&#x00E9; intellectuelle et celles applicables au titre du droit de la concurrence&nbsp;; l&#x2019;impact des sanctions financi&#x00E8;res sur les comportements des plateformes num&#x00E9;riques en mati&#x00E8;re de respect des droits voisins&nbsp;; le cadre juridique de mise en &#x0153;uvre des sanctions et les &#x00E9;ventuelles failles pouvant &#x00EA;tre exploit&#x00E9;es par les entreprises concern&#x00E9;es&nbsp;; la comparaison avec les pratiques d&#x2019;autres &#x00C9;tats membres de l&#x2019;Union europ&#x00E9;enne en mati&#x00E8;re de r&#x00E9;gulation des droits voisins.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;2 pr&#x00E9;cise que les sanctions applicables aux infractions relatives aux droits voisins ne sont pas exclusives des sanctions pr&#x00E9;vues par le droit de la concurrence.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement demande un rapport permettant d&#x2019;&#x00E9;valuer l&#x2019;application effective de ces sanctions et leur impact sur les n&#x00E9;gociations des droits voisins.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-14", "datePublication": "2025-02-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-02-18T18:21:23+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0824P0D1N000012?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "Z", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}