{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0325P0D1N001823", "chronotag": "1739874592544", "legislature": "17", "identification": {"numeroLong": "1823", "numeroOrdreDepot": "1823", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaacv", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794038", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Neuder"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 18", "articleDesignationCourte": "ART. 18", "articleDesignation": "ARTICLE 18", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_18"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;9, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Lorsqu&#x2019;il existe une tension sur les ressources se traduisant, pour l&#x2019;une des cat&#x00E9;gories de professionnels mentionn&#x00E9;s &#x00E0; l&#x2019;article L.&nbsp;313&#x2011;23&#x2011;4, dans le co&#x00FB;t de leur mise &#x00E0; disposition par les entreprises de travail temporaire rapport&#x00E9; &#x00E0; l&#x2019;emploi des m&#x00EA;mes professionnels permanents, le montant des d&#x00E9;penses susceptibles d&#x2019;&#x00EA;tre engag&#x00E9;es par les &#x00E9;tablissements et les services mentionn&#x00E9;s &#x00E0; l&#x2019;article L.&nbsp;315&#x2011;1, relevant des 1&#x00B0;&nbsp;, 2&#x00B0;&nbsp;, 4&#x00B0;&nbsp;, 6&#x00B0;&nbsp;et 7&#x00B0;&nbsp;du I de l&#x2019;article L.&nbsp;312&#x2011;1, au titre de ces prestations&nbsp;&#x00BB;, </p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Lorsque la mise &#x00E0; disposition par des entreprises de travail temporaire des professionnels de l&#x2019;une des cat&#x00E9;gories mentionn&#x00E9;es &#x00E0; l&#x2019;article L.&nbsp;313&#x2011;23&#x2011;4 se traduit par un surco&#x00FB;t important, rapport&#x00E9; au co&#x00FB;t de l&#x2019;emploi des professionnels permanents de la m&#x00EA;me cat&#x00E9;gorie, le montant des d&#x00E9;penses susceptibles d&#x2019;&#x00EA;tre engag&#x00E9;es par les &#x00E9;tablissements et services relevant de l&#x2019;article L.&nbsp;315&#x2011;1 et des 1&#x00B0;&nbsp;, 2&#x00B0;&nbsp;, 4&#x00B0;&nbsp;, 6&#x00B0;&nbsp;et 7&#x00B0;&nbsp;du I de l&#x2019;article L.&nbsp;312&#x2011;1&nbsp;au titre de la prestation de l&#x2019;un de ces professionnels&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement r&#x00E9;dactionnel.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-25", "datePublication": "2024-11-05", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-05T19:43:08+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001823?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28622", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}