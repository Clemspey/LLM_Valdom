rlDivisionTexteVise": "/17/textes/0325.asp#D_Article_16"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "4", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;4, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;En l&#x2019;absence du document mentionn&#x00E9; au premier alin&#x00E9;a, le professionnel appel&#x00E9; &#x00E0; ex&#x00E9;cuter la prescription le signale au prescripteur afin que celui-ci le lui transmette. L&#x2019;absence du document n&#x2019;emp&#x00EA;che pas l&#x2019;ex&#x00E9;cution de la prescription, ni donne lieu &#x00E0; r&#x00E9;p&#x00E9;tition d&#x2019;indu pour le patient. L&#x2019;assurance maladie proc&#x00E8;de au rappel des r&#x00E8;gles de prescriptions aupr&#x00E8;s du professionnel concern&#x00E9;.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, au d&#x00E9;but de l&#x2019;alin&#x00E9;a&nbsp;5, substituer aux mots&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;En l&#x2019;absence du document mentionn&#x00E9; au premier alin&#x00E9;a, ou lorsque celui&#x2011;ci indique qu&#x2019;une prescription ne s&#x2019;inscrit pas dans les indications remboursables ou les recommandations,&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Lorsque le document indique que la prescription ne s&#x2019;inscrit pas dans les indications remboursables ou les recommandations, le prescripteur en informe le patient et&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de repli, issu de propositions formul&#x00E9;es par APF France handicap, vise &#x00E0; affirmer la responsabilit&#x00E9; de la prescription par le professionnel de sant&#x00E9; et &#x00E0; emp&#x00EA;cher que le patient soit p&#x00E9;nalis&#x00E9; en cas de d&#x00E9;faut de transmission du formulaire par le prescripteur.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-04", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-04T23:24:47+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001498?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28651", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "73314", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}