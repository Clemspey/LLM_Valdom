{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0325P0D1N001407", "chronotag": "1739874578757", "legislature": "17", "identification": {"numeroLong": "AS1407", "numeroOrdreDepot": "1407", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "giaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadj", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720066", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793270", "PA720154", "PA720022", "PA719404", "PA719756", "PA720446", "PA722102"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Rist,  M.&#160;Rousset, Mme&#160;Dubr&#233;-Chirat, M.&#160;Lauzzana, M.&#160;Le&#160;Gac, Mme&#160;Le&#160;Nabour, Mme&#160;Liso et Mme&#160;Vidal"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 17", "articleDesignationCourte": "ART. 17", "articleDesignation": "ARTICLE 17", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_17"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "7", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;7, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;5 <i>bis</i>&#x00B0;&nbsp;Les conditions dans lesquelles les entreprises de taxi sont soumises &#x00E0; un dispositif &#x00E9;lectronique de facturation int&#x00E9;gr&#x00E9; et de g&#x00E9;olocalisation certifi&#x00E9; par l&#x2019;assurance maladie&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme le rel&#x00E8;ve l&#x2019;&#x00E9;tude d&#x2019;impact annex&#x00E9;e au pr&#x00E9;sent PLFSS, les d&#x00E9;penses en mati&#x00E8;re de transports demeurent en croissance constante, avec notamment des d&#x00E9;penses moyennes par trajet ayant augment&#x00E9; de 61&nbsp;&#x20AC; en 2023 pour les taxis. <br>&nbsp;<br>Dans le cadre de la red&#x00E9;finition du p&#x00E9;rim&#x00E8;tre de la convention-cadre entre l&#x2019;Assurance maladie et les taxis &#x00E0; l&#x2019;article&nbsp;17 et dans l&#x2019;objectif d&#x2019;am&#x00E9;liorer l&#x2019;efficience des d&#x00E9;penses de transports de patients, le pr&#x00E9;sent amendement propose d&#x2019;&#x00E9;largir aux taxis les astreintes li&#x00E9;es &#x00E0; la r&#x00E8;gle de suivi g&#x00E9;ographique d&#x00E9;j&#x00E0; applicable aux autres acteurs du transport sanitaire. Cela aura pour effet de s&#x00E9;curiser la profession des taxis et de lutter plus efficacement contre la fraude.</p><p style=\"text-align: justify;\">Pour rappel, le pr&#x00E9;judice li&#x00E9; aux fraudes relatives au transport sanitaire &#x00E9;tait estim&#x00E9; &#x00E0; 34 millions d&#x2019;euros en 2023.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-10-24", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-24T18:04:02+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0325P0D1N001407?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}