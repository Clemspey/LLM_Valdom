{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0325P0D1N001449", "chronotag": "1739874602225", "legislature": "17", "identification": {"numeroLong": "1449", "numeroOrdreDepot": "1449", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "kaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafboi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793146", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841563", "PA793314", "PA793330", "PA793230", "PA796118", "PA793102", "PA793928", "PA841621", "PA793548", "PA840801", "PA841487", "PA794954", "PA793672", "PA841981", "PA840159", "PA841777", "PA841605", "PA720668", "PA794670"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dragon,  Mme&#160;Marais-Beuil, Mme&#160;Alexandra&#160;Masson, M.&#160;Bryan&#160;Masson, Mme&#160;Ranc, Mme&#160;Auzanot, M.&#160;Christian&#160;Girard, Mme&#160;Diaz, M.&#160;Evrard, Mme&#160;Colombier, M.&#160;Fouquart, M.&#160;Boulogne, M.&#160;Salmon, Mme&#160;Loir, Mme&#160;Rimbert, M.&#160;Clavet, M.&#160;Sanvert, M.&#160;Golliot, M.&#160;Blairy et Mme&#160;Parmentier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23", "articleDesignationCourte": "APR\u00c8S ART. 23", "articleDesignation": "APR\u00c8S L'ARTICLE 23, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_23"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Une personne condamn&#x00E9;e pour un acte de terrorisme mentionn&#x00E9; aux articles L.&nbsp;421&#x2011;1 et L.&nbsp;421&#x2011;2 du code p&#x00E9;nal ne peut pas b&#x00E9;n&#x00E9;ficier de l&#x2019;allocation de solidarit&#x00E9; aux personnes &#x00E2;g&#x00E9;es mentionn&#x00E9;e &#x00E0; l&#x2019;article L.&nbsp;815&#x2011;1 du code de la s&#x00E9;curit&#x00E9; sociale.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; exclure du versement l&#x2019;Allocation de Solidarit&#x00E9; aux Personnes &#x00C2;g&#x00E9;es</p><p>(ASPA) les personnes ayant commis un ou plusieurs actes de terrorisme.</p><p>Par leur engagement et leurs actes criminels ou d&#x00E9;lictueux, les terroristes choisissent de se mettre</p><p>&#x00E0; la marge de la soci&#x00E9;t&#x00E9;. Il n&#x2019;est pas normal qu&#x2019;ils puisent avoir droit &#x00E0; la g&#x00E9;n&#x00E9;rosit&#x00E9; de cette</p><p>m&#x00EA;me soci&#x00E9;t&#x00E9; qu&#x2019;ils combattent et terrorisent.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-10-31", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001449?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "Z1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}