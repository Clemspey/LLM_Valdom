endementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article L.&nbsp;131&#x2011;7 du code de la s&#x00E9;curit&#x00E9; sociale est compl&#x00E9;t&#x00E9; par un III ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;III.&nbsp;&#x2013;&nbsp;Toute mesure de r&#x00E9;duction ou d&#x2019;exon&#x00E9;ration de cotisations de s&#x00E9;curit&#x00E9; sociale, institu&#x00E9;e &#x00E0; compter du 1<sup>er</sup>&nbsp;janvier 2024, est compens&#x00E9;e par la suppression, dans la m&#x00EA;me proportion, d&#x2019;une mesure de r&#x00E9;duction ou d&#x2019;exon&#x00E9;ration de cotisations de s&#x00E9;curit&#x00E9; sociale existante.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise a&#x0300; limiter la mise en place de nouveaux dispositifs d&#x2019;exone&#x0301;rations de cotisations sociales en pre&#x0301;voyant que chaque nouveau dispositif fait l&#x2019;objet de la suppression d&#x2019;un dispositif existant pour un montant e&#x0301;quivalent.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-10-30", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-30T17:07:00+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001286?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28632", "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L131-7"}, "discussionCommune": {"idDiscussion": "27212", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "73408", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}