{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0325P0D1N000109", "chronotag": "1739874587510", "legislature": "17", "identification": {"numeroLong": "AS109", "numeroOrdreDepot": "109", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "eeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaii", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA1567", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642764", "PA842211", "PA795762", "PA793394", "PA606573", "PA840939", "PA841717", "PA841091"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Guedj,  M.&#160;Aviragnet, Mme&#160;Bellay, M.&#160;Califer, M.&#160;Delaporte, Mme&#160;Dombre&#160;Coste, Mme&#160;Godard, Mme&#160;Runel et M.&#160;Simion"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "ANNEXE", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 14", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_ANNEXE"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "10", "alineaDesignation": "Alin\u00e9a 10"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s la troisi&#x00E8;me phrase de l&#x2019;alin&#x00E9;a&nbsp;10, ins&#x00E9;rer la phrase suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Elle tient compte de la compensation int&#x00E9;grale &#x00E0; la S&#x00E9;curit&#x00E9; sociale en 2025 de la perte de recettes li&#x00E9;e &#x00E0; l&#x2019;exon&#x00E9;ration de cotisations sociales salariales sur les heures suppl&#x00E9;mentaires.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement d'appel des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; compenser &#x00E0; l'euro pr&#x00E8;s&nbsp;l'exon&#x00E9;ration sur les heures suppl&#x00E9;mentaires sur la part salariale &#x00E0; la S&#x00E9;curit&#x00E9; sociale.</p><p>Son&nbsp;co&#x00FB;t est selon l'annexe&nbsp;2 du&nbsp;projet de loi d'approbation des comptes de la S&#x00E9;curit&#x00E9; sociale&nbsp;pour 2025 de 2,485 milliards d'euros.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-13", "datePublication": "2024-10-24", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-24T11:41:43+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"document