{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0325P0D1N001282", "chronotag": "1739874597443", "legislature": "17", "identification": {"numeroLong": "1282", "numeroOrdreDepot": "1282", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaams", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793174", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA774962", "PA720838", "PA796106", "PA795868", "PA267306", "PA721896", "PA795998", "PA335612", "PA796010", "PA842271", "PA795820", "PA721270", "PA795240", "PA795876", "PA605745", "PA842299"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Monnet,  Mme&#160;Lebon, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chassaigne, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Maurel, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Sansu et M.&#160;Tjibaou"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin de l&#x2019;alin&#x00E9;a&nbsp;14, substituer au nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3,2&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">le nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2,5&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement pre&#x0301;voit de limiter le champ d&#x2019;application de l&#x2019;alle&#x0300;gement de cotisations patronales familiales. En effet, ce dispositif qui s&#x2019;applique actuellement aux re&#x0301;mune&#x0301;rations jusqu&#x2019;a&#x0300; 3,5 SMIC, est inefficace en termes de cre&#x0301;ations d&#x2019;emplois et de compe&#x0301;titivite&#x0301;. Il est e&#x0301;galement cou&#x0302;teux pour les finances sociales avec une perte de recettes conside&#x0301;rable pour la branche Famille. Le nouveau ciblage propose&#x0301; &#x00E0; 2,5 SMIC permet d'&#x00EA;tre plus audacieux que le pr&#x00E9;sent projet de loi qui le fixe &#x00E0; 3,2 SMIC.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-10-30", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-10-30T16:48:12+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001282?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28632", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "27046", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "72780", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}