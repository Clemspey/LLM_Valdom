 "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport faisant l&#x2019;&#x00E9;valuation de la mise en &#x0153;uvre de l&#x2019;article&nbsp;18 de la loi n&#x00B0;&nbsp;2023&#x2011;1250 du 26 d&#x00E9;cembre 2023 de financement de la s&#x00E9;curit&#x00E9; sociale pour 2024.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement du Gouvernement fait suite &#x00E0; l&#x2019;amendement 132 des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s et vise &#x00E0; remettre au Parlement un rapport relatif &#x00E0; la r&#x00E9;forme de l&#x2019;assiette sociale des travailleurs ind&#x00E9;pendants contenue dans l&#x2019;article 18 de la LFSS pour 2024.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-30", "datePublication": "2024-10-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-10-30T15:32:25+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N002382?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28632", "article99":