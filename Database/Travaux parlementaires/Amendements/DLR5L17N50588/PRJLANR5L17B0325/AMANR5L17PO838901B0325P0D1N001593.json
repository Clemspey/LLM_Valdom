{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0325P0D1N001593", "chronotag": "1739874588584", "legislature": "17", "identification": {"numeroLong": "1593", "numeroOrdreDepot": "1593", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaatt", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719286", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793210", "PA267763", "PA719138", "PA794554", "PA267042", "PA822617", "PA795244", "PA720976", "PA607619", "PA721110", "PA720960", "PA793796", "PA721474", "PA796078"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Colombani,  M.&#160;Panifous, M.&#160;Bataille, M.&#160;Castellani, M.&#160;Castiglione, M.&#160;Favennec-B&#233;cot, Mme&#160;Froger, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Molac, Mme&#160;Sanquer, M.&#160;Serva, M.&#160;Taupiac, M.&#160;Viry et Mme&#160;Youssouffa"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "27", "alineaDesignation": "Alin\u00e9a 27"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer l&#x2019;alin&#x00E9;a&nbsp;27.<br>&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement s'oppose &#x00E0; la volont&#x00E9; du Gouvernement de prendre par ordonnance des mesures&nbsp;d&#x00E8;s 2025 pour modifier les r&#x00E8;gles relatives &#x00E0; certaines exon&#x00E9;rations de cotisations patronales.&nbsp;</p><p style=\"text-align: justify;\">De telles r&#x00E9;formes doivent pouvoir &#x00EA;tre discut&#x00E9;es au Parlement, conform&#x00E9;ment aux propositions faites depuis plusieurs exercices budg&#x00E9;taires sur ce sujet.&nbsp;</p><p style=\"text-align: justify;\">La r&#x00E9;forme contenue dans l'article 6 est importante, et toutes ses cons&#x00E9;quences doivent pouvoir &#x00EA;tre examin&#x00E9;es et d&#x00E9;battues au Parlement.&nbsp;</p><p style=\"text-align: justify;\">D'autant que parmi les exon&#x00E9;rations concern&#x00E9;es, figurent celles relatives au dispositif dit Lodeom, applicable aux entreprises situ&#x00E9;es en Outre-mer. Or une mission IGAS/IGF doit rendre ses conclusions en octobre 2024 qu'il conviendrait d'attendre avant toute &#x00E9;volution.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-10-30", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-10-30T16:48:21+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001593?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28632", "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "73112", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}