agmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 21", "articleDesignationCourte": "ART. 21", "articleDesignation": "ARTICLE 21", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_21"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "2", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;2, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;<i>bis</i>&nbsp;Au B du I, apr&#x00E8;s la seconde occurrence du mot&nbsp;:&nbsp; &#x00AB;&nbsp;code&nbsp;&#x00BB; sont ins&#x00E9;r&#x00E9;s les mots&nbsp;: &#x00AB;&nbsp;, &#x00E0; l&#x2019;exception des articles L.&nbsp;232&#x2011;9 et L.&nbsp;232&#x2011;10,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; assurer que la garantie de ressources dont b&#x00E9;n&#x00E9;ficient les r&#x00E9;sidents d&#x2019;EHPAD ou USLD admis &#x00E0; l&#x2019;aide sociale &#x00E0; l&#x2019;h&#x00E9;bergement, ou le cas &#x00E9;ch&#x00E9;ant leur conjoint rest&#x00E9; au domicile, continue bien &#x00E0; s&#x2019;appliquer dans les d&#x00E9;partements exp&#x00E9;rimentateurs.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-25", "datePublication": "2024-10-31", "soumis