{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0325P0D1N001105", "chronotag": "1739874583750", "legislature": "17", "identification": {"numeroLong": "AS1105", "numeroOrdreDepot": "1105", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "koaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaati", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794434", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794322", "PA842279", "PA793382", "PA793354", "PA793230", "PA793696", "PA841645", "PA840967", "PA793608", "PA794678", "PA840143", "PA842073", "PA794762", "PA840119"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bentz,  M.&#160;M&#233;nag&#233;, Mme&#160;Bamana, M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie, Mme&#160;M&#233;lin, Mme&#160;Ranc, M.&#160;Muller, M.&#160;Bernhardt, M.&#160;Lioret, Mme&#160;Levavasseur, M.&#160;Frapp&#233;, M.&#160;Florquin, M.&#160;Dussausaye, Mme&#160;Dogor-Such et Mme&#160;Delannoy"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 25", "articleDesignationCourte": "ART. 25", "articleDesignation": "ARTICLE 25", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_25"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;4, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;actions&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;des d&#x00E9;l&#x00E9;gations d&#x00E9;partementales&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La S&#x00E9;curit&#x00E9; sociale n&#x2019;est pas g&#x00E9;r&#x00E9;e d&#x2019;un bloc mais subdivis&#x00E9;e en diff&#x00E9;rents r&#x00E9;gimes et diff&#x00E9;rentes branches. Il n&#x2019;est donc pas pertinent d&#x2019;op&#x00E9;rer des transferts entre branches et entre caisses.</p><p style=\"text-align: justify;\">En l&#x2019;esp&#x00E8;ce, il n&#x2019;y a pas lieu d&#x2019;introduire un transfert&nbsp;de la Caisse nationale de solidarit&#x00E9; pour l&#x2019;autonomie (CNSA) vers les agences r&#x00E9;gionales de sant&#x00E9;. En raison de leur lourdeur technocratique, les ARS, signes de centralisation et de suradministration, doivent du reste &#x00EA;tre supprim&#x00E9;es.</p><p style=\"text-align: justify;\">&#x00C9;chelon de proximit&#x00E9; de l&#x2019;action de l&#x2019;&#x00C9;tat, les services pr&#x00E9;fectoraux sont plus &#x00E0; m&#x00EA;me d&#x2019;assurer le bon fonctionnement des &#x00E9;tablissements et services m&#x00E9;dico-sociaux au b&#x00E9;n&#x00E9;fice des personnes &#x00E2;g&#x00E9;es ou handicap&#x00E9;es.</p><p style=\"text-align: justify;\">Il s&#x2019;agit d&#x2019;un amendement de repli.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-10-25", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-25T10:23:14+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0325P0D1N001105?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}