{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0325P0D1N001046", "chronotag": "1739874583697", "legislature": "17", "identification": {"numeroLong": "AS1046", "numeroOrdreDepot": "1046", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "koaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaau", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794434", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794322", "PA793608", "PA793230", "PA793382", "PA793696", "PA793354", "PA840967", "PA841645", "PA794678", "PA840143", "PA842073", "PA840119", "PA794762"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bentz,  Mme&#160;Bamana, M.&#160;M&#233;nag&#233;, Mme&#160;Levavasseur, Mme&#160;Ranc, M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie, M.&#160;Muller, Mme&#160;M&#233;lin, M.&#160;Lioret, M.&#160;Bernhardt, M.&#160;Frapp&#233;, M.&#160;Florquin, M.&#160;Dussausaye, Mme&#160;Delannoy et Mme&#160;Dogor-Such"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 25", "articleDesignationCourte": "ART. 25", "articleDesignation": "ARTICLE 25", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_25"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;4, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;des agences r&#x00E9;gionales de sant&#x00E9;&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;des services pr&#x00E9;fectoraux&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Notre groupe s&#x2019;oppose au transfert de caisse, et tout particuli&#x00E8;rement au transfert de la CNSA vers les agences r&#x00E9;gionales de sant&#x00E9;.</p><p style=\"text-align: justify;\">En raison de leur lourdeur technocratique, les agences r&#x00E9;gionales de sant&#x00E9;, signe d&#x2019;une centralisation et d&#x2019;une suradministration, doivent &#x00EA;tre supprim&#x00E9;es.</p><p style=\"text-align: justify;\">Echelon de proximit&#x00E9; de l&#x2019;action de l&#x2019;&#x00C9;tat, les services pr&#x00E9;fectoraux sont plus &#x00E0; m&#x00EA;me d&#x2019;assurer le fonctionnement des &#x00E9;tablissements et services m&#x00E9;dico-sociaux au b&#x00E9;n&#x00E9;fice des personnes &#x00E2;g&#x00E9;es ou handicap&#x00E9;es.</p><p style=\"text-align: justify;\">Il s&#x2019;agit d&#x2019;un amendement de repli.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-10-25", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-25T10:23:15+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0325P0D1N001046?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}