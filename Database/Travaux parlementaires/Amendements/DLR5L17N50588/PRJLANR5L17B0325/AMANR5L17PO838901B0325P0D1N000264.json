{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0325P0D1N000264", "chronotag": "1739874602064", "legislature": "17", "identification": {"numeroLong": "264", "numeroOrdreDepot": "264", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0325P0D1", "texteLegislatifRef": "PRJLANR5L17B0325", "triAmendement": "jcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabjzy", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719968", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840979", "PA722390", "PA718838", "PA794178", "PA718884", "PA722328"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Sylvie&#160;Bonnet,  M.&#160;Le&#160;Fur, Mme&#160;Corneloup, M.&#160;Fabrice&#160;Brun, M.&#160;Taite, Mme&#160;Bazin-Malgras et Mme&#160;Kremer"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 21", "articleDesignationCourte": "APR\u00c8S ART. 21", "articleDesignation": "APR\u00c8S L'ARTICLE 21, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_21"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin du III de l&#x2019;article&nbsp;2 de la loi n&#x00B0;&nbsp;2024&#x2011;317 du 8 avril 2024 portant mesures pour b&#x00E2;tir la soci&#x00E9;t&#x00E9; du bien vieillir et de l&#x2019;autonomie, l&#x2019;ann&#x00E9;e&nbsp;: &#x00AB;&nbsp;1<sup>er</sup> janvier 2025&nbsp;&#x00BB; est remplac&#x00E9;e par l&#x2019;ann&#x00E9;e&nbsp;: &#x00AB;&nbsp;1<sup>er</sup> janvier 2026&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La loi &#x00AB;&nbsp;bien vieillir&nbsp;&#x00BB; a cr&#x00E9;&#x00E9; le service public d&#x00E9;partemental de l&#x2019;autonomie, pour f&#x00E9;d&#x00E9;rer l&#x2019;ensemble des acteurs de proximit&#x00E9; sous l&#x2019;&#x00E9;gide du D&#x00E9;partement. Le SPDA a une composante financi&#x00E8;re, &#x00E0; travers les commissions des financeurs de la pr&#x00E9;vention de la perte d'autonomie, d&#x00E9;clin&#x00E9;es sur l&#x2019;ensemble des D&#x00E9;partements.</p><p style=\"text-align: justify;\">Ce dispositif a &#x00E9;t&#x00E9; pr&#x00E9;figur&#x00E9; dans 18 d&#x00E9;partements en 2024, mais sa mise en place est progressive et n&#x00E9;cessite du temps&nbsp;il ne ne sera pas possible de le&nbsp;g&#x00E9;n&#x00E9;raliser au 1er janvier 2025 tel que pr&#x00E9;vu par la loi.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement propose par cons&#x00E9;quent&nbsp;de d&#x00E9;caler d'un an sa g&#x00E9;n&#x00E9;ralisation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-11-04", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N000264?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "loi 2024-317", "divisionCodeLoi": "2"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "73436", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}