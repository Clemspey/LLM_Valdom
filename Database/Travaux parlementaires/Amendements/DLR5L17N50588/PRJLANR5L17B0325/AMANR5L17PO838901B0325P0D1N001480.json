rlDivisionTexteVise": "/17/textes/0325.asp#D_Article_27"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>I. &#x2013; &#x00C0; la sixi&#x00E8;me ligne de la seconde colonne du tableau de l&#x2019;alin&#x00E9;a 2, substituer au nombre&nbsp;:&nbsp;</p><p>&#x00AB; 6,6&#x202F;&#x00BB;&nbsp;</p><p>le nombre&#x202F;:&nbsp;</p><p>&#x00AB; 6,7&#x202F;&#x00BB;.&nbsp;&nbsp;</p><p>II. &#x2013; En cons&#x00E9;quence, &#x00E0; l'avant-derni&#x00E8;re ligne de la m&#x00EA;me colonne du m&#x00EA;me tableau du m&#x00EA;me alin&#x00E9;a 2, substituer au nombre&nbsp;:&nbsp;</p><p>&#x00AB; 3,5&#x202F;&#x00BB;&nbsp;</p><p>le nombre&#x202F;:&nbsp;</p><p>&#x00AB; 3,4&#x202F;&#x00BB;.&nbsp;&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement, issu de propositions formul&#x00E9;es par France urbaine, vise &#x00E0; alerter sur la crise budg&#x00E9;taire que traversent trois quarts des centres de sant&#x00E9; non lucratifs. En vue d'apporter une premi&#x00E8;re r&#x00E9;ponse imm&#x00E9;diate, cet amendement&nbsp; ouvre la possibilit&#x00E9; de d&#x00E9;gager des cr&#x00E9;dits d&#x2019;urgence &#x00E0; hauteur de 100 millions d'euros en 2025.&nbsp;</p><p>En raison de l&#x2019;article 40, une re&#x0301;duction de l&#x2019;objectif de de&#x0301;penses des &#x00AB; autres prises en charge &#x00BB; a du&#x0302; e&#x0302;tre ope&#x0301;re&#x0301;e par les signataires de cet amendement mais ces derniers ne pre&#x0301;conisent d&#x2019;aucune manie&#x0300;re une telle re&#x0301;duction.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-24", "datePublication": "2024-11-04", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0325P0D1N001480?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "6", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "27932", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "73498", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}