, M.&#160;Arenas, M.&#160;Arnault, Mme&#160;Belouassa-Cherifi, M.&#160;Bernalicis, M.&#160;Bex, M.&#160;Bilongo, M.&#160;Bompard, M.&#160;Boumertit, M.&#160;Boyard, M.&#160;Cadalen, M.&#160;Caron, M.&#160;Carri&#232;re, Mme&#160;Cathala, M.&#160;Cernon, Mme&#160;Chikirou, M.&#160;Clouet, M.&#160;Coquerel, M.&#160;Coulomme, M.&#160;Delogu, M.&#160;Diouara, Mme&#160;Dufour, Mme&#160;Erodi, Mme&#160;Feld, M.&#160;Fernandes, Mme&#160;Ferrer, M.&#160;Gaillard, Mme&#160;Guett&#233;, M.&#160;Guiraud, Mme&#160;Hamdane, Mme&#160;Hignet, M.&#160;Kerbrat, M.&#160;Lachaud, M.&#160;Lahmar, M.&#160;Laisney, M.&#160;Le&#160;Coq, M.&#160;Le&#160;Gall, Mme&#160;Leboucher, M.&#160;Legavre, Mme&#160;Legrain, Mme&#160;Lejeune, Mme&#160;Lepvraud, M.&#160;L&#233;aument, Mme&#160;&#201;lisa&#160;Martin, Mme&#160;Maximi, Mme&#160;Mesmeur, Mme&#160;Manon&#160;Meunier, M.&#160;Nilor, Mme&#160;Nosb&#233;, Mme&#160;Obono, Mme&#160;Oziol, Mme&#160;Panot, M.&#160;Pilato, M.&#160;Piquemal, M.&#160;Portes, M.&#160;Prud&apos;homme, M.&#160;Ratenon, M.&#160;Saint-Martin, M.&#160;Saintoul, Mme&#160;Soudais, Mme&#160;Stambach-Terrenoir, M.&#160;Tach&#233;, Mme&#160;Taurinya, M.&#160;Tavel, Mme&#160;Trouv&#233;, M.&#160;Vannier et les membres du groupe La France insoumise - Nouveau Front Populaire"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "APR\u00c8S ART. 8", "articleDesignation": "APR\u00c8S L'ARTICLE 8, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0325.asp#D_Article_8"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le premier alin&#x00E9;a de l&#x2019;article L.&nbsp;114&#x2011;17&#x2011;1&#x2011;1 du code de la s&#x00E9;curit&#x00E9; sociale est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;Les mots&nbsp;: &#x00AB;&nbsp;peut proc&#x00E9;der&nbsp;&#x00BB; sont remplac&#x00E9;s par le mot&nbsp;: &#x00AB;&nbsp;proc&#x00E8;de&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;Les mots&nbsp;: &#x00AB;&nbsp;tout ou partie de&nbsp;&#x00BB; sont supprim&#x00E9;s.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">&#x00AB;&nbsp;Cet amendement vise &#x00E0; r&#x00E9;cup&#x00E9;rer l&#x2019;ensemble des cotisations sociales que l&#x2019;Assurance maladie a prise en charge au b&#x00E9;n&#x00E9;fice du professionnel de sant&#x00E9;, lorsque celui-ci est reconnu coupable de faits &#x00E0; caract&#x00E8;re frauduleux.</p><p style=\"text-align: justify;\">Afin de permettre la pleine op&#x00E9;rationnalit&#x00E9; des dispositions cr&#x00E9;&#x00E9;es &#x00E0; l&#x2019;article L.&nbsp;114&#x2011;17&#x2011;1&#x2011;1 du code de la s&#x00E9;curit&#x00E9; sociale, ce dernier vise &#x00E9;galement &#x00E0; instaurer une automaticit&#x00E9; de sanction.</p><p style=\"text-align: justify;\">L&#x2019;annulation des cotisations sociales prises en charge par l&#x2019;Assurance maladie doit &#x00EA;tre syst&#x00E9;matique et porter sur l&#x2019;ensemble du montant ainsi pris en charge.&nbsp;&#x00BB;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-17", "datePublication": "2024-10-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-10-23T19:14:22+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0325P0D1N001134?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L144-17-1-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "72264", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}