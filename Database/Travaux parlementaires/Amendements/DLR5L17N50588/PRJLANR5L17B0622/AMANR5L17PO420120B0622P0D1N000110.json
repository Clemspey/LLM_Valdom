{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0622P0D1N000110", "chronotag": "1739874697946", "legislature": "17", "identification": {"numeroLong": "AS110", "numeroOrdreDepot": "110", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "ofaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793210", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719286", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Panifous,  M.&#160;Colombani et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 20", "articleDesignationCourte": "ART. 20", "articleDesignation": "ARTICLE 20", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_20"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;7, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x00E9;tail&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de mani&#x00E8;re anonymis&#x00E9;e&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Par cet amendement, il est propos&#x00E9; de pr&#x00E9;ciser que les donn&#x00E9;es de sant&#x00E9; enregistr&#x00E9;es dans le cadre de l&#x2019;observance des dispositifs m&#x00E9;dicaux, sont transmises de mani&#x00E8;re anonymis&#x00E9;e lorsqu&#x2019;elles sont envoy&#x00E9;es&nbsp;aux distributeurs d&#x00E9;tails.</p><p style=\"text-align: justify;\">Les donn&#x00E9;es de sant&#x00E9; sont particuli&#x00E8;rement sensibles et il convient de les prot&#x00E9;ger. Leur transmission aux distributeurs ne peut se faire qu&#x2019;&#x00E0; des fins de recherche, et donc doivent &#x00EA;tre anonymis&#x00E9;es. D&#x2019;autant que ces distributeurs peuvent &#x00EA;tre des acteurs extra-europ&#x00E9;ens, non-soumis au RGPD.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-22", "datePublication": "2025-01-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-29T21:38:22+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0622P0D1N000110?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}