{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0622P0D1N000208", "chronotag": "1739874693351", "legislature": "17", "identification": {"numeroLong": "AS208", "numeroOrdreDepot": "208", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "baoaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaah", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774962", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA793174"}, "suffixe": "et les membres du groupe Gauche D\u00e9mocrate et R\u00e9publicaine", "libelle": "Mme&#160;Lebon,  M.&#160;Monnet et les membres du groupe Gauche D&#233;mocrate et R&#233;publicaine"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5 quater", "articleDesignationCourte": "ART. 5 QUATER", "articleDesignation": "ARTICLE 5 QUATER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_5_quater"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;tablir cet article dans la r&#x00E9;daction suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport &#x00E9;valuant la mise en &#x0153;uvre de l&#x2019;article&nbsp;18 de la loi&nbsp;n&#x00B0;&nbsp;2023&#x2011;1250 du 26&nbsp;d&#x00E9;cembre&nbsp;2023 de financement de la s&#x00E9;curit&#x00E9; sociale pour 2024.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Ce rapport analyse plus largement la pertinence de d&#x00E9;finir l&#x2019;assiette des cotisations sociales sur celle de la cotisation sociale g&#x00E9;n&#x00E9;ralis&#x00E9;e, en &#x00E9;valuant l&#x2019;impact budg&#x00E9;taire pour les organismes de s&#x00E9;curit&#x00E9; sociale et par cas&#x2011;type d&#x2019;assur&#x00E9;.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les auteurs de cet amendement souhaitent r&#x00E9;tablir l&#x2019;article&nbsp;5 <i>quater</i>. Ce dernier formule en effet une demande de rapport utile&nbsp;en ce qu&#x2019;il &#x00E9;valuera&nbsp;la r&#x00E9;forme de l&#x2019;assiette sociale des travailleurs ind&#x00E9;pendants contenue dans l&#x2019;article&nbsp;18 de la LFSS pour 2024 et prolongera les observations contenues dans le&nbsp;rapport Bozio-Wasmer sur la prolif&#x00E9;ration des exemptions d&#x2019;assiettes de cotisations sociales.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-23", "datePublication": "2025-01-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-01-27T23:19:49+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0622P0D1N000208?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "75088", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}