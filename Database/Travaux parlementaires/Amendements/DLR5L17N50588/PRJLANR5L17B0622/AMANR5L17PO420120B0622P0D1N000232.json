nuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer les alin&#x00E9;as 1 &#x00E0; 35.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les auteurs de cet amendement proposent une alternative aux dispositions de l&#x2019;article&nbsp;7 <i>bis</i> B afin de r&#x00E9;pondre aux besoins de financement de la branche &#x00AB;&nbsp;Autonomie&nbsp;&#x00BB; tout en garantissant le droit des salari&#x00E9;s &#x00E0; &#x00EA;tre r&#x00E9;mun&#x00E9;r&#x00E9;s en contrepartie du travail effectu&#x00E9;. Cet amendement vise ainsi &#x00E0; doubler le niveau de la contribution de solidarit&#x00E9; pour l&#x2019;autonomie pay&#x00E9;e par les entreprises, sans augmenter la dur&#x00E9;e de travail des salari&#x00E9;s et sans instituer le &#x00AB;&nbsp;travail gratuit&nbsp;&#x00BB; au motif de la solidarit&#x00E9;.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-23", "datePublication": "2025-01-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-28T18:54:17+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0622P0D1N000232?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRe