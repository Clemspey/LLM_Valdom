{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000635", "chronotag": "1740392270574", "legislature": "17", "identification": {"numeroLong": "635", "numeroOrdreDepot": "635", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "dwaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaakso", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840075", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA840915", "PA842255", "PA794434"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Casterman,  M.&#160;Monnier, Mme&#160;Sicard et M.&#160;Bentz"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9 bis", "articleDesignationCourte": "ART. 9 BIS", "articleDesignation": "ARTICLE 9 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_9_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la deuxi&#x00E8;me ligne de la seconde colonne du tableau de l&#x2019;alin&#x00E9;a&nbsp;4, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;4&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3,5&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la derni&#x00E8;re ligne de la seconde colonne du tableau de l&#x2019;alin&#x00E9;a&nbsp;4, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;35&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;28&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement de repli vise &#x00E0; revenir &#x00E0; la hausse de la taxe soda adopt&#x00E9;e par l&#x2019;Assembl&#x00E9;e nationale le 4 novembre dernier, les volumes envisag&#x00E9;s ensuite par les s&#x00E9;nateurs &#x00E9;tant sans commune mesure avec ce que le secteur est capable d&#x2019;absorber.</p><p style=\"text-align: justify;\">Cette taxe &#x00E0; vis&#x00E9;e comportementale n&#x2019;aura aucun effet sur les pratiques de consommation mais viendra peser &#x00E0; la baisse sur les prix des betteraves sucri&#x00E8;res et aggravera la situation de nos agriculteurs et de nos industries de transformation agro-alimentaire d&#x00E9;j&#x00E0; rudement impact&#x00E9;s par les contraintes r&#x00E9;glementaires, normatives et fiscales fran&#x00E7;aises et europ&#x00E9;ennes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000635?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}