{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0622P0D1N000010", "chronotag": "1739874693071", "legislature": "17", "identification": {"numeroLong": "AS10", "numeroOrdreDepot": "10", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "dwaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaais", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA1567", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642764", "PA842211", "PA795762", "PA793394", "PA606573", "PA840939", "PA841717", "PA841091"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Guedj,  M.&#160;Aviragnet, Mme&#160;Bellay, M.&#160;Califer, M.&#160;Delaporte, Mme&#160;Dombre&#160;Coste, Mme&#160;Godard, Mme&#160;Runel et M.&#160;Simion"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9 bis", "articleDesignationCourte": "ART. 9 BIS", "articleDesignation": "ARTICLE 9 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_9_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "8", "alineaDesignation": "Alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer les alin&#x00E9;as 8 &#x00E0; 13. </p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; supprimer le renforcement de la fiscalit&#x00E9; sur les boissons &#x00E9;dulcor&#x00E9;es adopt&#x00E9; au S&#x00E9;nat.</p><p style=\"text-align: justify;\">En effet, s&#x2019;il nous semble logique d&#x2019;inciter &#x00E0; r&#x00E9;duire la consommation de boissons sucr&#x00E9;es en alourdissant leur fiscalit&#x00E9;, il nous semble en parall&#x00E8;le n&#x00E9;cessaire de ne pas d&#x00E9;sinciter &#x00E0; la consommation des boissons &#x00E9;dulcor&#x00E9;es, qui ne pr&#x00E9;sentent pas les m&#x00EA;mes effets nocifs pour la sant&#x00E9;&nbsp;; ceci afin&nbsp;d&#x2019;accro&#x00EE;tre l&#x2019;&#x00E9;cart entre le prix des boissons sucr&#x00E9;es et celui des boissons &#x00E9;dulcor&#x00E9;es</p><p style=\"text-align: justify;\">L&#x2019;alourdissement de la fiscalit&#x00E9; pesant sur&nbsp;les boissons &#x00E9;dulcor&#x00E9;es introduit par le S&#x00E9;nat nous semble donc peu pertinent.</p><p style=\"text-align: justify;\">Il convient donc de le supprimer.</p><p style=\"text-align: justify;\">Tel est l&#x2019;objet du pr&#x00E9;sent amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-17", "datePublication": "2025-01-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-01-28T23:43:31+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0622P0D1N000010?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": "s8-13"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "75066", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}