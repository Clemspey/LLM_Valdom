{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000359", "chronotag": "1740392268300", "legislature": "17", "identification": {"numeroLong": "359", "numeroOrdreDepot": "359", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "reaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaar", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793174", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA774962", "PA720838", "PA796106", "PA795868", "PA267306", "PA721896", "PA795998", "PA335612", "PA796010", "PA842271", "PA795820", "PA721270", "PA795240", "PA795876", "PA605745", "PA842299"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Monnet,  Mme&#160;Lebon, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chassaigne, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Maurel, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Sansu et M.&#160;Tjibaou"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 24", "articleDesignationCourte": "ART. 24", "articleDesignation": "ARTICLE 24", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_24"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "58", "alineaDesignation": "Alin\u00e9a 58"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer &#x00E0; la seconde phrase de l&#x2019;alin&#x00E9;a&nbsp;58 les deux phrases suivantes&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Elle est compose&#x0301;e de repre&#x0301;sentants des partenaires sociaux sie&#x0301;geant a&#x0300; la commission des accidents du travail et des maladies professionnelles de la Caisse nationale de l&#x2019;assurance maladie mentionn&#x00E9;e au m&#x00EA;me article L.&nbsp;221&#x2011;5, de repre&#x0301;sentants des associations de de&#x0301;fense des victimes et des organisations nationales d&#x2019;aide aux victimes, d&#x2019;experts judiciaires et me&#x0301;dicaux spe&#x0301;cialise&#x0301;s dans la re&#x0301;paration du dommage corporel et des accidents du travail et des maladies professionnelles. Ces membres sont nomme&#x0301;s par arre&#x0302;te&#x0301; des ministres charg&#x00E9;s du travail et de la sante&#x0301; et sie&#x0300;gent a&#x0300; titre gratuit.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement, issu de propositions formul&#x00E9;es par l&#x2019;Andeva et la FNATH, vise &#x00E0; pr&#x00E9;ciser la composition de la Commission des Garanties en garantissant la participation des associations de victimes, des partenaires sociaux sie&#x0301;geant a&#x0300; la commission des accidents du travail et des maladies professionnelles de la Caisse nationale de l&#x2019;assurance maladie et d&#x2019;un ensemble de personnalit&#x00E9;s qualifi&#x00E9;es. L&#x2019;expertise de ces membres&nbsp; appara&#x00EE;t n&#x00E9;cessaire au bon fonctionnement de la Commission, &#x00E0; l&#x2019;enrichissement de son travail et participera &#x00E0; garantir l&#x2019;impartialit&#x00E9; de ses travaux.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000359?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "2", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}