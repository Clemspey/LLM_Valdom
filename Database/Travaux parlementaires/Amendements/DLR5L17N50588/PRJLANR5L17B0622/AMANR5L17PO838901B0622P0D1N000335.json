{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000335", "chronotag": "1740392267231", "legislature": "17", "identification": {"numeroLong": "335", "numeroOrdreDepot": "335", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "qeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaad", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719286", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA822617", "PA721474", "PA267763", "PA793210", "PA607619", "PA841017", "PA719138", "PA794554", "PA942", "PA795244", "PA720976", "PA841931", "PA841359", "PA267042", "PA1592", "PA721486", "PA721110", "PA720960", "PA793796", "PA2952", "PA796078"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Colombani,  Mme&#160;Froger, M.&#160;Viry, M.&#160;Bataille, M.&#160;Panifous, M.&#160;Molac, M.&#160;Huwart, M.&#160;Castellani, M.&#160;Castiglione, M.&#160;de&#160;Courson, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Mazaury, Mme&#160;de&#160;P&#233;lichy, M.&#160;Favennec-B&#233;cot, M.&#160;Habib, M.&#160;Naegelen, Mme&#160;Sanquer, M.&#160;Serva, M.&#160;Taupiac, M.&#160;Warsmann et Mme&#160;Youssouffa"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23", "articleDesignationCourte": "ART. 23", "articleDesignation": "ARTICLE 23", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_23"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;23 initial d&#x00E9;calait de mani&#x00E8;re p&#x00E9;renne la revalorisation annuelle des pensions de retraite du 1<sup>er</sup> janvier au 1<sup>er</sup> juillet, ce qui&nbsp;s&#x2019;apparente &#x00E0; une baisse de pouvoir d&#x2019;achat pour les retrait&#x00E9;s.&nbsp;</p><p style=\"text-align: justify;\">Sa r&#x00E9;&#x00E9;criture au S&#x00E9;nat, ni m&#x00EA;me en CMP, ne propose&nbsp;pas de compromis satisfaisant car elle continue de gr&#x00E9;ver le pouvoir d&#x2019;achat des retrait&#x00E9;s, et donc de faire peser les efforts sur les plus modestes.&nbsp;</p><p style=\"text-align: justify;\">En outre, la motion de censure rend la mise en oeuvre de ce dispositif, qui devait intervenir au 1<sup>er</sup> janvier 2025, inop&#x00E9;rante.&nbsp;</p><p style=\"text-align: justify;\">Il convient donc de supprimer cet article.</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000335?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}