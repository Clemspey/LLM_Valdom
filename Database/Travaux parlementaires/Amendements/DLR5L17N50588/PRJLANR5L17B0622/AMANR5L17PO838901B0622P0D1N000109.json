{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000109", "chronotag": "1740392271003", "legislature": "17", "identification": {"numeroLong": "109", "numeroOrdreDepot": "109", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "bjoaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaacf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719968", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Sylvie&#160;Bonnet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer les alin&#x00E9;as 1 &#x00E0; 4.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;apprentissage est un puissant levier d&#x2019;ascension sociale, particuli&#x00E8;rement pour les jeunes qui n&#x2019;ont pas ou peu acc&#x00E8;s &#x00E0; l&#x2019;enseignement sup&#x00E9;rieur traditionnellement.&nbsp;</p><p style=\"text-align: justify;\">Pourtant, cet article &nbsp;pr&#x00E9;voit une r&#x00E9;duction significative des exon&#x00E9;rations de CSG-CRDS, qui vient se cumuler &#x00E0; une diminution annonc&#x00E9;e des aides accord&#x00E9;es aux employeurs d&#x2019;apprentis. Cette double peine impos&#x00E9;e aux entreprises et aux apprentis entra&#x00EE;nerait des cons&#x00E9;quences lourdes sur le d&#x00E9;veloppement de l&#x2019;apprentissage en France, pourtant pl&#x00E9;biscit&#x00E9; par les jeunes et les entreprises. En effet, les baisses des plafonds d&#x2019;exon&#x00E9;ration pr&#x00E9;vues, entra&#x00EE;neraient une baisse de la r&#x00E9;mun&#x00E9;ration nette des apprentis et auraient des impacts sur la qualit&#x00E9; de vie de ces apprentis.</p><p style=\"text-align: justify;\">Il est donc propos&#x00E9; de supprimer les alin&#x00E9;as 1 &#x00E0; 4.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-27", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000109?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}