ivisionTexteVise": "/17/textes/0622.asp#D_Article_11"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013; A&#x0300; la deuxie&#x0300;me ligne de la deuxie&#x0300;me colonne du tableau a&#x0300; l&#x2019;aline&#x0301;a 2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;246,7&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;262,98&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II. &#x2013; En conse&#x0301;quence, a&#x0300; la m&#x00EA;me deuxie&#x0300;me ligne de la dernie&#x0300;re colonne du m&#x00EA;me tableau au me&#x0302;me aline&#x0301;a 2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-13,2&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2,98&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III. &#x2013; En conse&#x0301;quence, a&#x0300; la septie&#x0300;me ligne de la m&#x00EA;me dernie&#x0300;re colonne dudit tableau audit aline&#x0301;a 2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-15,7&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;0,58&nbsp;&#x00BB;. <br><br>IV. &#x2013; En conse&#x0301;quence, a&#x0300; la dernie&#x0300;re ligne de la m&#x00EA;me dernie&#x0300;re colonne du m&#x00EA;me tableau au me&#x0302;me aline&#x0301;a 2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;-15&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1,28&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise a&#x0300; re&#x0301;affecter a&#x0300; l&#x2019;assurance maladie les de&#x0301;penses engage&#x0301;es pendant la pande&#x0301;mie et indu&#x0302;ment affecte&#x0301;es a&#x0300; la dette sociale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-29", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000230?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "2", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http:/