{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0622P0D1N000120", "chronotag": "1739874699886", "legislature": "17", "identification": {"numeroLong": "AS120", "numeroOrdreDepot": "120", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "qeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719286", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793210", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Colombani,  M.&#160;Panifous et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23", "articleDesignationCourte": "ART. 23", "articleDesignation": "ARTICLE 23", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_23"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;23 initial d&#x00E9;calait de mani&#x00E8;re p&#x00E9;renne la revalorisation annuelle des pensions de retraite du 1<sup>er</sup> janvier au 1<sup>er</sup> juillet, ce qui&nbsp;s&#x2019;apparente &#x00E0; une baisse de pouvoir d&#x2019;achat pour les retrait&#x00E9;s.&nbsp;</p><p style=\"text-align: justify;\">Sa r&#x00E9;&#x00E9;criture au S&#x00E9;nat, ni m&#x00EA;me en CMP, ne propose&nbsp;pas de compromis satisfaisant car elle continue de gr&#x00E9;ver le pouvoir d&#x2019;achat des retrait&#x00E9;s, et donc de faire peser les efforts sur les plus modestes.&nbsp;</p><p style=\"text-align: justify;\">En outre, la motion de censure rend la mise en oeuvre de ce dispositif, qui devait intervenir au 1<sup>er</sup> janvier 2025, inop&#x00E9;rante.&nbsp;</p><p style=\"text-align: justify;\">Il convient donc de supprimer cet article.</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-22", "datePublication": "2025-01-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-01-29T22:18:40+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0622P0D1N000120?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": 