DivisionTexteVise": "/17/textes/0622.asp#D_Article_16"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;3, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;lorsqu&#x2019;elle est particuli&#x00E8;rement co&#x00FB;teuse pour l&#x2019;assurance maladie ou&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de repli vise &#x00E0; clarifier l'enjeu du pr&#x00E9;sent article qui, selon les auteurs de l'amendement, ne saurait &#x00EA;tre de priver des patients de soins&nbsp;dont la prise en charge est jug&#x00E9;e \"particuli&#x00E8;rement co&#x00FB;teuse\" sans consid&#x00E9;ration de l'utilit&#x00E9; de ces soins. En outre, l'expression \"particuli&#x00E8;rement co&#x00FB;teuse\" laisse une marge importante et variable d'appr&#x00E9;ciation. Pour toutes ces raisons, les auteurs de cet amendement proposent une r&#x00E9;daction conforme &#x00E0; l'expos&#x00E9; des motifs de l'article 16 en ne conservant que l'expression de \"risque de m&#x00E9;susage\".</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-29", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true