{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000793", "chronotag": "1740392267131", "legislature": "17", "identification": {"numeroLong": "793", "numeroOrdreDepot": "793", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "qiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA642847", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bazin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23", "articleDesignationCourte": "APR\u00c8S ART. 23", "articleDesignation": "APR\u00c8S L'ARTICLE 23, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_23"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;L&#x2019;article&nbsp;L.&nbsp;161&#x2011;24&#x2011;1 du code de la s&#x00E9;curit&#x00E9; sociale est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;Au d&#x00E9;but, est ajout&#x00E9;e la mention&nbsp;: &#x00AB;&nbsp;I.&nbsp;&#x2013;&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;&#x00C0; la premi&#x00E8;re phrase, les mots&nbsp;: &#x00AB;&nbsp;peut &#x00EA;tre&nbsp;&#x00BB; sont remplac&#x00E9;s par le mot&nbsp;: &#x00AB;&nbsp;est&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">3&#x00B0;&nbsp;&#x00C0; la deuxi&#x00E8;me phrase, les mots&nbsp;: &#x00AB;&nbsp;pouvant &#x00EA;tre&nbsp;&#x00BB; sont supprim&#x00E9;s&nbsp;;</p><p style=\"text-align: justify;\">4&#x00B0;&nbsp;Il est ajout&#x00E9; un&nbsp;II ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;Par d&#x00E9;rogation au&nbsp;I, la preuve d&#x2019;existence peut &#x00EA;tre apport&#x00E9;e&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Par un &#x00E9;change automatique de donn&#x00E9;es entre l&#x2019;organisme ou le service mentionn&#x00E9;s &#x00E0; l&#x2019;article&nbsp;L.&nbsp;161&#x2011;24 et un organisme ou un service charg&#x00E9; de l&#x2019;&#x00E9;tat civil du pays de r&#x00E9;sidence du b&#x00E9;n&#x00E9;ficiaire&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Par un contr&#x00F4;le sur place par un organisme tiers de confiance conventionn&#x00E9;&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3&#x00B0;&nbsp;En fournissant un certificat d&#x2019;existence vis&#x00E9; par le service consulaire du pays de r&#x00E9;sidence du b&#x00E9;n&#x00E9;ficiaire.&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;Le&nbsp;I entre en vigueur le 1<sup>er</sup>&nbsp;janvier&nbsp;2028.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement reprend les dispositions de l'article 7 <i>bis</i> qu'il propose de d&#x00E9;placer vers la troisi&#x00E8;me partie du projet de loi afin de respecter la structure des lois de financements de la s&#x00E9;curit&#x00E9; sociale pr&#x00E9;vue par le cadre organique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000793?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L161-24-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}