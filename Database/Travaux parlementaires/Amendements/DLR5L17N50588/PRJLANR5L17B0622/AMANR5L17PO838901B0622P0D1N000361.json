{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0622P0D1N000361", "chronotag": "1740392272286", "legislature": "17", "identification": {"numeroLong": "361", "numeroOrdreDepot": "361", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0622P0D1", "texteLegislatifRef": "PRJLANR5L17B0622", "triAmendement": "teaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793174", "groupePolitiqueRef": "PO845514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA774962", "PA720838", "PA796106", "PA795868", "PA267306", "PA721896", "PA795998", "PA335612", "PA796010", "PA842271", "PA795820", "PA721270", "PA795240", "PA795876", "PA605745", "PA842299"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Monnet,  Mme&#160;Lebon, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chassaigne, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Maurel, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Sansu et M.&#160;Tjibaou"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 27", "articleDesignationCourte": "ART. 27", "articleDesignation": "ARTICLE 27", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_27"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la sixie&#x0300;me ligne de la seconde colonne du tableau &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;6,4&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;6,5&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II&nbsp;&#x2013;&nbsp;En conse&#x0301;quence, a&#x0300; l&#x2019;avant-derni&#x00E8;re ligne de la&nbsp;m&#x00EA;me colonne du m&#x00EA;me tableau du m&#x00EA;me alin&#x00E9;a, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3,5&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3,4&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement, issu de propositions formule&#x0301;es par France urbaine, vise a&#x0300; alerter sur la crise budge&#x0301;taire que traversent trois quarts des centres de sante&#x0301; non lucratifs. En vue d&#x2019;apporter une premie&#x0300;re re&#x0301;ponse imme&#x0301;diate, cet amendement ouvre la possibilite&#x0301; de de&#x0301;gager des cre&#x0301;dits d&#x2019;urgence a&#x0300; hauteur de 100 millions d&#x2019;euros en 2025.</p><p style=\"text-align: justify;\">En raison de l&#x2019;article&nbsp;40, une re&#x0301;duction de l&#x2019;objectif de de&#x0301;penses des &#x00AB;&nbsp;autres prises en charge&nbsp;&#x00BB; a du&#x0302; e&#x0302;tre ope&#x0301;re&#x0301;e par les signataires de cet amendement mais ces derniers ne pre&#x0301;conisent d&#x2019;aucune manie&#x0300;re une telle re&#x0301;duction.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"code": "R", "libelle": "R"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0622P0D1N000361?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "6", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}