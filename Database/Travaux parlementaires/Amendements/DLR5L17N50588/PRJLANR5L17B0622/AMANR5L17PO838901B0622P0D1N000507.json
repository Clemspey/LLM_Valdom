leRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9 ter C", "articleDesignationCourte": "ART. 9 TER C", "articleDesignation": "ARTICLE 9 TER C", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0622.asp#D_Article_9_ter_C"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement propose de supprimer l&#x2019;article&nbsp;9 <i>ter</i> C, introduit par le S&#x00E9;nat, taxant les sachets de nicotine et en confiant le monopole aux buralistes.&nbsp;</p><p style=\"text-align: justify;\">A l&#x2019;automne dernier, le Gouvernement s&#x2019;&#x00E9;tait engag&#x00E9; &#x00E0; leur interdiction par voie r&#x00E9;glementaire.&nbsp;</p><p style=\"text-align: justify;\">Au regard de leur danger pour la sant&#x00E9;, et en particulier celle des plus jeunes, il est aujourd&#x2019;hui pr&#x00E9;f&#x00E9;rable que le Gouvernement r&#x00E9;it&#x00E8;re et concr&#x00E9;tise rapidement cet engagement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-30", "datePublication": "2025-02-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",