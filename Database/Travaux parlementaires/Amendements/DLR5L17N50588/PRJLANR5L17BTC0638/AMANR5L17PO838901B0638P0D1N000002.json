{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0638P0D1N000002", "chronotag": "1739874712854", "legislature": "17", "identification": {"numeroLong": "2", "numeroOrdreDepot": "2", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0638P0D1", "texteLegislatifRef": "PRJLANR5L17BTC0638", "triAmendement": "btaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Gouvernement", "gouvernementRef": "PO847639", "acteurRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "groupePolitiqueRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "le Gouvernement"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0638.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013;&nbsp;&#x00C0; la fin de l&#x2019;alin&#x00E9;a&nbsp;7, substituer au taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;67,18&nbsp;%&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;63,25&nbsp;%&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II. &#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la fin de l&#x2019;alin&#x00E9;a&nbsp;8, substituer au taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;10,05&nbsp;%&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;10,38&nbsp;%&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III. &#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la fin de l&#x2019;alin&#x00E9;a&nbsp;9, substituer au taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;17,69&nbsp;%&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le taux&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;21,28&nbsp;%&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de coordination modifie les taux de taxes sur les salaires affect&#x00E9;e aux branches maladie, vieillesse et famille, afin de tenir compte de l&#x2019;impact financier des amendements adopt&#x00E9;s au cours de l&#x2019;examen parlementaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-12-02", "datePublication": "2024-12-02", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0638P0D1N000002?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}