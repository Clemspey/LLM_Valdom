{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1001P0D1N000020", "chronotag": "1741800115752", "legislature": "17", "identification": {"numeroLong": "20", "numeroOrdreDepot": "20", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1001P0D1", "texteLegislatifRef": "PNREANR5L17BTC1001", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaal", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841931", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Mazaury"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1001.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "36", "alineaDesignation": "Alin\u00e9a 36"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;36&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Consid&#x00E9;rant le rapprochement diplomatique r&#x00E9;cent entre les &#x00C9;tats-Unis et la Russie, notamment lors de l&#x2019;entretien t&#x00E9;l&#x00E9;phonique bilat&#x00E9;ral du 12 f&#x00E9;vrier 2025 entre le pr&#x00E9;sident am&#x00E9;ricain et le pr&#x00E9;sident russe, puis lors des deux r&#x00E9;unions organis&#x00E9;es &#x00E0; Riyad le 18 f&#x00E9;vrier 2025 et &#x00E0; Istanbul le 27 f&#x00E9;vrier 2025, sans consultation de l&#x2019;Union europ&#x00E9;enne&nbsp;;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; actualiser l&#x2019;alin&#x00E9;a&nbsp;36 &#x00E0; la suite des r&#x00E9;unions organis&#x00E9;es &#x00E0; Riyad et &#x00E0; Istanbul.</p><p style=\"text-align: justify;\">Il permet &#x00E9;galement de mentionner un &#x00AB;&nbsp;rapprochement diplomatique&nbsp;&#x00BB; entre les &#x00C9;tats-Unis et la Russie.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-08", "datePublication": "2025-03-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-12T18:21:53+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"