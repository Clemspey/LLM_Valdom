{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO59047BTC0940P0D1N000012", "chronotag": "1739963906064", "legislature": "17", "identification": {"numeroLong": "AE12", "numeroOrdreDepot": "12", "prefixeOrganeExamen": "CION_AFETR", "numeroRect": "0"}, "examenRef": "EXANR5L17PO59047BTC0940P0D1", "texteLegislatifRef": "PNREANR5L17BTC0940", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaam", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793314", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842001", "PA793182", "PA793134", "PA720468", "PA841613", "PA793146", "PA794734", "PA795900", "PA794030", "PA841075", "PA720614", "PA794582", "PA795966", "PA794442"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Alexandra&#160;Masson,  M.&#160;Bigot, M.&#160;Bovet, M.&#160;Buisson, M.&#160;Chenu, M.&#160;de&#160;Fleurian, M.&#160;Dragon, M.&#160;Guiniot, Mme&#160;Hamelet, M.&#160;Jolly, Mme&#160;Josserand, Mme&#160;Le&#160;Pen, M.&#160;Pfeffer, M.&#160;Rambaud et Mme&#160;Robert-Dehault"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0940.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "59", "alineaDesignation": "Alin\u00e9a 59"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer l&#x2019;alin&#x00E9;a&nbsp;59.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; supprimer cette disposition qui met gravement en danger l&#x2019;ind&#x00E9;pendance &#x00E9;nerg&#x00E9;tique de l&#x2019;Europe. La volont&#x00E9; d&#x2019;interdire toute importation de combustibles fossiles russes ignore la r&#x00E9;alit&#x00E9; du march&#x00E9; et les cons&#x00E9;quences d&#x00E9;sastreuses pour nos industries et nos m&#x00E9;nages. La crise &#x00E9;nerg&#x00E9;tique actuelle a montr&#x00E9; que l&#x2019;Europe n&#x2019;&#x00E9;tait pas pr&#x00EA;te &#x00E0; se passer brutalement de cette source d&#x2019;approvisionnement, ce qui a conduit &#x00E0; une explosion des prix et &#x00E0; des r&#x00E9;percussions sur la comp&#x00E9;titivit&#x00E9; de nos entreprises, et rend de ce fait tout aide future &#x00E0; l&#x2019;Ukraine plus difficile.</p><p style=\"text-align: justify;\">De plus l&#x2019;importation de gaz de schiste am&#x00E9;ricain est contradictoire avec nos politiques de protection de la plan&#x00E8;te en Europe.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-02-15", "datePublication": "2025-02-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-02-19T12:17:58+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59047BTC0940P0D1N000012?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "S", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}