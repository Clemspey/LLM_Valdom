{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0580P0D1N000022", "chronotag": "1739874682497", "legislature": "17", "identification": {"numeroLong": "AS22", "numeroOrdreDepot": "22", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0580P0D1", "texteLegislatifRef": "PIONANR5L17B0580", "triAmendement": "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadae", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793608", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794434", "PA840119", "PA794762", "PA842073", "PA794678", "PA793672", "PA793696", "PA793354", "PA794322", "PA793230", "PA793382", "PA841645", "PA840143", "PA840967"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Levavasseur,  Mme&#160;Bamana, M.&#160;Bentz, Mme&#160;Delannoy, Mme&#160;Dogor-Such, M.&#160;Dussausaye, M.&#160;Frapp&#233;, Mme&#160;Loir, M.&#160;Muller, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, Mme&#160;Ranc, M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie, M.&#160;Bernhardt, M.&#160;Florquin et M.&#160;Lioret"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0580.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;9&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;4&#x00B0;&nbsp;Apr&#x00E8;s le mot&nbsp;:&nbsp;&#x00AB;&nbsp;punie&nbsp;&#x00BB;, la fin du dernier alin&#x00E9;a est ainsi r&#x00E9;dig&#x00E9;e&nbsp;:&nbsp;&#x00AB;&nbsp;d&#x2019;une amende pouvant aller jusqu&#x2019;&#x00E0; 8 500&nbsp;&#x20AC;.&nbsp;&#x00BB;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; r&#x00E9;introduire des sanctions en cas de violation des interdictions pr&#x00E9;vues par l&#x2019;article L.&nbsp;3611&#x2011;3 du Code de la sant&#x00E9; publique, tout en renfor&#x00E7;ant le dispositif par l&#x2019;instauration d&#x2019;une amende pouvant atteindre 8 500&nbsp;euros.</p><p style=\"text-align: justify;\">La suppression des sanctions, telle qu&#x2019;envisag&#x00E9;e dans la proposition de loi, affaiblirait consid&#x00E9;rablement la port&#x00E9;e dissuasive du dispositif l&#x00E9;gislatif. Sans m&#x00E9;canismes coercitifs, les interdictions pr&#x00E9;vues perdraient en efficacit&#x00E9; et risqueraient de ne pas &#x00EA;tre respect&#x00E9;es.</p><p style=\"text-align: justify;\">Cet amendement entend donc mieux pr&#x00E9;venir les infractions tout en renfor&#x00E7;ant la cr&#x00E9;dibilit&#x00E9; des mesures pr&#x00E9;vues dans la loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-18", "datePublication": "2025-01-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-22T12:37:59+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0580P0D1N000022?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": "rg9"}, "discussionCommune": {"idDiscussion": "28658", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}