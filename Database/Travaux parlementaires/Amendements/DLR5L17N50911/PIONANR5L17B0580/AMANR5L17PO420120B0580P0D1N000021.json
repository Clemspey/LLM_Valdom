{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0580P0D1N000021", "chronotag": "1739874683033", "legislature": "17", "identification": {"numeroLong": "AS21", "numeroOrdreDepot": "21", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0580P0D1", "texteLegislatifRef": "PIONANR5L17B0580", "triAmendement": "daaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaag", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793608", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794434", "PA840119", "PA794762", "PA842073", "PA794678", "PA793672", "PA793696", "PA793354", "PA794322", "PA793230", "PA793382", "PA841645", "PA840143", "PA840967"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Levavasseur,  Mme&#160;Bamana, M.&#160;Bentz, Mme&#160;Delannoy, Mme&#160;Dogor-Such, M.&#160;Dussausaye, M.&#160;Frapp&#233;, Mme&#160;Loir, M.&#160;Muller, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, Mme&#160;Ranc, M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie, M.&#160;Bernhardt, M.&#160;Florquin et M.&#160;Lioret"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0580.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;1, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, avec les agences r&#x00E9;gionales de sant&#x00E9;,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le Rassemblement National (RN) ne souhaite pas d&#x00E9;l&#x00E9;guer davantage de comp&#x00E9;tences aux Agences R&#x00E9;gionales de Sant&#x00E9; (ARS), dont il propose la suppression afin de simplifier le syst&#x00E8;me de sant&#x00E9;.</p><p style=\"text-align: justify;\">Ces structures, qui incarnent une suradministration, p&#x00E8;sent sur les professionnels de sant&#x00E9; et les &#x00E9;tablissements hospitaliers en alourdissant les processus d&#x00E9;cisionnels, tout en g&#x00E9;n&#x00E9;rant un co&#x00FB;t important.</p><p style=\"text-align: justify;\">Dans une d&#x00E9;marche de rationalisation des ressources publiques, le Rassemblement National pr&#x00E9;conise, dans son programme, de remplacer les ARS par un pr&#x00E9;fet d&#x00E9;l&#x00E9;gu&#x00E9; &#x00E0; la sant&#x00E9;.</p><p style=\"text-align: justify;\">Ce dispositif permettrait de recentrer les moyens publics sur des actions concr&#x00E8;tes et directement b&#x00E9;n&#x00E9;fiques pour les citoyens.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-18", "datePublication": "2025-01-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-01-22T12:51:05+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0580P0D1N000021?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}