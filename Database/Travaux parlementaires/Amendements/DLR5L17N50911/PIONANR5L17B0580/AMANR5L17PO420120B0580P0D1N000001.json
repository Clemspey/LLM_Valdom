{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0580P0D1N000001", "chronotag": "1739874681449", "legislature": "17", "identification": {"numeroLong": "AS1", "numeroOrdreDepot": "1", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0580P0D1", "texteLegislatifRef": "PIONANR5L17B0580", "triAmendement": "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaacai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840765", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642764", "PA842211", "PA795762", "PA793394", "PA606573", "PA840939", "PA1567", "PA841717", "PA841091"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Lhardit,  M.&#160;Aviragnet, Mme&#160;Bellay, M.&#160;Califer, M.&#160;Delaporte, Mme&#160;Dombre&#160;Coste, Mme&#160;Godard, M.&#160;Guedj, Mme&#160;Runel et M.&#160;Simion"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0580.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer l&#x2019;alin&#x00E9;a&nbsp;9.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement des d&#x00E9;put&#x00E9;s socialistes et apparent&#x00E9;s vise &#x00E0; ne pas supprimer&nbsp;l&#x2019;amende pr&#x00E9;vue par le droit actuel (d&#x2019;un montant de 3 750&nbsp;&#x20AC;) en cas de violation de l&#x2019;interdiction de vente de protoxyde d&#x2019;azote.</p><p style=\"text-align: justify;\">Les d&#x00E9;put&#x00E9;es et d&#x00E9;put&#x00E9;s signataires du pr&#x00E9;sent amendement soutiennent naturellement cette proposition de loi, qui r&#x00E9;pond &#x00E0; un enjeu majeur de sant&#x00E9; publique.</p><p style=\"text-align: justify;\">Toutefois, ils s&#x2019;interrogent sur&nbsp;la raison pour laquelle le neuvi&#x00E8;me alin&#x00E9;a de l&#x2019;article&nbsp;1<sup>er</sup> de cette proposition de loi supprime le dernier alin&#x00E9;a de l&#x2019;article L.&nbsp;3611&#x2011;3 du code de la sant&#x00E9; publique, qui fixe une amende en cas de violation des r&#x00E8;gles de vente du protoxyde d&#x2019;azote.</p><p style=\"text-align: justify;\">Or il appara&#x00EE;t&nbsp;n&#x00E9;cessaire de devoir maintenir dans notre droit cette amende, au moment o&#x00F9; la proposition de loi vient renforcer les r&#x00E8;gles de vente du&nbsp;protoxyde d&#x2019;azote.</p><p style=\"text-align: justify;\">C&#x2019;est l&#x2019;objectif du pr&#x00E9;sent amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-15", "datePublication": "2025-01-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-01-22T12:37:51+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0580P0D1N000001?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": "s9"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "74898", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}