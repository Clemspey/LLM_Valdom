{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0580P0D1N000020", "chronotag": "1739874683076", "legislature": "17", "identification": {"numeroLong": "AS20", "numeroOrdreDepot": "20", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0580P0D1", "texteLegislatifRef": "PIONANR5L17B0580", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793608", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794434", "PA840119", "PA794762", "PA842073", "PA794678", "PA793672", "PA793696", "PA793354", "PA794322", "PA793230", "PA793382", "PA841645", "PA840143", "PA840967"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Levavasseur,  Mme&#160;Bamana, M.&#160;Bentz, Mme&#160;Delannoy, Mme&#160;Dogor-Such, M.&#160;Dussausaye, M.&#160;Frapp&#233;, Mme&#160;Loir, M.&#160;Muller, Mme&#160;M&#233;lin, M.&#160;M&#233;nag&#233;, Mme&#160;Ranc, M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie, M.&#160;Bernhardt, M.&#160;Florquin et M.&#160;Lioret"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0580.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;2 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, tels que le protoxyde d&#x2019;azote&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement de r&#x00E9;daction vise &#x00E0; int&#x00E9;grer le protoxyde d&#x2019;azote dans les campagnes de sensibilisation men&#x00E9;es en milieu scolaire, en ajoutant explicitement cette substance parmi les th&#x00E9;matiques abord&#x00E9;es aux c&#x00F4;t&#x00E9;s du cannabis.</p><p style=\"text-align: justify;\">En effet, ce produit, banalis&#x00E9; dans les milieux festifs, fait l&#x2019;objet d&#x2019;une consommation croissante chez les adolescents et les jeunes adultes, avec des cons&#x00E9;quences graves pour leur sant&#x00E9;, telles que des troubles neurologiques, des br&#x00FB;lures par le froid ou encore des risques d&#x2019;asphyxie.</p><p style=\"text-align: justify;\">Inclure cette probl&#x00E9;matique dans les programmes de pr&#x00E9;vention scolaire permettra d&#x2019;aborder les dangers sp&#x00E9;cifiques de ce gaz hilarant et d&#x2019;&#x00E9;duquer les &#x00E9;l&#x00E8;ves sur ses effets n&#x00E9;fastes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-18", "datePublication": "2025-01-22", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-01-22T12:54:57+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0580P0D1N000020?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}