{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N000177", "chronotag": "1742486458097", "legislature": "17", "identification": {"numeroLong": "CS177", "numeroOrdreDepot": "177", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "abaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaep", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774954", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722390", "PA719272", "PA266808", "PA607090"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Blin,  Mme&#160;Corneloup, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Rolland et M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "8", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;8, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;VIII <i>bis</i>.&nbsp;&#x2013;&nbsp;Le chapitre 1<sup>er</sup> du titre I<sup>er</sup> du livre II de la premi&#x00E8;re partie du code g&#x00E9;n&#x00E9;ral des collectivit&#x00E9;s territoriales est abrog&#x00E9;.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le Comit&#x00E9; des finances locales (CFL) est une instance consultative qui contr&#x00F4;le la r&#x00E9;partition de la dotation globale de fonctionnement (DGF).&nbsp;</p><p style=\"text-align: justify;\">Ses comp&#x00E9;tences se superposent avec celles de la Cour des comptes, qui publie chaque ann&#x00E9;e un rapport annuel sur la situation financi&#x00E8;re des collectivit&#x00E9;s territoriales.</p><p style=\"text-align: justify;\">Au regard de&nbsp;son statut de doublon administratif,&nbsp;il convient de supprimer l&#x2019;OFGL.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-19", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N000177?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "CGCT", "divisionCodeLoi": "L1211-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}