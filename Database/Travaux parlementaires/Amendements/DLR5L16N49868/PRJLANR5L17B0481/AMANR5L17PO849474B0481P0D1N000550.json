{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N000550", "chronotag": "1742486460401", "legislature": "17", "identification": {"numeroLong": "CS550", "numeroOrdreDepot": "550", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "oiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721816", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793508", "PA718838", "PA722390", "PA793094", "PA840979", "PA266808", "PA794058", "PA695512", "PA721442", "PA841047"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Boucard,  M.&#160;Brigand, M.&#160;Fabrice&#160;Brun, Mme&#160;Corneloup, M.&#160;Ray, M.&#160;Le&#160;Fur, M.&#160;Rolland, Mme&#160;Gruet, M.&#160;Lepers, Mme&#160;Petex et M.&#160;Ceccoli"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 26 bis", "articleDesignationCourte": "APR\u00c8S ART. 26 BIS", "articleDesignation": "APR\u00c8S L'ARTICLE 26 BIS, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_26_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": "Sous r\u00e9serve de son traitement par les services de l'Assembl\u00e9e nationale et de sa recevabilit\u00e9", "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;article 26 bis, ins&#x00E9;rer l&#x2019;article suivant :</p><p style=\"text-align: justify;\">Apr&#x00E8;s le cinqui&#x00E8;me alin&#x00E9;a de l&#x2019;article L4362-1 du Code de la Sant&#x00E9; Publique, ajouter la phrase suivante : &#x00AB; Les personnes peuvent obtenir un num&#x00E9;ro d&#x2019;inscription temporaire au fichier national des professions de sant&#x00E9;, permettant d&#x2019;acc&#x00E9;l&#x00E9;rer les d&#x00E9;marches de cr&#x00E9;ation ou reprise d&#x2019;activit&#x00E9; &#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; simplifier les d&#x00E9;marches administratives pour l'ouverture et la reprise de magasins d'optique et d'audioproth&#x00E8;se en introduisant un num&#x00E9;ro d'inscription temporaire au fichier national des professions de sant&#x00E9; (FNPS).</p><p style=\"text-align: justify;\">Les professionnels doivent actuellement attendre plusieurs mois pour obtenir leur inscription d&#x00E9;finitive, qui est obligatoire pour obtenir un identifiant de facturation. Ce d&#x00E9;lai retarde consid&#x00E9;rablement l&#x2019;ouverture de leur commerce.</p><p style=\"text-align: justify;\">En permettant aux opticiens et audioproth&#x00E9;sistes de demander un num&#x00E9;ro temporaire d&#x00E8;s la signature du compromis de vente, cet amendement acc&#x00E9;l&#x00E8;re le processus d'installation et permet une ouverture de magasin qui ne soit pas suspendue &#x00E0; l'obtention du num&#x00E9;ro FNPS d&#x00E9;finitif apr&#x00E8;s la cession.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "ET", "libelle": "En traitement"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N000550?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}