{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0584P0D1N000007", "chronotag": "1739874678987", "legislature": "17", "identification": {"numeroLong": "CE7", "numeroOrdreDepot": "7", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0584P0D1", "texteLegislatifRef": "PIONANR5L17B0584", "triAmendement": "paaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841825", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841495", "PA793238", "PA642988", "PA793246", "PA841139", "PA841605", "PA793592", "PA794354", "PA795912", "PA841837", "PA794590", "PA719436", "PA842227", "PA793298", "PA841479"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Patrice&#160;Martin,  M.&#160;Amblard, M.&#160;Barth&#232;s, M.&#160;de&#160;L&#233;pinau, M.&#160;Falcon, M.&#160;Gabarron, M.&#160;Golliot, Mme&#160;Grangier, Mme&#160;Laporte, Mme&#160;Lavalette, M.&#160;Le&#160;Bourgeois, M.&#160;Loubet, M.&#160;Meizonnet, M.&#160;Rivi&#232;re, M.&#160;Tivoli et M.&#160;Weber"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "relative \u00e0 l\u2019exercice de la d\u00e9mocratie agricole,", "articleDesignationCourte": "TITRE", "articleDesignation": "TITRE", "type": "TITRE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0584.asp#D_relative_a_lexercice_de_la_democratie_"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi le titre de la proposition de loi&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;relative &#x00E0; la gouvernance et &#x00E0; la r&#x00E9;organisation &#x00E9;lectorale des instances agricoles&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le titre initial de la proposition de loi en l&#x2019;&#x00E9;tat appara&#x00EE;t disproportionn&#x00E9; et inadapt&#x00E9; au regard de son contenu r&#x00E9;el. Si la r&#x00E9;organisation des modalit&#x00E9;s &#x00E9;lectorales des instances agricoles, telles que les chambres d&#x2019;agriculture et la Mutualit&#x00E9; sociale agricole, est un objectif louable et n&#x00E9;cessaire, ce texte ne constitue en rien une refonte majeure de la d&#x00E9;mocratie agricole fran&#x00E7;aise que pr&#x00E9;tend son titre.</p><p style=\"text-align: justify;\"><br>Cet amendement propose de modifier le titre dans l&#x2019;objectif de refl&#x00E9;ter plus pr&#x00E9;cis&#x00E9;ment son objet. Cette r&#x00E9;vision vise &#x00E0; en garantir une meilleure lisibilit&#x00E9; et &#x00E0; &#x00E9;viter toute confusion quant &#x00E0; l&#x2019;ampleur et la port&#x00E9;e des r&#x00E9;formes envisag&#x00E9;es.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-12-06", "datePublication": "2024-12-11", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2024-12-11T11:51:39+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0584P0D1N000007?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "28522", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}