"urlDivisionTexteVise": "/17/textes/0772.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer au taux&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;5&nbsp;%&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">le taux&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;10&nbsp;%&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;3, proc&#x00E9;der &#x00E0; la m&#x00EA;me substitution. </p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Propos&#x00E9; par l'Association Mayotte A Soif, cet amendement&nbsp;vise &#x00E0; offrir davantage de flexibilit&#x00E9;, notamment pour mieux r&#x00E9;pondre aux besoins sp&#x00E9;cifiques des projets et pour s&#x2019;adapter aux contraintes locales, mais &#x00E9;galement pour tenir&nbsp;compte des r&#x00E9;alit&#x00E9;s op&#x00E9;rationnelles et des attentes des acteurs concern&#x00E9;s.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-10", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-01-13T23:10:39+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canon