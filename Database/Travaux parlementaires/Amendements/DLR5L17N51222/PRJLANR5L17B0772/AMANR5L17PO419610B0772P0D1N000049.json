rlDivisionTexteVise": "/17/textes/0772.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;1, substituer &#x00E0; l&#x2019;ann&#x00E9;e&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2027&nbsp;&#x00BB;, </p><p style=\"text-align: justify;\">l&#x2019;ann&#x00E9;e&nbsp;: </p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2026&nbsp;&#x00BB;. </p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;6, proc&#x00E9;der &#x00E0; la m&#x00EA;me substitution. </p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les d&#x00E9;rogations pr&#x00E9;vues par le pr&#x00E9;sent article afin de permettre la reconstruction du b&#x00E2;ti scolaire appartenant aux collectivit&#x00E9;s locales par l&#x2019;&#x00C9;tat, en lieu et place de celles-ci, est pr&#x00E9;vu jusqu'au 31 d&#x00E9;cembre 2027. Ceci laisse entendre que les travaux de construction, de reconstruction et de r&#x00E9;habilitation des &#x00E9;coles publiques pourraient prendre pr&#x00E8;s de trois ans, alors m&#x00EA;me que le gouvernement a pris l'engagement de reconstruire Mayotte en trois ans et que la r&#x00E9;habilitation du b&#x00E2;ti scolaire constitue bien &#x00E9;videmment un objectif prioritaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-09", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-01-13T18:48:27+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0772P0D1N000049?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}