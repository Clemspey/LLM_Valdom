{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0772P0D1N000069", "chronotag": "1739874720801", "legislature": "17", "identification": {"numeroLong": "CE69", "numeroOrdreDepot": "69", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0772P0D1", "texteLegislatifRef": "PRJLANR5L17B0772", "triAmendement": "ccaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721852", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642695", "PA607395", "PA722000", "PA794798", "PA722366", "PA721170", "PA721764", "PA719388", "PA720342", "PA795730", "PA719080", "PA795664", "PA842147", "PA795920", "PA720154"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Lebec,  M.&#160;Bothorel, M.&#160;Travert, M.&#160;Buchou, Mme&#160;Buffet, M.&#160;Fugit, M.&#160;Vojetta, Mme&#160;Olivia&#160;Gr&#233;goire, Mme&#160;Le&#160;Meur, Mme&#160;Le&#160;Peih, M.&#160;Marchive, Mme&#160;Marsaud, M.&#160;Midy, M.&#160;Mongardien, Mme&#160;Thevenot et Mme&#160;Dubr&#233;-Chirat"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0772.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "10", "alineaDesignation": "Alin\u00e9a 10"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;10, substituer &#x00E0; la dur&#x00E9;e&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;quarante-cinq&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">la dur&#x00E9;e&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;trente-cinq&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement propose de fixer un d&#x00E9;lai limite de majoration du d&#x00E9;lai d&#x2019;instruction d&#x2019;une autorisation d&#x2019;urbanisme, lorsque celle-ci est subordonn&#x00E9;e &#x00E0; la r&#x00E9;alisation d&#x2019;une participation du public par voie &#x00E9;lectronique (PPVE) &#x00E0; trente-cinq jours au lieu de quarante-cinq comme actuellement propos&#x00E9; par la loi. Le d&#x00E9;lai minimal pour r&#x00E9;aliser une PPVE en droit commun est de 30 jours, mais il est fr&#x00E9;quent que ces PPVE soient r&#x00E9;alis&#x00E9;es en une trentaine de jours (entre 31 et 33), c&#x2019;est pourquoi cet amendement propose de ramener le d&#x00E9;lai de majoration &#x00E0; 35 jours.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-09", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-01-13T23:45:16+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0772P0D1N000069?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}