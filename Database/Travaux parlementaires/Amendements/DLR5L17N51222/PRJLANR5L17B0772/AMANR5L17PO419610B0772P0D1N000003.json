{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0772P0D1N000003", "chronotag": "1739874717911", "legislature": "17", "identification": {"numeroLong": "CE3", "numeroOrdreDepot": "3", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0772P0D1", "texteLegislatifRef": "PRJLANR5L17B0772", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaal", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607395", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Travert"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 6", "articleDesignationCourte": "APR\u00c8S ART. 6", "articleDesignation": "APR\u00c8S L'ARTICLE 6, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0772.asp#D_Article_6"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Par d&#x00E9;rogation au B du II de l&#x2019;article L.&nbsp;34&#x2011;9&#x2011;1 du code des postes et des communications &#x00E9;lectroniques, l&#x2019;obligation de transmission du dossier d&#x2019;information en vue de l&#x2019;exploitation ou de la modification d&#x2019;une installation radio&#x00E9;lectrique sur le territoire d&#x2019;une commune est suspendue pendant une dur&#x00E9;e de deux ans lorsque cette exploitation ou cette modification est strictement n&#x00E9;cessaire pour assurer la continuit&#x00E9; du fonctionnement des services et des r&#x00E9;seaux de communications &#x00E9;lectroniques. Le maire ou le pr&#x00E9;sident de l&#x2019;&#x00E9;tablissement public de coop&#x00E9;ration intercommunale du territoire concern&#x00E9; reste inform&#x00E9; par l&#x2019;exploitant, pr&#x00E9;alablement et par tous moyens, de l&#x2019;exploitation ou de la modification projet&#x00E9;e.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Compte tenu du d&#x00E9;lai moyen de d&#x00E9;ploiement d&#x2019;un site mobile qui se situe autour de 24 mois en France, la dispense temporaire de d&#x00E9;p&#x00F4;t d&#x2019;un dossier d&#x2019;information au Maire pr&#x00E9;alable au d&#x00E9;p&#x00F4;t de la demande d&#x2019;urbanisme permettra d&#x2019;acc&#x00E9;l&#x00E9;rer significativement la reconstruction et la densification des antennes relais de t&#x00E9;l&#x00E9;phonie mobile.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-09", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-01-13T23:25:01+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0772P0D1N000003?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "C postes communications", "divisionCodeLoi": "L34-9-1"}, "discussionCommune": {"idDiscussion": "28554", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "74810", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}