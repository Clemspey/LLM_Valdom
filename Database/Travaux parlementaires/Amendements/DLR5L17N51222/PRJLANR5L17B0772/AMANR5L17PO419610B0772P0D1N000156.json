{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0772P0D1N000156", "chronotag": "1739874713411", "legislature": "17", "identification": {"numeroLong": "CE156", "numeroOrdreDepot": "156", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0772P0D1", "texteLegislatifRef": "PRJLANR5L17B0772", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA643157", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA342415", "PA841315", "PA794854", "PA840765", "PA607553", "PA840673", "PA793816"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "M.&#160;Naillet,  Mme&#160;Battistel, M.&#160;Benbrahim, M.&#160;Echaniz, M.&#160;Lhardit, M.&#160;Potier, Mme&#160;Rossi, Mme&#160;Thomin et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0772.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;3, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;y associer les collectivit&#x00E9;s territoriales de Mayotte et&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;maintenir une repr&#x00E9;sentation &#x00E9;quilibr&#x00E9;e des repr&#x00E9;sentants de l&#x2019;&#x00C9;tat et des collectivit&#x00E9;s territoriales de Mayotte et &#x00E0; y associer&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s vise &#x00E0; pr&#x00E9;server une juste repr&#x00E9;sentation des collectivit&#x00E9;s territoriales de Mayotte au sein du Conseil d&#x2019;administration de l&#x2019;EPFA de Mayotte.&nbsp;</p><p style=\"text-align: justify;\">En effet, en l&#x2019;&#x00E9;tat actuel du droit, le conseil d&#x2019;administration de l&#x2019;EPFA de Mayotte comporte douze membres avec voix d&#x00E9;lib&#x00E9;ratives dont six repr&#x00E9;sentants de l&#x2019;&#x00C9;tat et six repr&#x00E9;sentants des collectivit&#x00E9;s territoriales, auxquels s&#x2019;ajoutent trois repr&#x00E9;sentants avec voix consultative des acteurs &#x00E9;conomiques du territoire. Si un &#x00E9;largissement et une diversification de sa composition peuvent tout &#x00E0; fait se justifier au regard du r&#x00F4;le que le Gouvernement entend d&#x00E9;sormais confier &#x00E0; cet &#x00E9;tablissement dans le cadre de la reconstruction de l&#x2019;&#x00EE;le, il nous semble fondamental que l&#x2019;&#x00E9;quilibre actuel de repr&#x00E9;sentation entre &#x00C9;tat et collectivit&#x00E9;s locales soit pr&#x00E9;serv&#x00E9;, tout en associant pleinement les acteurs &#x00E9;conomiques locaux.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-10", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-01-13T17:46:09+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0772P0D1N000156?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}