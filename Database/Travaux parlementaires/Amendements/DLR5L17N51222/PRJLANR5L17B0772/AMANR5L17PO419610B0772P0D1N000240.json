{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0772P0D1N000240", "chronotag": "1739874717243", "legislature": "17", "identification": {"numeroLong": "CE240", "numeroOrdreDepot": "240", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0772P0D1", "texteLegislatifRef": "PRJLANR5L17B0772", "triAmendement": "bcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaas", "cardinaliteAmdtMultiples": "1", "amendementParentRef": "AMANR5L17PO419610B0772P0D1N000120", "signataires": {"auteur": {"typeAuteur": "Gouvernement", "gouvernementRef": "PO855052", "acteurRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "groupePolitiqueRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "le Gouvernement"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "ART. 4", "articleDesignation": "ARTICLE 4", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0772.asp#D_Article_4"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Au&nbsp;quatri&#x00E8;me alin&#x00E9;a, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ou permis de construire&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;urbanisme&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;Compl&#x00E9;ter le quatri&#x00E8;me alin&#x00E9;a par&nbsp;la phrase suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Elle peut &#x00E9;galement pr&#x00E9;voir de s&#x2019;appliquer aux constructions temporaires dispens&#x00E9;es de formalit&#x00E9; au titre du code de l&#x2019;urbanisme mentionn&#x00E9;es &#x00E0; l&#x2019;article&nbsp;3 de la pr&#x00E9;sente loi.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ce sous-amendement a pour objet d&#x2019;assurer que les mesures d&#x2019;adaptation pr&#x00E9;vues par l&#x2019;ordonnance s&#x2019;appliquent aux constructions temporaires dispens&#x00E9;es de formalit&#x00E9;s au titre du code de l&#x2019;urbanisme en application de l&#x2019;article&nbsp;3 de la pr&#x00E9;sente loi.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">De plus, le sous-amendement proc&#x00E8;de &#x00E0; une modification r&#x00E9;dactionnelle en faisant uniquement r&#x00E9;f&#x00E9;rence aux autorisations d&#x2019;urbanisme.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-01-13", "datePublication": "2025-01-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-01-13T22:14:48+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {