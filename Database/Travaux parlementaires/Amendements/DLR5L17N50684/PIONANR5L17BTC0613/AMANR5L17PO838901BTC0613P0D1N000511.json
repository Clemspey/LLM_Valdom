{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC0613P0D1N000511", "chronotag": "1739874702348", "legislature": "17", "identification": {"numeroLong": "511", "numeroOrdreDepot": "511", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC0613P0D1", "texteLegislatifRef": "PIONANR5L17BTC0613", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaanqcz", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722102", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794206", "PA267200", "PA720066", "PA795864", "PA722190", "PA795176", "PA2960", "PA719550", "PA841709", "PA721296"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Vidal,  M.&#160;Marion, M.&#160;Huyghe, Mme&#160;Rist, M.&#160;Pierre&#160;Cazeneuve, M.&#160;Attal, M.&#160;Metzdorf, M.&#160;Woerth, Mme&#160;Liliana&#160;Tanguy, Mme&#160;Hoffman et M.&#160;Gouffier&#160;Valente"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0613.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi cet article&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Dans un d&#x00E9;lai d&#x2019;un an &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport sur les cons&#x00E9;quences sur les finances publiques d&#x2019;une abrogation de l&#x2019;augmentation de la dur&#x00E9;e d&#x2019;assurance requise &#x00E0;&nbsp;quarante-trois annuit&#x00E9;s telle que pr&#x00E9;vue par la loi n&#x00B0;&nbsp;2014&#x2011;40 du 20 janvier 2014 garantissant l&#x2019;avenir et la justice du syst&#x00E8;me de retraites.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement a pour objet d&#x2019;&#x00E9;clairer la repr&#x00E9;sentation nationale sur les cons&#x00E9;quences sur les finances publiques d&#x2019;une abrogation de la r&#x00E9;forme param&#x00E9;trique de 2014 (dite &#x00AB;&nbsp;r&#x00E9;forme Touraine&nbsp;&#x00BB;) sur les finances publiques.<br>&nbsp;<br>Eu &#x00E9;gard &#x00E0; l&#x2019;ampleur de la r&#x00E9;forme propos&#x00E9;e par le Nouveau Front Populaire par la pr&#x00E9;sente proposition de loi du groupe LFI, il est indispensable que le Parlement soit d&#x00FB;ment inform&#x00E9; par la remise pr&#x00E9;alable d&#x2019;un document pouvant s&#x2019;apparenter &#x00E0; une &#x00E9;tude d&#x2019;impact.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-25", "datePublication": "2024-11-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-11-28T18:20:47+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC0613P0D1N000511?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28718", "article99": "false", "loiReference": {"codeLoi": "Z3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "28342", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}