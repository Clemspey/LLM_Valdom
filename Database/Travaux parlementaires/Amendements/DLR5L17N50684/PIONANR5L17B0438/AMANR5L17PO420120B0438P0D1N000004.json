{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0438P0D1N000004", "chronotag": "1739874673863", "legislature": "17", "identification": {"numeroLong": "AS4", "numeroOrdreDepot": "4", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0438P0D1", "texteLegislatifRef": "PIONANR5L17B0438", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaae", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795920", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793270", "PA840713", "PA720154", "PA720022", "PA719404", "PA719756", "PA720446", "PA795962", "PA842147", "PA720066", "PA722102"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Thevenot,  M.&#160;Rousset, Mme&#160;Delorme&#160;Duret, Mme&#160;Dubr&#233;-Chirat, M.&#160;Lauzzana, M.&#160;Le&#160;Gac, Mme&#160;Le&#160;Nabour, Mme&#160;Liso, Mme&#160;Missoffe, M.&#160;Mongardien, Mme&#160;Rist et Mme&#160;Vidal"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0438.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "7", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;7, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;La caisse nationale d&#x2019;assurance vieillesse fournit aux assur&#x00E9;s atteignant l&#x2019;&#x00E2;ge mentionn&#x00E9; au premier alin&#x00E9;a un document informatif concernant l&#x2019;acc&#x00E8;s au cumul emploi&#x2011;retraite et au fonctionnement de ce cumul.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le contenu de ce document et les modalit&#x00E9;s d&#x2019;application du pr&#x00E9;sent article sont d&#x00E9;finis par d&#x00E9;cret.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement reprend le contenu de l&#x2019;amendement AS11 d&#x00E9;pos&#x00E9; par St&#x00E9;phane VIRY lors l&#x2019;examen de la proposition de loi&nbsp;visant &#x00E0; restaurer un syst&#x00E8;me de retraite plus juste en annulant les derni&#x00E8;res r&#x00E9;formes portant sur l&#x2019;&#x00E2;ge de d&#x00E9;part et le nombre d&#x2019;annuit&#x00E9;s du groupe Rassemblement national.</p><p style=\"text-align: justify;\">Adopt&#x00E9; lors de l&#x2019;examen en commission, celui-ci n&#x2019;a pu &#x00EA;tre maintenu du fait du rejet du texte par l&#x2019;Assembl&#x00E9;e nationale.</p><p style=\"text-align: justify;\">Eu &#x00E9;gard &#x00E0; sa pertinence, il est donc propos&#x00E9; de le r&#x00E9;tablir au sein de la pr&#x00E9;sente proposition de loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-15", "datePublication": "2024-11-20", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-11-20T11:31:23+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0438P0D1N000004?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}