s": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Expertise, information g\u00e9ographique et m\u00e9t\u00e9orologie", "programmeRef": "538578", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Pr\u00e9vention des risques", "programmeRef": "538580", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "\u00c9nergie, climat et apr\u00e8s-mines", "programmeRef": "538582", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Service public de l'\u00e9nergie", "programmeRef": "538584", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Conduite et pilotage des politiques de l'\u00e9cologie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "538586", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "15594", "libelle": "dont titre 2", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification"}}}, {"libelle": "Fonds d'acc\u00e9l\u00e9ration de la transition \u00e9cologique dans les territoires", "programmeRef": "538588", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "totalAE": {"supplementaire": "0", "annule": "-3000000", "solde": "3000000"}, "totalCP": {"supplementaire": "0", "annule": "-3000000", "solde": "3000000"}, "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le Groupe &#x00E9;cologiste et social reprend pour apporter son soutien l&#x2019;<a href=\"https://www.assemblee-nationale.fr/dyn/17/amendements/0538/CION_FIN/CF72\">amendement n&#x00B0;&nbsp;CF72</a> de nos coll&#x00E8;gues Socialistes et apparent&#x00E9;s, notamment Romain Eskenazi et Mickael Bouloux, pour r&#x00E9;tablir les cr&#x00E9;dits de la Soci&#x00E9;t&#x00E9; nationale de sauvetage en mer (SNSM).</p><p style=\"text-align: justify;\">C&#x2019;est une alternative budg&#x00E9;taire &#x00E0; ce que nous souhaitions porter &#x00E9;galement par la fiscalit&#x00E9; sp&#x00E9;ciale sur les bateaux de grande plaisance&nbsp;: les pr&#x00E9;l&#x00E8;vements sur les yachts doivent soutenir depuis 2018 les moyens de la SNSM, apr&#x00E8;s suppression de l&#x2019;assiette de l&#x2019;Imp&#x00F4;t sur la fortune de ces biens de luxe particuli&#x00E8;rement polluants, et force est de constater que cette fiscalit&#x00E9; sp&#x00E9;cifique ne fonctionne pas de fa&#x00E7;on satisfaisante (<a href=\"https://www.assemblee-nationale.fr/dyn/17/amendements/0324A/AN/3551\">voir notre amendement Girard I-3551</a> sur le PLF 2025 1re partie).</p><p style=\"text-align: justify;\">Afin d&#x2019;assurer la recevabilit&#x00E9; de cet amendement budg&#x00E9;taire, ce dernier diminue de 3 millions (en AE et CP) les annulations au programme 205 &#x00AB;&nbsp;Affaires maritimes, p&#x00EA;che et aquaculture&nbsp;&#x00BB;,. Et nous appelons &#x00E0; ce que les sommes ainsi conserv&#x00E9;es soient r&#x00E9;orient&#x00E9;es vers&nbsp;l&#x2019;action 1 du programme 205 qui concerne la SNSM.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-15", "datePublication": "2024-11-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0538P0D1N000136?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-in