dits": {"ligneCredit": {"id": "15018", "libelle": "dont titre 2", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification"}}}, {"libelle": "Conseil \u00e9conomique, social et environnemental", "programmeRef": "535996", "autorisationEngagementSupplementaire": "-1970430", "creditPaiementSupplementaire": "-1970430", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cour des comptes et autres juridictions financi\u00e8res", "programmeRef": "535998", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification", "lignesCredits": {"ligneCredit": {"id": "15020", "libelle": "dont titre 2", "autorisationEngagementSupplementaire": "0", "creditPaiementSupplementaire": "0", "autorisationEngagementAnnule": "0", "creditPaiementAnnule": "0", "action": "modification"}}}]}, "totalAE": {"supplementaire": "-1970430", "annule": "0", "solde": "-1970430"}, "totalCP": {"supplementaire": "-1970430", "annule": "0", "solde": "-1970430"}, "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Avec ce projet de loi de finances de fin de gestion, le Gouvernement ouvre pr&#x00E8;s de 2 millions d&#x2019;euros de cr&#x00E9;dits suppl&#x00E9;mentaires pour le Conseil &#x00E9;conomique, social et environnemental, sans autre explication qu&#x2019;une &#x00AB;&nbsp;sur-ex&#x00E9;cution pr&#x00E9;visionnelle des cr&#x00E9;dits&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">Nonobstant la pertinence de l&#x2019;institution, qui reste &#x00E0; d&#x00E9;montrer pr&#x00E8;s de 80 ans apr&#x00E8;s sa constitutionnalisation &#x00E0; l&#x2019;av&#x00E8;nement de la Quatri&#x00E8;me R&#x00E9;publique, il convient de rappeler que le S&#x00E9;nat comme l&#x2019;Assembl&#x00E9;e nationale ont refus&#x00E9; toute augmentation de cr&#x00E9;dits pour leur fonctionnement&nbsp;; et ce alors que cette derni&#x00E8;re a connu une dissolution en cours d&#x2019;ann&#x00E9;e. Il appara&#x00EE;t donc hasardeux d&#x2019;ouvrir davantage de cr&#x00E9;dits pour une institution qui co&#x00FB;te d&#x00E9;j&#x00E0; bien cher au contribuable fran&#x00E7;ais comparativement au service qu&#x2019;elle rend.</p><p style=\"text-align: justify;\">En cons&#x00E9;quence, le pr&#x00E9;sent &#x00E0; amendement pr&#x00E9;voit de minorer les ouvertures de cr&#x00E9;dits suppl&#x00E9;mentaires, en autorisations d&#x2019;engagement et en cr&#x00E9;dits de paiement, d&#x2019;un montant de 1&nbsp;970&nbsp;430&nbsp;&#x20AC; pour l&#x2019;action &#x00AB;&nbsp;05&nbsp;&#x2013;&nbsp;Fonctions supports &#x00E0; l&#x2019;institution&nbsp;&#x00BB; du programme n&#x00B0;&nbsp;126 &#x00AB;&nbsp;Conseil &#x00E9;conomique, social et environnemental&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-13", "datePublication": "2024-11-19", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-insta