{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0538P0D1N000066", "chronotag": "1739874685003", "legislature": "17", "identification": {"numeroLong": "66", "numeroOrdreDepot": "66", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0538P0D1", "texteLegislatifRef": "PRJLANR5L17B0538", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaam", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795386", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Lef&#232;vre"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article liminaire", "articleDesignationCourte": "ART. LIMINAIRE", "articleDesignation": "ARTICLE LIMINAIRE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0538.asp#D_Article_liminaire"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la onzi&#x00E8;me ligne de la troisi&#x00E8;me colonne du tableau de l&#x2019;alin&#x00E9;a&nbsp;2, substituer au nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1 657&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">le nombre&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1 652,6&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement rappelle que des marges de man&#x0153;uvre sont encore existantes cette ann&#x00E9;e pour r&#x00E9;duire le d&#x00E9;ficit public &#x00E0; un niveau inf&#x00E9;rieur &#x00E0; 6,1 % de PIB en 2024 ainsi que le pr&#x00E9;voit le pr&#x00E9;sent article.</p><p style=\"text-align: justify;\">Dans la lign&#x00E9;e de ce qui a &#x00E9;t&#x00E9; effectu&#x00E9; par le pr&#x00E9;c&#x00E9;dent Gouvernement en f&#x00E9;vrier et en avril (deux fois 10 milliards d&#x2019;annulation de cr&#x00E9;dits), cet amendement appelle le Gouvernement &#x00E0; annuler 4,4 milliards d&#x2019;euros de d&#x00E9;penses suppl&#x00E9;mentaires. Ajout&#x00E9;s aux 5,6 milliards d&#x2019;euros pr&#x00E9;vus dans le pr&#x00E9;sent projet de loi de fin de gestion, l&#x2019;annulation de cr&#x00E9;dits serait ainsi port&#x00E9;e &#x00E0; 10 milliards d&#x2019;euros.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-15", "datePublication": "2024-11-19", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-11-19T19:01:42+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0538P0D1N000066?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS28649", "article99": "false", "loiReference": {"codeLoi": "11", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}